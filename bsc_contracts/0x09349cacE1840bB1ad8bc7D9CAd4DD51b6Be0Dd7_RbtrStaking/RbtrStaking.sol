contract RbtrStaking is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    // Info of each user.
    struct UserInfo {
        uint256 amount;         // How many LP tokens the user has provided.
        uint256 rewardDebt;     // Reward debt. See explanation below.
        uint256 start;
    }

    // Info of each pool.
    struct PoolInfo {
        IERC20 token;           // Address of LP token contract.
        uint256 allocPoint;       // How many allocation points assigned to this pool. RBTR to distribute per block.
        uint256 lastRewardBlock;  // Last block number that RBTR distribution occurs.
        uint256 accRbtrPerShare;   // Accumulated RBTR per share, times 1e18. See below.
        uint256 depositFeeBP;      // Deposit fee in basis points
        uint256 lpSupply;
        uint256 lockingPeriod;    // locking block numbers
    }

    // The RBTR Token!
    IERC20 public immutable rbtrToken;

    address public adminAddress;

    // Info of each pool.
    PoolInfo[] public poolInfo;
    // Info of each user that stakes LP tokens.
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;
    // The block number when RBTR mining starts.
    uint256 public immutable startBlock;

    mapping(address => address) public referrers; // user address => referrer address
    mapping(address => uint256) public referralsCount; // referrer address => referrals count
    mapping(address => uint256) public totalReferralCommissions; // referrer address => total referral commissions

    uint256 public referralCommissionRate;
    uint256 public adminCommissionRate;

    // Max deposit fee: 4%.
    uint256 public constant MAXIMUM_DEPOSIT_FEE_BP = 10000;
    event addPool(uint256 indexed pid, address token, uint256 allocPoint, uint256 depositFeeBP, uint256 lockingPeriod);
    event setPool(uint256 indexed pid,  uint256 allocPoint, uint256 depositFeeBP, uint256 lockingPeriod);
    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event SetAdminAddress(address indexed user, address indexed newAddress);
    event UpdateEmissionRate(address indexed user, uint256 YeldPerBlock);
    event ReferralRecorded(address indexed user, address indexed referrer);
    event ReferralCommissionRecorded(address indexed referrer, uint256 commission);
    event OperatorUpdated(address indexed operator, bool indexed status);

    constructor(
        IERC20 _rbtrToken,
        uint256 _startBlock,
        address _adminAddress
    ) public {
        rbtrToken = _rbtrToken;
        startBlock = _startBlock;
        adminAddress = _adminAddress;
    }

    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }

    // Pool Exists Mapper
    mapping(IERC20 => mapping (uint256 => bool)) public poolExistence;
    modifier nonDuplicated(IERC20 _token, uint256 _lockingPeriod) {
        require(poolExistence[_token][_lockingPeriod] == false, "nonDuplicated: duplicated");
        _;
    }

    // Add a new lp to the pool. Can only be called by the owner.
    function add(uint256 _allocPoint, IERC20 _token, uint256 _depositFeeBP, uint256 _lockingPeriod) external onlyOwner nonDuplicated(_token, _lockingPeriod) {
        require(_depositFeeBP <= MAXIMUM_DEPOSIT_FEE_BP, "add: invalid deposit fee basis points");
        _token.balanceOf(address(this));

        uint256 lastRewardBlock = block.number > startBlock ? block.number : startBlock;
        poolExistence[_token][_lockingPeriod] = true;
        poolInfo.push(PoolInfo({
            token: _token,
            allocPoint: _allocPoint,
            lastRewardBlock: lastRewardBlock,
            accRbtrPerShare: 0,
            depositFeeBP: _depositFeeBP,
            lpSupply: 0,
            lockingPeriod: _lockingPeriod
        }));

        emit addPool(poolInfo.length - 1, address(_token), _allocPoint, _depositFeeBP, _lockingPeriod);
    }

    // Update the given pool's RBTR allocation point and deposit fee. Can only be called by the owner.
    function set(uint256 _pid, uint256 _allocPoint, uint256 _depositFeeBP, uint256 _lockingPeriod) external onlyOwner {
        require(_depositFeeBP <= MAXIMUM_DEPOSIT_FEE_BP, "set: invalid deposit fee basis points");

        poolInfo[_pid].allocPoint = _allocPoint;
        poolInfo[_pid].depositFeeBP = _depositFeeBP;
        poolInfo[_pid].lockingPeriod = _lockingPeriod;

        emit setPool(_pid, _allocPoint, _depositFeeBP, _lockingPeriod);
    }

    // Return reward multiplier over the given _from to _to block.
    function getMultiplier(uint256 _from, uint256 _to) public view returns (uint256) {
        return _to.sub(_from);
    }

    // View function to see pending RBTR on frontend.
    function pendingRBTR(uint256 _pid, address _user) external view returns (uint256) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accRbtrPerShare = pool.accRbtrPerShare;

        if (block.number > pool.lastRewardBlock && pool.lpSupply != 0) {
            uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
            accRbtrPerShare = pool.accRbtrPerShare.add(multiplier.mul(1e18).mul(pool.allocPoint).div(10000).div(28800));
        }
        return user.amount.mul(accRbtrPerShare).div(1e18).sub(user.rewardDebt);
    }

    // Update reward variables of the given pool to be up-to-date.
    function updatePool(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }

        if (pool.lpSupply == 0 || pool.allocPoint == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }

        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
        pool.accRbtrPerShare = pool.accRbtrPerShare.add(multiplier.mul(1e18).mul(pool.allocPoint).div(10000).div(28800));
        pool.lastRewardBlock = block.number;
    }

    // Deposit LP tokens to TimeLockPool for RBTR allocation.
    function deposit(uint256 _pid, uint256 _amount, address _referrer) external nonReentrant {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        updatePool(_pid);

        // harvest before deposit new amount
        if (user.amount > 0) {
            uint256 pending = user.amount.mul(pool.accRbtrPerShare).div(1e18).sub(user.rewardDebt);
            if (pending > 0) {
                uint256 commissionAmount = pending.mul(referralCommissionRate.add(adminCommissionRate)).div(10000);
                uint256 claimableAmount = pending.sub(commissionAmount);
                safeRBTRTransfer(msg.sender, claimableAmount);
                payReferralCommission(msg.sender, pending);        
            }
        }

        if (_amount > 0) {
            recordReferral(msg.sender, _referrer);
            uint256 balanceBefore = pool.token.balanceOf(address(this));
            pool.token.safeTransferFrom(address(msg.sender), address(this), _amount);
            _amount = pool.token.balanceOf(address(this)) - balanceBefore;
            require(_amount > 0, "we dont accept deposits of 0 size");

            if (pool.depositFeeBP > 0) {
                uint256 depositFee = _amount.mul(pool.depositFeeBP).div(10000);
                pool.token.safeTransfer(adminAddress, depositFee);
                user.amount = user.amount.add(_amount);
                pool.lpSupply = pool.lpSupply.add(_amount).sub(depositFee);
            } else {
                user.amount = user.amount.add(_amount);
                pool.lpSupply = pool.lpSupply.add(_amount);
            }
            user.start = block.number;
        }
        user.rewardDebt = user.amount.mul(pool.accRbtrPerShare).div(1e18);
        emit Deposit(msg.sender, _pid, _amount);
    }    

    function isWithdrawable(uint256 _pid) external view returns (bool) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        if(block.number - user.start > pool.lockingPeriod) {
            return true;
        }
        return false;
    }

    // Withdraw LP tokens from TimeLockPool.
    function withdraw(uint256 _pid, uint256 _amount) external nonReentrant {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        require(user.amount >= _amount, "withdraw: not good");
        require(block.number - user.start > pool.lockingPeriod, "Withdraw: locking is not released yet!");
        updatePool(_pid);
        uint256 pending = user.amount.mul(pool.accRbtrPerShare).div(1e18).sub(user.rewardDebt);
        if (pending > 0) {
            uint256 commissionAmount = pending.mul(referralCommissionRate.add(adminCommissionRate)).div(10000);
            uint256 claimableAmount = pending.sub(commissionAmount);
            safeRBTRTransfer(msg.sender, claimableAmount);
            payReferralCommission(msg.sender, pending);
        }
        if (_amount > 0) {
            user.amount = user.amount.sub(_amount);
            pool.token.safeTransfer(address(msg.sender), _amount);
            pool.lpSupply = pool.lpSupply - _amount;
        }
        user.rewardDebt = user.amount.mul(pool.accRbtrPerShare).div(1e18);
        emit Withdraw(msg.sender, _pid, _amount);
    }

    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw(uint256 _pid) external nonReentrant {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        require(block.number - user.start > pool.lockingPeriod, "Withdraw: locking is not released yet!");
        uint256 amount = user.amount;
        user.amount = 0;
        user.rewardDebt = 0;
        pool.token.safeTransfer(address(msg.sender), amount);
        
        if(pool.lpSupply >= amount)
            pool.lpSupply = pool.lpSupply - amount;
        else
            pool.lpSupply = 0;
            
        emit EmergencyWithdraw(msg.sender, _pid, amount);
    }

    // Safe RBTR transfer funcftion, just in case if rounding error causes pool to not have enough RBTRs.
    function safeRBTRTransfer(address _to, uint256 _amount) internal {
        uint256 rbtrBalance = rbtrToken.balanceOf(address(this));
        bool transferSuccess = false;
        if (_amount > rbtrBalance) {
            transferSuccess = rbtrToken.transfer(_to, rbtrBalance);
        } else {
            transferSuccess = rbtrToken.transfer(_to, _amount);
        }
        require(transferSuccess, "safeRBTRTransfer: Transfer failed");
    }

    function setAdminAddress(address _adminAddress) external onlyOwner {
        require(_adminAddress != address(0), "!nonezero");
        adminAddress = _adminAddress;
        emit SetAdminAddress(msg.sender, _adminAddress);
    }

    function setReferralCommissionRate(uint256 _referralCommissionRate, uint256 _adminCommissionRate) external onlyOwner {
        referralCommissionRate = _referralCommissionRate;
        adminCommissionRate = _adminCommissionRate;
    }

    function recordReferral(address _user, address _referrer) internal {
        if (_user != address(0)
            && _referrer != address(0)
            && _user != _referrer
            && referrers[_user] == address(0)
        ) {
            referrers[_user] = _referrer;
            referralsCount[_referrer] += 1;
            emit ReferralRecorded(_user, _referrer);
        }
    }

    function recordReferralCommission(address _referrer, uint256 _commission) internal {
        if (_referrer != address(0) && _commission > 0) {
            totalReferralCommissions[_referrer] += _commission;
            emit ReferralCommissionRecorded(_referrer, _commission);
        }
    }

            // Pay referral commission to the referrer who referred this user.
    function payReferralCommission(address _user, uint256 _pending) internal {
        address referrer = getReferrer(_user);
        if (referrer != address(0) && referrer != _user && referralCommissionRate > 0) {
            uint256 referralCommssionAmount = _pending.mul(referralCommissionRate).div(10000);
            safeRBTRTransfer(referrer, referralCommssionAmount);
            recordReferralCommission(referrer, referralCommssionAmount);
        }
        
        uint256 adminCommissionAmount = _pending.mul(referralCommissionRate.add(adminCommissionRate)).div(10000);
        safeRBTRTransfer(adminAddress, adminCommissionAmount);
    }

    // Get the referrer address that referred the user
    function getReferrer(address _user) public view returns (address) {
        return referrers[_user];
    }
}