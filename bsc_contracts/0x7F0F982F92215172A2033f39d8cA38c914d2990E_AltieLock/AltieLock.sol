    function getLocksByTokenAndRange(uint256 start, uint256 end)
        public
        view
        returns (AggregatedLocksInfo[] memory)
    {
        if (end >= _tokenAddresses.length()) {
            end = _tokenAddresses.length() - 1;
        }
        uint256 length = end - start + 1;
        AggregatedLocksInfo[]
            memory _aggregatedLocksInfo = new AggregatedLocksInfo[](length);
        uint256 currentIndex = 0;
        for (uint256 i = start; i <= end; i++) {
            _aggregatedLocksInfo[currentIndex] = aggregatedLocksInfo[
                _tokenAddresses.at(i)
            ];
            currentIndex++;
        }
        return _aggregatedLocksInfo;
    }

