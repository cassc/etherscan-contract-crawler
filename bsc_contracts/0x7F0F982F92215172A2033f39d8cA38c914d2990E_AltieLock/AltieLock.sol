            cliff: cliff,
            cliffPercentage: cliffPercentage,
            cycle: cycle,
            releasePerCycle: releasePerCycle,
            unlockedAmount: 0,
            description: description
        });
        _locks.push(newLock);

        return id;
    }

    function _vestingLock(
        address token,
        address user,
        uint256 amount,
        uint256 cliff,
        uint256 cliffPercentage,
        uint256 cycle,
        uint256 releasePerCycle,
        string memory description
    ) private returns (uint256 id) {
        id = _addNewLock(
            token,
            user,
            amount,
            cliff,
            cliffPercentage,
            cycle,
            releasePerCycle,
            description
        );
        _tokenLockIds[token].add(id);
        _userTokenLockIds[user].add(id);
        _tokenAddresses.add(token);

        IERC20(token).safeTransferFrom(msg.sender, address(this), amount);

        AggregatedLocksInfo storage _lockedInfo = aggregatedLocksInfo[token];
        if (_lockedInfo.token == address(0)) {
            _lockedInfo.token = token;
            _lockedInfo.uniswapFactory = address(0);
        }
        _lockedInfo.amount += amount;

        return id;
    }

    function _lpLock(
        address token,
        address user,
        uint256 amount,
        uint256 unlockTimestamp,
        string memory description
    ) private returns (uint256 id) {
        address factory = _getUniswapFactory(token);
        id = _addNewLock(
            token,
            user,
            amount,
            unlockTimestamp,
            0,
            0,
            0,
            description
        );
        _lpTokenLockIds[token].add(id);
        _userLpLockIds[user].add(id);

        AggregatedLocksInfo storage _lockedInfo = aggregatedLocksInfo[token];
        if (_lockedInfo.token == address(0)) {
            _lockedInfo.token = token;
            _lockedInfo.uniswapFactory = factory;
        }
        _lockedInfo.amount += amount;

        emit LockedLP(id, token, amount, unlockTimestamp);

        return id;
    }

    function _lockToken(
        bool isLP,
        address token,
        address user,
        uint256 amount,
        uint256 unlockTimestamp,
        string memory description
    ) private returns (uint256 id) {
        if (isLP) {
            id = _lpLock(token, user, amount, unlockTimestamp, description);
            _lpTokenAddresses.add(token);
        } else {
            id = _addNewLock(
                token,
                user,
                amount,
                unlockTimestamp,
                0,
                0,
                0,
                description
            );
            _tokenLockIds[token].add(id);
            _userTokenLockIds[user].add(id);
            _tokenAddresses.add(token);
            AggregatedLocksInfo storage _lockedInfo = aggregatedLocksInfo[
                token
            ];
            emit LockedToken(id, token, amount, unlockTimestamp);
            if (_lockedInfo.token == address(0)) {
                _lockedInfo.token = token;
                _lockedInfo.uniswapFactory = address(0);
            }
            _lockedInfo.amount += amount;
        }
        IERC20(token).safeTransferFrom(msg.sender, address(this), amount);

        return id;
    }

    // Edit lock

    function updateLock(uint256 id, uint256 newunlockTimestamp) public {
        Lock storage userLock = _locks[id];
        require(userLock.user == _msgSender(), "You are not locker");
        require(userLock.unlockedAmount == 0, "Lock was unlocked");
        require(
            newunlockTimestamp >= userLock.cliff &&
                newunlockTimestamp > block.timestamp,
            "New unlock time should not be before old unlock time or current time"
        );
        userLock.cliff = newunlockTimestamp;

        emit LockUpdated(userLock.token, newunlockTimestamp);
    }

    //Unlock

    function _calculateReleasableVestingTokens(Lock storage userData)
        internal
        view
        returns (uint256)
    {
        if (userData.cliff > block.timestamp) return 0;

        uint256 cliffAmount = FullMath.mulDiv(
            userData.amount,
            userData.cliffPercentage,
            10_000
        );
        uint256 cycleAmount = FullMath.mulDiv(
            userData.amount,
            userData.releasePerCycle,
            10_000
        );
        uint256 elapsed = block.timestamp - userData.cliff;

        uint256 pending = 0;
        if (block.timestamp >= userData.cliff) {
            pending = ((elapsed / userData.cycle) * cycleAmount) + cliffAmount;
        }

        uint256 releasable = 0;
        if (pending >= userData.amount) {
            releasable = userData.amount - userData.unlockedAmount;
        } else {
            releasable = pending - userData.unlockedAmount;
        }

        return releasable;
    }

    function _vestingUnlock(Lock storage userData) internal {
        require(
            _calculateReleasableVestingTokens(userData) > 0,
            "Nothing to unlock"
        );

        uint256 withdrawable = _calculateReleasableVestingTokens(userData);
        uint256 totalAmount = userData.unlockedAmount + withdrawable;

        AggregatedLocksInfo storage _lockedInfo = aggregatedLocksInfo[
            userData.token
        ];

        if (totalAmount == _lockedInfo.amount) {
            _userTokenLockIds[msg.sender].remove(userData.id);
            _tokenLockIds[userData.token].remove(userData.id);
        }
        if (_lockedInfo.amount <= withdrawable) {
            _lockedInfo.amount = 0;
        } else {
            _lockedInfo.amount = _lockedInfo.amount - withdrawable;
        }
        userData.unlockedAmount += withdrawable;

        IERC20(userData.token).safeTransfer(userData.user, withdrawable);

        emit Unlocked(userData.token, withdrawable);
    }

    function _tokenUnlock(Lock storage userData) internal {
        require(block.timestamp >= userData.cliff, "Too early");
        require(userData.unlockedAmount == 0, "Nothing to unlock");
        AggregatedLocksInfo storage _lockedInfo = aggregatedLocksInfo[
            userData.token
        ];

        bool isLP = _lockedInfo.uniswapFactory != address(0);

        if (isLP) {
            _userLpLockIds[msg.sender].remove(userData.id);
        } else {
            _userTokenLockIds[msg.sender].remove(userData.id);
        }

        uint256 amountToUnlock = userData.amount;

        if (amountToUnlock >= _lockedInfo.amount) {
            _lockedInfo.amount = 0;
        } else {
            _lockedInfo.amount = _lockedInfo.amount - amountToUnlock;
        }
        userData.unlockedAmount = amountToUnlock;
        _tokenLockIds[userData.token].remove(userData.id);
        IERC20(userData.token).safeTransfer(msg.sender, amountToUnlock);

        emit Unlocked(userData.token, amountToUnlock);
    }

    function unlock(uint256 id) public {
        Lock storage _lock = _locks[id];
        require(_lock.user == _msgSender());
        if (_lock.cycle > 0) {
            _vestingUnlock(_lock);
        } else {
            _tokenUnlock(_lock);
        }
    }

