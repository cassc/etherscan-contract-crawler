    //Uniswap
    function _getUniswapFactory(address token) public view returns (address) {
        address factoryAddress;
        try IUniswapV2Pair(token).factory() returns (address factory) {
            factoryAddress = factory;
        } catch {
            revert("AltieLock: Not LP token");
        }
        require(
            factoryAddress != address(0) &&
                _isValidLpToken(token, factoryAddress),
            "AltieLock: Not LP token"
        );
        return factoryAddress;
    }

    function _isValidLpToken(address token, address factory)
        public
        view
        returns (bool)
    {
        IUniswapV2Pair pair = IUniswapV2Pair(token);
        address factoryPair = IUniswapV2Factory(factory).getPair(
            pair.token0(),
            pair.token1()
        );
        return factoryPair == token;
    }

