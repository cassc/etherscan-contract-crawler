        }

        return total;
    }

    function totalSupplyAll() public view returns (uint256) {
        // sum of all token ids
        uint256 total = 0;

        for (uint256 id = startTokenId(); id < nextTokenId(); id++) {
            total += totalSeriesSupply(id);
        }

