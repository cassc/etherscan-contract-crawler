// File: miladycola/icola5.sol


pragma solidity ^0.8.7;







contract MiladyColaRaffle is VRFConsumerBaseV2, ConfirmedOwner {

    //reverts
    error RaffleNotLive();
    error OnlyTenAtATime();
    error Loser();
    error NotYourBottles();

    //important events
    event RequestSent(uint256 requestId, uint32 numWords);
    event RequestFulfilled(address playa, uint256 requestId, uint256[] randomWords);
    event winner(uint256 indexed requestId, address playa);

    //request struct linkvrf
    struct RequestStatus {
        bool fulfilled; // whether the request has been successfully fulfilled
        bool exists; // whether a requestId exists
        uint256[] randomWords;
        address playa;
    }

    mapping(uint256 => RequestStatus)
    public s_requests; /* requestId --> requestStatus */
    VRFCoordinatorV2Interface COORDINATOR;

    // Your subscription ID.
    uint64 s_subscriptionId;

    // past requests Id.
    uint256[] public requestIds;
    uint256 public lastRequestId;

    // 500gwei gaslane
    bytes32 keyHash =
        0xff8dedfbfa60af186cf3c830acbc32c05aae823045ae5ea7da1e45fbfaba4f92;

    uint32 callbackGasLimit = 120000;
    uint16 requestConfirmations = 3;
    uint8 public miladyWinners;
    uint32 public odds;
    bool public raffleOn;

    //mainnet milx
    //PROD
    address public _miladyXAddress = 0x227c7DF69D3ed1ae7574A1a7685fDEd90292EB48;

    //mainnet bottles
    //PROD
    address public constant _bottleAddress = 0xE0c839D60F15dD0B939e93d9eeaB5ecDA9AffE57;

    //mainnnet caps
    //PROD
    address public constant _capAddress = 0xB91541bda08d83EcCdEFD8Da2d6F9566F8a8FcB9;


    IERC20 _tmilx = IERC20(_miladyXAddress);

    IERC721ABurnable _bottle = IERC721ABurnable(_bottleAddress);
    MiladyColaCap _cap = MiladyColaCap(_capAddress);

    mapping(address=>bool) private winners;

     /**
     *NETWORK 1 ETHEREUM COORDINATOR
     *0x271682DEB8C4E0901D1a1550aD2e64D568E69909
     */
    constructor(uint64 subscriptionId) 
        VRFConsumerBaseV2(0x271682DEB8C4E0901D1a1550aD2e64D568E69909)
        ConfirmedOwner(msg.sender)      {
        COORDINATOR = VRFCoordinatorV2Interface(0x271682DEB8C4E0901D1a1550aD2e64D568E69909);
        s_subscriptionId = subscriptionId;
    }

    //admin functions
    //
    function setOdds(uint32 newOdds) public onlyOwner  {
        odds = newOdds;
    }

    function setGasLimit(uint32 newGas) public onlyOwner {
        callbackGasLimit = newGas;
    }

    function goSwitch() public onlyOwner {
        raffleOn = !raffleOn;
    }

    function annointWinner(address luckyMilady) public onlyOwner {
        winners[luckyMilady] = true;
        ++miladyWinners;
    }

    function checkMIL() public view returns(uint256 mil) {
        return _tmilx.balanceOf(address(this));
    }

    function withdrawMIL() public onlyOwner {
        require(
            _tmilx.transfer(msg.sender,_tmilx.balanceOf(address(this))),
            "Unable to transfer"
        );
    }

    //Raffle
    //
    function bottlePop(uint256[] calldata bottleTokenNums) public payable onlyBottleOwner(bottleTokenNums) {
        if(!raffleOn){revert RaffleNotLive();}
        if(bottleTokenNums.length > 10){revert OnlyTenAtATime();}
        require(checkMIL() > 1020000000000000000,"We need more Milady");
        require(_bottle.isApprovedForAll(msg.sender,address(this)),"must approve bottles for burning");
        for (uint8 i = 0; i < bottleTokenNums.length; ++i){
            //burn bottle
            _bottle.burn(bottleTokenNums[i]);
        }
        //request random words
        requestRandomWords(uint32(bottleTokenNums.length), msg.sender);
        //mint cap
        _cap.mintLCaps(bottleTokenNums.length, msg.sender);
    }

    function fulfillRandomWords(
        uint256 _requestId,
        uint256[] memory _randomWords
    ) internal override {
        require(s_requests[_requestId].exists, "request not found");
        RequestStatus memory request = s_requests[_requestId];
        request.fulfilled = true;
        request.randomWords = _randomWords;
        //wincheck
        for (uint8 i = 0; i < _randomWords.length; ++i) {
            if (_randomWords[i]%odds + 1 == 1) {
                //milady
                sendIt(request.playa);
                emit winner(_requestId, request.playa);
            }
        }
        emit RequestFulfilled(request.playa, _requestId, _randomWords);
    }

    function sendIt(address recip) internal {
        //milady
        _tmilx.transfer(recip,1.02e18);
    }
    
    function claimWCap() public payable {
        if(!winners[msg.sender]){ revert Loser();}
        _cap.mintWCap(msg.sender);
    }

    function getRequestStatus(
        uint256 _requestId
    ) external view returns (bool fulfilled, uint256[] memory randomWords) {
        require(s_requests[_requestId].exists, "request not found");
        RequestStatus memory request = s_requests[_requestId];
        return (request.fulfilled, request.randomWords);
    }

        // Assumes the subscription is funded sufficiently.
    function requestRandomWords(uint32 numWords, address player)
        internal
        returns (uint256 requestId)
    {
        // Will revert if subscription is not set and funded.
        requestId = COORDINATOR.requestRandomWords(
            keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit*numWords,
            numWords
        );
        s_requests[requestId] = RequestStatus({
            randomWords: new uint256[](0),
            exists: true,
            fulfilled: false,
            playa: player
        });
        requestIds.push(requestId);
        lastRequestId = requestId;
        emit RequestSent(requestId, numWords);
        return requestId;
    }

    //bottle auth
    function checkBottleOwner(uint256[] calldata bottleTokenNums) public view{
        for (uint8 i=0; i < bottleTokenNums.length; ++i) {
            if(_bottle.ownerOf(bottleTokenNums[i]) != msg.sender){ revert NotYourBottles();}
        }
    }

    modifier onlyBottleOwner(uint256[] calldata bottleTokenNums) virtual {
        checkBottleOwner(bottleTokenNums);
        _;
    }

}