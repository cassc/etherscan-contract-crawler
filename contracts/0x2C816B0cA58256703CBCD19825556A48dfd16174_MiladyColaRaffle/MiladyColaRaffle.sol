contract MiladyColaCap is ERC1155, Ownable {

    error AdminOnlyMint();

    string public name = "Caps";
    string public symbol = "MCC";
    string public _uri;

    uint256 public count;
    uint256 public capLoop;

    address public admin;

    constructor()  {      
        _initializeOwner(msg.sender); 
    }

    /// solady override with token id
    function uri(uint256) public view virtual override returns (string memory) {
        return _uri;
    }

    function setURI(string memory newuri) external onlyOwner {
        _uri = newuri;
    }

    function setAdmin(address newAdmin) public onlyOwner {
        admin = newAdmin;
    }

    function setCapLoop(uint256 newLoop) public onlyOwner {
        capLoop = newLoop;
    }

    function mintLCaps(uint256 numberOfTokens, address player) public payable adminOnly {
        uint256 _count = count;
        uint256 _loop = capLoop;
        for(uint256 i=0; i < numberOfTokens; ++i){
            _mint(player,_count%_loop,1,"");
            ++_count;
        }
        count = _count;
    }
    
    function mintWCap(address player) public payable adminOnly {
        _mint(player, 349, 1, "");
    }

    modifier adminOnly() virtual {
        if(msg.sender!=admin){revert AdminOnlyMint();}
        _;
    }
}
