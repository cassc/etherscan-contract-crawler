struct AOW_CHAR_DATA {
   uint256 hp; // 0 - hp_max
   uint256 damage; // 10 - 100
   uint256 armor; // 1-100
   uint256 class; //0 rouge, 1 mage, 3 priest, 4 warrior, 5 archer, 7 tank
   uint256 level; // 1-20
   uint256 activePotion; // damage + 15
   uint256 frozenSince; // timestamp
   uint256 kills;
   string name; // name of the character
   address owner; // owner of the character
   uint256 rerolls;
   uint256 faction;
}

contract AOW_Character_Base is ERC721A, Ownable {
    using SafeMath for uint256;

    uint256 public PRICE = 0.005 ether;
    uint256 public CONSUMABLE_BASE_PRICE = 10;

    mapping(uint256 => uint256) public factionCount;
    uint256 public FACTION_LIMIT_WL = 2500;
    uint256 public FACTION_LIMIT_PUBLIC = 1500;

    bool public _revealed;

    uint256 public MINT_LIMIT = 4;
    uint256 public SALE_PHASE = 0;

    mapping (address => uint256) addressBlockBought;
    mapping (address => uint256) public activeCharacter;
    mapping (address => bool) public isWhitelisted;
    mapping (uint256 => AOW_CHAR_DATA) public character_data;

    ERC721A public WarPassContract = ERC721A(address(this));
    AOWGame public GameContract;
    IERC20 public BloodMark;

    mapping(address => uint256) public factionLock;
    mapping(address => uint256) public freeMints;
    mapping(address => mapping(uint256 => uint256)) public potions;

    event minted(uint256 faction, uint256 count);
    event joined(uint256 char, string name,uint256 faction);

    string private baseURI = "https://data.alloutwar.io";

    constructor() ERC721A("AllOutWar", "AOW_CHARS", 10, 200000) { }

    modifier isAllowed() {
        require(
            msg.sender == owner()  || 
            msg.sender == address(GameContract) || 
            msg.sender == address(this),
            "Only specific contracts can interact"
        );
        
        _; 
    }

    modifier validateMint(uint256 faction, uint256 price, uint256 numToken, uint mintType) {

        require(
            addressBlockBought[msg.sender] < block.timestamp,
            "CANNOT_MINT_ON_THE_SAME_BLOCK"
        );
        require(tx.origin == msg.sender, "CONTRACTS_NOT_ALLOWED_TO_MINT");

        require(
            ( mintType == 1 && SALE_PHASE > 0) ||
            ( mintType == 2 && SALE_PHASE > 1), "MINT_PHASE_IS_NOT_YET_ACTIVE"
        );


        require( 
            totalSupply() + numToken <= FACTION_LIMIT_WL.mul(2),
            "Max supply reached"
        );

        require(
            numberMinted(msg.sender) + numToken <= MINT_LIMIT,
            "Only 4 per wallet"
        );

        require(
            factionCount[faction] + numToken < FACTION_LIMIT_WL,
            "faction is full"
        );
      
        require(
            getFaction(msg.sender)  == 0 || getFaction(msg.sender)  == faction,
            "You can only mint characters in your faction"
        );
        
        require(
            faction == 1 || faction == 2,
            "Faction must be 1: good or 2: evil"
        );
       
        _; 
    }

    function mintPublic(
        uint256 faction,
        uint256 numToken) 
        external 
        payable 
        validateMint(faction,PRICE,numToken,2) {
        
        PRICE = 0.007 ether;
        
        uint256 payableToken = numToken;
        
        if(freeMints[msg.sender] <= 0) {
            payableToken = payableToken.sub(1);
            freeMints[msg.sender] = freeMints[msg.sender] + 1;
        }
        
        require(
            msg.value >= PRICE.mul(payableToken),
            "You can only mint 1 for free"
        );


        // Set faction of sender
        if(getFaction(msg.sender) == 0)
            factionLock[msg.sender] = faction;
        
        addressBlockBought[msg.sender] = block.timestamp;
        initCharacter(msg.sender, numToken, faction);
    }

    function mintWhite(
        uint256 faction,       
        uint256 numToken) 
        external 
        payable 
        validateMint(faction,PRICE,numToken,1) {

        require(
                isWhitelisted[msg.sender]
                || WarPassContract.balanceOf(msg.sender) > 0,
                "PROOF_INVALID OR NO WARPASS OWNER"
        );

        uint256 payableToken = numToken;
        
        if(freeMints[msg.sender] <= 0) {
            payableToken = payableToken.sub(1);
            freeMints[msg.sender] = freeMints[msg.sender] + 1;
        }
        
        if(hasWarPass(msg.sender) && freeMints[msg.sender] < 2) {
            payableToken = payableToken.sub(1);
            freeMints[msg.sender] = freeMints[msg.sender] + 1;
        }

        require(
            msg.value >= PRICE.mul(payableToken),
            "You can only mint 1 for free"
        );


        // Set faction of sender
        if(getFaction(msg.sender) == 0)
            factionLock[msg.sender] = faction;
        
        addressBlockBought[msg.sender] = block.timestamp;
        initCharacter(msg.sender, numToken, faction);
    }

    function devMint(
        address[] memory _addresses,
        uint256[] memory quantities,
        uint256[] memory factions) external onlyOwner {
        require(_addresses.length == quantities.length,"WRONG_PARAMETERS");
        uint256 totalTokens = 0;

        for(uint256 i = 0 ; i < quantities.length; i++){
            totalTokens+= quantities[i];
        }
        
        for(uint256 i = 0 ; i < _addresses.length; i++){
            initCharacter(_addresses[i], quantities[i], factions[i]);
        }
    }

    function initCharacter(address target, uint256 amount, uint256 faction) internal {

        for(uint256 i = currentIndex; i < amount; i++) {
            // Fixed stats
            character_data[i].class = rand(4);
            character_data[i].level = 1;
            character_data[i].faction = faction;

            // Rerollable stats
            character_data[i].damage = rand(100) + 1;
            character_data[i].armor = rand(100) + 1;
            character_data[i].hp = rand(200) + 100;
            factionCount[faction]++;
        }
        
        emit minted(faction,amount);
         _safeMint(target, amount);
    }

    function setCharacter(uint256 token, string memory name) external {
        require(ownerOf(token) == msg.sender, "You need to own the token!");
        require(activeCharacter[msg.sender] == 0, "You already have an active character");

        character_data[token].owner = msg.sender;
        character_data[token].name = name;
        activeCharacter[msg.sender] == token;
        emit joined(token, name, character_data[token].faction);
    }

    function grantConsumable(uint256 consumable, address reciever) external payable isAllowed {
        potions[reciever][consumable] = potions[reciever][consumable] + 1;
    }

    function consumeConsumable(uint256 consumable, address reciever) external payable isAllowed {
        if(potions[reciever][consumable] == 1) {
            potions[reciever][consumable] = 0;
        } else {
        potions[reciever][consumable] = potions[reciever][consumable] - 1;
        }

    }

    function purchaseConsumable(uint256 consumable, address reciever) external payable {

        require(
            consumable == 1 || consumable == 2 || consumable == 3,
            "Consumable must be 1: heal potion or 2: damage potion or 3: freeze potion"
        );

        uint256 price = CONSUMABLE_BASE_PRICE.mul(consumable);
        
        if(hasWarPass(msg.sender))
            price = CONSUMABLE_BASE_PRICE.div(4);
  
        require(
            BloodMark.balanceOf(msg.sender) >= price, "NOT_ENOUGH_BLOODMARK"
        );

        require(
            BloodMark.allowance(msg.sender, address(this)) >= price, "NOT_ENOUGH_BLOODMARK_ALLOWANCE"
        );
        
        potions[reciever][consumable] ++;

        BloodMark.transferFrom(msg.sender, address(GameContract), CONSUMABLE_BASE_PRICE.mul(consumable));
    }

    function hasWarPass(address _player) public view returns (bool) {
        return WarPassContract.balanceOf(_player) > 0;
    }

    function getFaction(address wallet) 
    public 
    view
    returns (uint256) {
        return factionLock[wallet];
    }

    ///// INTERNAL FUNCTIONS /////
    function setBaseURI(string calldata URI) external onlyOwner {
        baseURI = URI;
    }

    function contractURI() public view returns (string memory) {
        return string(abi.encodePacked(baseURI,"/war_meta/contract.json"));
    }

    function reveal(bool revealed, string calldata _baseURI) public onlyOwner {
        _revealed = revealed;
        baseURI = _baseURI;
    }

    //Essential
    function advanceSalePhase() external onlyOwner {
        SALE_PHASE +=1;
    }

    function withdraw(address receiver) external onlyOwner {
        uint256 balance = address(this).balance;
        require(balance > 0, "No balance to withdraw");
        payable(receiver).transfer(address(this).balance);
    }

    function withdrawBM() public onlyOwner {
        uint256 balance = BloodMark.balanceOf(address(this));
        BloodMark.transfer(owner(), balance);
    }

    function canReroll(uint256 tokenId) external returns (bool) {

        require(
            ownerOf(tokenId) == msg.sender, 
            "You can only reroll your own characters"
        );

        require(
            character_data[tokenId].level == 1,
            "Character level is too high"
        );

        uint256 price = CONSUMABLE_BASE_PRICE.mul(10).mul(character_data[tokenId].rerolls);

        require(
            BloodMark.balanceOf(msg.sender) > price,
            "Need to have at least 100 Bloodmark multiplied by past rerolls"
        );

        character_data[tokenId].rerolls = character_data[tokenId].rerolls + 1;

        return true;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(_exists(tokenId),"Token not minted yet");

        AOW_CHAR_DATA memory data = character_data[tokenId];

        if(_revealed) {
            return string(abi.encodePacked(
            baseURI,"/chars/",
            Strings.toString(data.faction),
            "/",
            Strings.toString(tokenId),
            ".json"));
        } else {
            return string(abi.encodePacked(
            baseURI,
            "/",
            Strings.toString(data.faction),
            "/",
            "placeholder.json"));
        }
    }

    function tokensOfOwner(address owner)
        public
        view
        returns (uint256[] memory)
    {
        uint256 tokenCount = balanceOf(owner);
        uint256[] memory tokenIds = new uint256[](tokenCount);
        for (uint256 i = 0; i < tokenCount; i++) {
            tokenIds[i] = tokenOfOwnerByIndex(owner, i);
        }
        return tokenIds;
    }

    function numberMinted(address owner) public view returns (uint256) {
        return _numberMinted(owner);
    }
 
    function setMintLimit(uint256 _mintLimit) external onlyOwner{
        MINT_LIMIT = _mintLimit;
    }

    function getCharData(uint256 tokenId) internal view returns (AOW_CHAR_DATA memory charData) {
        if(_revealed){
            return character_data[tokenId];
        } 
    }

    function setCharData(uint256 tokenId, AOW_CHAR_DATA memory data) external isAllowed {
        character_data[tokenId] = data;
    }

    function setWhitelist(address[] memory _addresses) onlyOwner public {
        for(uint i = 0; i < _addresses.length; i++) {
            isWhitelisted[_addresses[i]] = true;
        }
    }

    function amIWhitelisted() view public returns(bool) {
        return isWhitelisted[msg.sender];
    }

    function setContracts(address gameContract, address seasonPassContract, address bloodmark) external onlyOwner {
        WarPassContract = ERC721A(seasonPassContract);
        GameContract = AOWGame(gameContract);
        BloodMark = IERC20(bloodmark);
    }

    function rand(uint max)
        private
        view
        returns(uint256)
    {
        uint256 seed = uint256(keccak256(abi.encodePacked(
            block.timestamp + block.difficulty +
            ((uint256(keccak256(abi.encodePacked(block.coinbase)))) / (block.timestamp)) +
            block.gaslimit + 
            ((uint256(keccak256(abi.encodePacked(msg.sender)))) / (block.timestamp)) +
            block.number
        )));

        return (seed - ((seed / max) * max));
    }

    function setPrice(uint256 _wlPrice) external onlyOwner {
        PRICE = _wlPrice;
    }
}