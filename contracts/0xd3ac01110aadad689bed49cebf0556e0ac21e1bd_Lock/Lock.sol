contract Lock is Ownable {

    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    
    uint256 public lockTime;
    address public lockToken = 0xb269Ab3B20E09e6fd17C3ED5B4CECF3dEA1a5141;//SAK
    uint256 public releaseSpeed;
    uint256 public lastTime;
   
    function lock(uint256 amount,uint256 time,bool linely) public onlyOwner{
        require(lockTime==0,"locked");
        require(time>block.timestamp);
        IERC20(lockToken).safeTransferFrom(msg.sender,address(this),amount);
        if(linely){
           releaseSpeed = amount.div(time.sub(block.timestamp));
           lastTime = block.timestamp;
        }        
        lockTime = time;
    }


    function pending() public view returns(uint256){
        if(block.timestamp>=lockTime){
            return IERC20(lockToken).balanceOf(address(this));
        }
        else{
            return block.timestamp.sub(lastTime).mul(releaseSpeed);  
        }
    }

    function withdraw() public onlyOwner{
        uint256 amount = pending();
        IERC20(lockToken).safeTransfer(msg.sender,amount);
        lastTime = block.timestamp;
    }

}