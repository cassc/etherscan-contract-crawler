import "./strings.sol";

contract Token is ERC20,Ownable,WhiteList,BlackList {
    using SafeMath for uint256;
    using strings  for *;


    receive () external payable {}

    uint256 public total = 100000000000 * 10 ** 18;

    uint256 public _feeRatio;
    bool public _isStartLock;
    bool public _isStartSwapTrade;

    mapping(address=>bool) public uniswapV2PairMapping;
    mapping(address=>LockInfo) public lockAmountMapping;
    address[] public addressArray;
    
    constructor(address addr) ERC20("mxdao","mxdao"){
        super._mint(addr, total);
        _owner = addr;
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
        address uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this),  0xdAC17F958D2ee523a2206206994597C13D831ec7);
        uniswapV2PairMapping[uniswapV2Pair] = true;
    }

    struct LockInfo{
        uint256 lockAmount;
        uint256 lockAmountFinal;
    }

    function setFeeRatio(uint256 ratio)public onlyOwner{
        _feeRatio = ratio;
    }

    function setStartLock(bool isStart)public onlyOwner{
        _isStartLock = isStart;
    }
    function setStartSwapTrade(bool isStart)public onlyOwner{
        _isStartSwapTrade = isStart;
    }

    function setUniswapV2PairMapping(address addr,bool flag)public onlyOwner{
        uniswapV2PairMapping[addr] = flag;
    }

    function transferBatch(address[] memory accountArray,uint256[] memory amountArray)public onlyOwner{
        require(accountArray.length == amountArray.length,"error");
        for(uint256 i = 0;i<accountArray.length;i++){
            _transfer(_msgSender(),accountArray[i],amountArray[i]);
        }
    }


    function release(uint256 ratio)public onlyOwner{
        for(uint256 i = 0; i < addressArray.length; i++){
            LockInfo storage lockInfo = lockAmountMapping[addressArray[i]];
            if(lockInfo.lockAmount == 0) continue;
            uint256 releaseAmount = calculateRatio(lockInfo.lockAmountFinal,ratio);
            if(lockInfo.lockAmount < releaseAmount){
                lockInfo.lockAmount = 0;
            }else{
                lockInfo.lockAmount -= releaseAmount;
            }
        }
    }


    function _transfer(address sender,address recipient,uint256 amount) internal virtual override{
        require(!isBlackListed(sender),"error");
        if((uniswapV2PairMapping[sender] || uniswapV2PairMapping[recipient]) && !_isStartSwapTrade){
            require(isWhiteListed(sender) || isWhiteListed(recipient) || sender == _owner || recipient == _owner,"Only white list users can trade");
        }

        _beforeTokenTransfer(sender, recipient, amount);
        

        uint256 senderBalance = _balances[sender];
        require(senderBalance >= amount, "ERC20: transfer amount exceeds balance");
        
        if(!isWhiteListed(sender) && _isStartLock){
            if(sender == _owner){
                lockTransfer(recipient,amount);
            }else{
                normalTransfer(sender,amount);
            }
        }

        unchecked {
            _balances[sender] = senderBalance - amount;
        }
        _balances[recipient] += amount;
        emit Transfer(sender, recipient, amount);
         
        _afterTokenTransfer(sender, recipient, amount);
    }

    function lockTransfer(address recipient,uint256 amount) internal{
        LockInfo storage lockInfo = lockAmountMapping[recipient];
        bool exist;
        if(lockInfo.lockAmountFinal == 0){
            exist = true;
        }
        lockInfo.lockAmount += amount;
        lockInfo.lockAmountFinal += amount;
        if(exist){
            addressArray.push(recipient);
        }
    }

    function normalTransfer(address sender,uint256 amount) internal view{

        LockInfo storage lockInfo = lockAmountMapping[sender];

        uint256 availableBalnce = _balances[sender].sub(lockInfo.lockAmount);
        string memory text = "Available Credit:";
        string memory value = Strings.toString(availableBalnce);
        string memory message = text.toSlice().concat(value.toSlice());
        require(amount <= availableBalnce, message);
    }
   
    function calculateRatio(uint256 _amount,uint256 ratio) private pure returns (uint256) {
        return _amount.mul(ratio).div(
            10**5
        );
    }

}
