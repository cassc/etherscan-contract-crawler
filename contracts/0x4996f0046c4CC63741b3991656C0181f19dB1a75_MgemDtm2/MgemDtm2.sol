pragma solidity ^0.8.6;


contract MgemDtm2 is Ownable, Pausable {

    struct NftInfo {
        uint256 tokenId;
        uint256 startTime;
    }

    event GemzClaimed(uint256 earned);

    // reference to the dtm2 NFT contract
    DiscoverTheMandox dtm2;
    // reference to the Points Token contract for $mGEM earnings
    MandoxGem mGem;

    // maps tokenId to NFTInfo
    mapping(uint256 => NftInfo) private register;


    uint256 public earningRate = 10000; // Values in Gem Points tokens for Payout per Day
    uint256 public maxClaimPeriod = 45 days; // Maximum number of days for building rewards

    // amount of $mGEM earned so far
    uint256 public totalEarned;

    constructor(address _dtm2, address _mGem) {
        dtm2 = DiscoverTheMandox(_dtm2);
        mGem = MandoxGem(_mGem);
    }

modifier notInitialized(uint256 tokenId) {
    require(register[tokenId].tokenId == 0, "Already Initialized!");
    _;
}

function startEarn(uint256[] calldata tokenIds) external onlyOwner {
    for (uint i = 0; i < tokenIds.length; i++) {
        initializeNFT(tokenIds[i]);
    }
}

function initializeNFT(uint256 tokenId) internal notInitialized(tokenId) {
    NftInfo storage nft = register[tokenId];
    nft.tokenId = tokenId;
    nft.startTime = block.timestamp;

}


    /* CLAIMING */

    function setMaxClaimPeriod(uint256 _days) external onlyOwner {
        maxClaimPeriod = _days * 1 days; // Convert input days to seconds
}

 
    /*
    * @param tokenIds called from NFt contractto verify only owned tokens claimed
    * @param claim pending calls _claimPending for each id and returns rewards owed.
    */

    function claimPending() external whenNotPaused {
        address sender = _msgSender();
        uint256[] memory tokenIds = dtm2.tokensOfOwner(sender);
        uint256 rewards = 0;
        for (uint i = 0; i < tokenIds.length; i++) {
             rewards += _claimPending(tokenIds[i]);
        }
        if (rewards == 0) return;

        totalEarned += rewards / 1e9;

        mGem.mint(sender, rewards);
}

    /*
    * @param tokenId the ID of the NFT to claim earnings from
    * 
    * @return rewards - the amount earned
    */
    function _claimPending(uint256 tokenId) internal returns (uint256 rewards) {
        require(isInitialized(tokenId), "Token ID Not Registered");
        NftInfo storage owned = register[tokenId];
        require(block.timestamp - owned.startTime > 1 days , "Can only claim once per day");
        

         if (block.timestamp - owned.startTime <= maxClaimPeriod) {
            rewards = uint256((block.timestamp - owned.startTime) / 1 days) * earningRate * (10**9);
         } else { 
            rewards = maxClaimPeriod * earningRate * (10**9);
         }
            //reset rewards time
            owned.startTime = block.timestamp;
        

        emit GemzClaimed(rewards);
    }


    function isInitialized(uint256 tokenId)
        public
        view
        returns (bool _isInitialized)
    {
        return register[tokenId].tokenId == tokenId;
    }

    function totalPendingOwedByIDs(uint256[] calldata tokenIds ) external view returns(uint256 allRewards) {
        for (uint i = 0; i < tokenIds.length; i++) {
            allRewards += _allPending(tokenIds[i]);
        }      
    }

    function totalPendingOwed2(address owner) external view returns(uint256 allRewards) {
        uint256[] memory tokenIds = dtm2.tokensOfOwner(owner);
        for (uint i = 0; i < tokenIds.length; i++) {
            allRewards += _allPending(tokenIds[i]);
        }       
    }

    function _allPending(uint256 tokenId) internal view returns (uint256 rewards) {
        require(isInitialized(tokenId), "Token ID Not Registered");
        NftInfo storage owned = register[tokenId];

         if ( block.timestamp - owned.startTime <= maxClaimPeriod) {
            rewards = uint256((block.timestamp - owned.startTime) / 1 days) * earningRate;
         } else if (block.timestamp - owned.startTime > maxClaimPeriod) {
            rewards = maxClaimPeriod * earningRate;
        }
    }

    function setRewardRate(uint256 value) public onlyOwner{
        earningRate = value;
    }

    /**
    * enables owner to pause / unpause claiming
    */
    function setPaused(bool _paused) external onlyOwner {
        if (_paused) _pause();
        else _unpause();
    }


}