    string private NAME = "OPPCULTURE EQUITY";
    string private SYMBOL = "eCULT";
    uint8 constant DECIMALS = 18;
    uint256 constant TOTAL_SUPPLY = 100 * 10**uint256(DECIMALS); //One hundred tokens is representative one hundred percent.

