        _setTokenURI(tokenId, metadataURI);

        // SAVE IN THE MEMORY
        _holderTokens[msg.sender].add(tokenId);
        userOwnedTokens[msg.sender].push(tokenId);
        
        emit BoxCreated(tokenId);
        emit BoxSold(from, to, planet);
        emit BoxMinted(tokenId);
    }

    function setLastSoldBoxNumber(uint64 lastSoldBoxNumber_) internal {
        lastSoldBoxNumber = lastSoldBoxNumber_;
    }

    function setLevelAmount(uint256 amount, uint level) internal{
        levels[level] = amount;
    }

    function _calculateAmount(uint256 countOfBoxes) internal returns(uint256){
        uint256 sumPrice;

        for(uint i = 1; i <= 10; i++) {
            if(levels[i] > 0){

                if(levels[i] > countOfBoxes){
                    levels[i] = levels[i] - countOfBoxes;
                    sumPrice = pricing[i] * countOfBoxes;
                    break;
                } else {
                    countOfBoxes = countOfBoxes - levels[i];
                    sumPrice = levels[i] * pricing[i];
                    levels[i] = 0;

                    if(levels[i + 1] > countOfBoxes){
                        levels[i + 1] = levels[i + 1] - countOfBoxes;
                        sumPrice = sumPrice + (countOfBoxes * pricing[i + 1]);
                        break;
                    }else{
                        countOfBoxes = countOfBoxes - levels[i + 1];
                        sumPrice = sumPrice + (levels[i + 1] * pricing[i + 1]);
                        levels[i + 1] = 0;

                        if(levels[i + 2] > countOfBoxes){
                            levels[i + 2] = levels[i + 2] - countOfBoxes;
                            sumPrice = sumPrice + (countOfBoxes * pricing[i + 2]);
                            break;
                        }else{
                            countOfBoxes = countOfBoxes - levels[i + 2];
                            sumPrice = sumPrice + (levels[i + 2] * pricing[i + 2]);
                            levels[i + 2] = 0;

                            if(levels[i + 3] > countOfBoxes){
                                levels[i + 3] = levels[i + 3] - countOfBoxes;
                                sumPrice = sumPrice + (countOfBoxes * pricing[i + 3]);
                                break;
                            }else{
                                countOfBoxes = countOfBoxes - levels[i + 3];
                                sumPrice = sumPrice + (levels[i + 3] * pricing[i + 3]);
                                levels[i + 3] = 0;

                                if(levels[i + 4] > countOfBoxes){
                                    levels[i + 4] = levels[i + 4] - countOfBoxes;
                                    sumPrice = sumPrice + (countOfBoxes * pricing[i + 4]);
                                    break;
                                }
                            }
                        }
                    }
                    
                }

            }
        }   

        return sumPrice;
    }

    function setReceivedAmount(uint256 _value) internal {
        _receivedAmount += _value;
    }

    /** ****************************************************************
        EXTERNAL,PUBLIC FUNCTIONS
    */

    function getOwnerTokens(address _address)
        public
        view
        returns (uint256[] memory)
    {
        return userOwnedTokens[_address];
    }

    function totalSupply() public view virtual returns (uint256) {
        return _totalSupply;
    }

    function getLastSoldBoxNumber() public view virtual returns (uint256) {
        return lastSoldBoxNumber;
    }

    function available() public view virtual returns (uint256) {
        return _available;
    }

    function sold() public view virtual returns (uint256) {
        return _sold;
    }

    function getMinAmount() public view virtual returns (uint256) {
        return minAmount;
    }

    function getMaxAmount() public view virtual returns (uint256) {
        return maxAmount;
    }

    function getCurrentIndex() public view virtual returns (uint256) {
        return currentIndex;
    }

    function getBaseURI() public view returns (string memory) {
        return _currentBaseURI;
    }

    function getTokenInfo(uint256 tokenId)
        external
        view
        returns (
            uint64 from,
            uint64 to,
            uint8 planet
        )
    {
        require(_exists(tokenId), "Token not Minted!");
        Metadata memory box = id_to_box[tokenId];
        from = box.from;
        to = box.to;
        planet = box.planet;
    }

    function ownerOf(uint256 tokenId) public view override returns (address) {
        return ownerOf(tokenId);
    }

    function balanceOf(address owner)
        public
        view
        virtual
        override
        returns (uint256)
    {
        require(
            owner != address(0),
            "ERC721: balance query for the zero address"
        );
        return _holderTokens[owner].length();
    }


    // MINTING
    function claim(uint64 countOfBoxes, string memory tokenURI)
        external
        payable
    {
        require(countOfBoxes > 0, "ERC721: minting a non-positive amount");
        require(countOfBoxes <= available(), "ERC721: minting more than available");

        uint256 amount = _calculateAmount(countOfBoxes);
        
        require(msg.value <= maxAmount, "Lot of ETH here; check maxAmount!");
        require(msg.value >= minAmount, "Not enough ETH sent; check minAmount!");
        require(msg.value >= amount, "ERC721: not enought ether");

        mint(countOfBoxes, tokenURI);

        lastSoldPrice = amount;
        setReceivedAmount(msg.value);
        
    }

    function getLastSoldPrice() public view returns (uint256) {
        return lastSoldPrice;
    }

    function getLevelAmount(uint level) public view returns (uint256) {
        return levels[level];
    }

    // Get all level pricing in array
    function getAllAmount() public view returns (uint[] memory) {
        return levels;
    }

    function getPricing(uint level) public view returns (uint256) {
        return pricing[level];
    }

    // Get all level pricing in array
    function getAllPricing() public view returns (uint[] memory) {
        return pricing;
    } 

    function getReceivedAmount() public view returns (uint256){
        return _receivedAmount;
    }

    /** ****************************************************************
        FUNCTIONS FOR ONLY CONTRACT OWNER
     */

    function setPrice(uint256 _price, uint256 _level)
        public
        onlyOwner
        returns (bool)
    {
        require(_price > 0, "Price must be greater than 0!");
        pricing[_level] = _price;

        return true;
    }

    function setMinAmount(uint256 _amount) public onlyOwner {
        minAmount = _amount;
    }

    function setMaxAmount(uint256 _amount) public onlyOwner {
        maxAmount = _amount;
    }

    function setBaseURI(string memory baseURI) public onlyOwner {
        _currentBaseURI = baseURI;
    }

    function withdraw() public payable onlyOwner {
        uint balance = address(this).balance;
        payable(owner()).transfer(balance);
    }
    
}