contract OSAKUMARKETPLACE is ReentrancyGuard {

    // Variables
    address payable public immutable feeAccount; // the account that receives fees
    uint public immutable feePercent; // the fee percentage on sales 
    uint public itemCount;
    ERC20 _token;

    struct Item {
        uint itemId;
        IERC721 nft;
        uint tokenId;
        uint price;
        uint priceOsaku;
        address payable seller;
        bool sold;
        bool listed;
    }

    // itemId -> Item
    mapping(uint => Item) public items;

    event Offered(
        uint itemId,
        address indexed nft,
        uint tokenId,
        uint price,
        uint priceOsaku,
        address indexed seller
    );
    event Bought(
        uint itemId,
        address indexed nft,
        uint tokenId,
        uint price,
        uint priceOsaku,
        address indexed seller,
        address indexed buyer,
        string BoughtWith
    );
    event Delisted(
        uint itemId,
        address indexed nft,
        uint tokenId,
        address indexed owner
    );


    modifier checkAllowance(uint amount) {
        require(_token.allowance(msg.sender, address(this)) >= amount, "Error");
        _;
    }

    constructor() {
        feeAccount = payable(0x68ae7Ee1580BB5713B610471cbDFFF906D02E1BF);
        feePercent = 4;
        _token = ERC20(0xCBCCBb06F572df174a7DA47684cdC28121fec421);
    }

    // Make item to offer on the marketplace
    function makeItem(IERC721 _nft, uint _tokenId, uint _price, uint _priceOsaku) external nonReentrant {
        require(_price > 0, "Price must be greater than zero");
        // increment itemCount
        itemCount ++;
        // transfer nft
        _nft.transferFrom(msg.sender, address(this), _tokenId);
        // add new item to items mapping
        items[itemCount] = Item (
            itemCount,
            _nft,
            _tokenId,
            _price,
            _priceOsaku,
            payable(msg.sender),
            false,
            true
        );
        // emit Offered event
        emit Offered(
            itemCount,
            address(_nft),
            _tokenId,
            _price,
            _priceOsaku,
            msg.sender
        );
    }

    function purchaseItemWithEth(uint _itemId) external payable nonReentrant {
        uint _totalPrice = getTotalPrice(_itemId);
        Item storage item = items[_itemId];
        require(_itemId > 0 && _itemId <= itemCount, "item doesn't exist");
        require(msg.value >= _totalPrice, "not enough ether to cover item price and market fee");
        require(!item.sold, "item already sold");
        // pay seller and feeAccount
        item.seller.transfer(item.price);
        feeAccount.transfer(_totalPrice - item.price);
        // update item to sold
        item.sold = true;
        item.listed = false;
        // update owner
        item.seller = payable(msg.sender);
        // transfer nft to buyer
        item.nft.transferFrom(address(this), msg.sender, item.tokenId);
        // emit Bought event
        emit Bought(
            _itemId,
            address(item.nft),
            item.tokenId,
            item.price,
            item.priceOsaku,
            item.seller,
            msg.sender,
            'ETH'
        );
    }

    function purchaseItemWithOsaku(uint _itemId) external payable nonReentrant {
        Item storage item = items[_itemId];
        require(_itemId > 0 && _itemId <= itemCount, "item doesn't exist");
        //require(msg.value >= _totalPrice, "not enough ether to cover item price and market fee");
        require(!item.sold, "item already sold");
        // update item to sold
        item.sold = true;
        item.listed = false;
        // update owner
        item.seller = payable(msg.sender);
        // transfer nft to buyer
        item.nft.transferFrom(address(this), msg.sender, item.tokenId);
        // emit Bought event
        emit Bought(
            _itemId,
            address(item.nft),
            item.tokenId,
            item.price,
            item.priceOsaku,
            item.seller,
            msg.sender,
            'OSAKU'
        );
    }

    function getTotalPrice(uint _itemId) view public returns(uint){
        return((items[_itemId].price*(100 + feePercent))/100);
    }

    function getTotalPriceOsaku(uint _itemId) view public returns(uint) {
        return((items[_itemId].priceOsaku*(100 + feePercent))/100);
    }

    function changePriceEth(uint _itemId, uint _newPrice) external nonReentrant {
        Item storage item = items[_itemId];
        item.price = _newPrice;
    }

    function changePriceOsaku(uint _itemId, uint _newPrice) external nonReentrant {
        Item storage item = items[_itemId];
        item.priceOsaku = _newPrice;
    }

    function delistItem(uint _itemId) external nonReentrant {
        Item storage item = items[_itemId];
        item.nft.transferFrom(address(this), msg.sender, item.tokenId);
        item.listed = false;
        emit Delisted(
            _itemId,
            address(item.nft),
            item.tokenId,
            msg.sender
        );
    }

    function listItem(IERC721 _nft, uint _itemId, uint _price, uint _priceOsaku) external nonReentrant {
        Item storage item = items[_itemId];
        require(_price > 0, "Price must be greater than zero");
        require(_nft.ownerOf(item.tokenId)==msg.sender);
        // transfer nft from owner to marketplace
        _nft.transferFrom(msg.sender, address(this), item.tokenId);
        item.sold = false;
        item.listed = true;
        // Edit item info
        item.price = _price;
        item.priceOsaku = _priceOsaku;
        // emit Offered event
        emit Offered(
            itemCount,
            address(item.nft),
            item.tokenId,
            _price,
            _priceOsaku,
            msg.sender
        );
    }

}