    pragma solidity >= 0.8.13;






    contract AIGirls is ERC721, AdminControl {
        
        uint256 private _royaltyAmount; //in % 
        uint256 public _nameChangePrice = 0.02*10**18;
        uint256 public _mintPrice = 0.15*10**18;
        uint256 public _tokenId;

        bool public _nameChangeActivated = false;
        bool public _mintOpened = false;

        mapping (uint256 => string) _AIGirlName;
        address payable private _royalties_recipient;
        string[] private _uriComponents;

        string _uri = "https://arweave.net/TGQBuH4zFZSz8q6Q_2msS1bhcjIRQx6yAccA94WG444/";
        
        constructor () ERC721("AIGirl", "AIGirl") {
            _royalties_recipient = payable(msg.sender);
            _royaltyAmount = 10;
            _uriComponents = [
                'data:application/json;utf8,{"name":"',
                '", "description":"',
                '", "created_by":"Elypse", "image":"',
                '", "image_url":"',
                '"}'];
            _tokenId = 0;
        } 

        function supportsInterface(bytes4 interfaceId)
            public
            view
            virtual
            override(ERC721, AdminControl)
            returns (bool)
        {
            return
            AdminControl.supportsInterface(interfaceId) ||
            ERC721.supportsInterface(interfaceId) ||
            interfaceId == type(IEIP2981).interfaceId ||
            super.supportsInterface(interfaceId);
        }

        function adminMint(
            address account
        ) external adminRequired{
            _mint(account, _tokenId);
            _AIGirlName[_tokenId] = Strings.toString(_tokenId);
            _tokenId += 1;
        }

        function togglePublicMint()external adminRequired{
            _mintOpened =! _mintOpened;
        }

        function publicMint(
        ) external payable{
            require(_mintOpened,"Mint currently closed");
            require(msg.value >= _mintPrice, "Insufficient funds");
            payable(_royalties_recipient).transfer(_mintPrice);
            _mint(msg.sender, _tokenId);
            _AIGirlName[_tokenId] = Strings.toString(_tokenId);
            _tokenId += 1;
        }

        function setURI(string calldata updatedURI) external adminRequired{
            _uri = updatedURI;
        }

        function setMintPrice(uint256 mintPrice) external adminRequired{
            _mintPrice = mintPrice;
        }

        function toggleNameChange()external adminRequired{
            _nameChangeActivated =! _nameChangeActivated;
        }


        function setNameChangePrice(uint256 newPrice) external adminRequired{
            _nameChangePrice = newPrice;
        }

        function nameGirl(uint256 tokenId, string calldata girlName) external payable{
            require(ownerOf(tokenId) == msg.sender, "Only its owner can name a girl");
            require(msg.value >= _nameChangePrice, "Not enough funds to change name");
            require(_nameChangeActivated, "Name change noot activated");
            payable(_royalties_recipient).transfer(_nameChangePrice);
            _AIGirlName[tokenId] = girlName;
        }

        function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
            require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        
            bytes memory byteString = abi.encodePacked(
                abi.encodePacked(_uriComponents[0], _AIGirlName[tokenId]),
                abi.encodePacked(_uriComponents[1], "This collection is made up of 1/1 items in 3d of AI semi robot/human girls created by AI and ElyX and minted through an intelligent contract that will allow the owner of each item give their AI Girl a proper name"),
                abi.encodePacked(_uriComponents[2],_uri, Strings.toString(tokenId), ".png"),
                abi.encodePacked(_uriComponents[3], _uri, Strings.toString(tokenId), ".png"),
                abi.encodePacked(_uriComponents[4]));
            return string(byteString);
        }

        function burn(uint256 tokenId) public {
            require(ownerOf(tokenId)== msg.sender, "You can only burn your own tokens");
            _burn(tokenId);
        }

        function setRoyalties(address payable _recipient, uint256 _royaltyPerCent) external adminRequired {
            _royalties_recipient = _recipient;
            _royaltyAmount = _royaltyPerCent;
        }

        function royaltyInfo(uint256 salePrice) external view returns (address, uint256) {
            if(_royalties_recipient != address(0)){
                return (_royalties_recipient, (salePrice * _royaltyAmount) / 100 );
            }
            return (address(0), 0);
        }

        function withdraw(address recipient) external adminRequired {
            payable(recipient).transfer(address(this).balance);
        }

    }