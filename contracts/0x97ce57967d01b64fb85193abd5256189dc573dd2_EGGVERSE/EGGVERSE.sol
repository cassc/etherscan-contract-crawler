// File: EGGVERSE.sol


pragma solidity ^0.8.4;










/*
 *  EGGLORD Dimensional Gate Engineer Guild
 *
 *  EGGLORD Dimensional Gate Engineer Guild is a group of guardians who protect the universe of all dimensions from the collapse of the 9th element that started on Mars 0.
 */
contract EGGVERSE is ERC721, ERC721URIStorage, Pausable, Ownable, ERC721Burnable, EIP712, ERC721Votes {
    address payable private _owner;

    // To prevent bot attack, the last contract call block number is recorded.
    mapping(address => uint256) private _lastCallBlockNumber;
    uint256 private _antibotInterval;

    // If someone burns NFT in the middle of minting,
    // the tokenId will go wrong, so the index instead of totalSupply() is used.
    uint256 private _mintIndexForSale;

    // Public Mint
    uint256 private _publicMintLimitPerBlock;                   // Maximum purchase nft per person per block
    uint256 private _publicMintLimitPerSale;                    // Maximum purchase nft per person per sale
    uint256 private _publicMintStartBlockNumber;                // In blockchain, blocknumber is the standard of time.
    uint256 private _publicMintPrice;                           // 1 ether = 1000000000000000000 wei

    // Whitelist Mint
    uint256 private _whitelistMintLimitPerBlock;                // Maximum purchase nft per person per block
    uint256 private _whitelistMintLimitPerSale;                 // Maximum purchase nft per person per sale
    uint256 private _whitelistMintStartBlockNumber;             // In blockchain, blocknumber is the standard of time.
    uint256 private _whitelistMintPrice;                        // 1 ether = 1000000000000000000 wei

    // Common
    uint256 private _mintSeason;                                // Minting season number
    uint256 private _maxSaleAmountTotal;                        // Maximum purchase volume in total
    uint256 private _maxSaleAmountPerSeason;                    // Maximum purchase volume for each mint season
    mapping(uint256 => uint256) private seasonSales;            // The number of nfts sold for each mint season

    string private _tokenBaseURI;
    string private baseURI;
    string private notRevealedUri;
    uint256 private balance;
    mapping(uint256 => uint256) private revealStatus;           // Reveal status for each mint season, 1 = true, 0 = false
    mapping(uint256 => uint256) private seasonOfTokens;         // Season information of each tokens

    constructor() ERC721("EGGLORD Dimensional Gate Engineer Guild", "EDGEG") EIP712("EGGLORD Dimensional Gate Engineer Guild", "EDGEG") {
        _owner = payable(msg.sender);
        _mintIndexForSale = 1;
        _mintSeason = 1;
    }

    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    function _notRevealedURI() internal view returns (string memory) {
        return notRevealedUri;
    }

    function getPublicBalanceOf(uint256 mintSeason, address _address) external view returns (uint256) {
        return publicMintedWallets[mintSeason][_address];
    }

    function getWhitelistBalanceOf(uint256 mintSeason, address _address) external view returns (uint256) {
        return whitelistMintedWallets[mintSeason][_address];
    }

    function getSeasonSales(uint256 season) external view returns (uint256) {
        return seasonSales[season];
    }

    function getSeasonOfToken(uint256 tokenId) external view returns (uint256) {
        return seasonOfTokens[tokenId];
    }

    function getRevealStatus(uint256 season) external view returns (uint256) {
        return revealStatus[season];
    }

    function setBaseURI(string memory _newBaseURI) public onlyOwner {
        baseURI = _newBaseURI;
    }

    function setNotRevealedURI(string memory _newNotRevealedURI) public onlyOwner {
        notRevealedUri = _newNotRevealedURI;
    }

    function setRevealStatus(uint256 season, uint256 _state) external onlyOwner {
        require(_state == 1 || _state == 0, "Invalid state value");
        revealStatus[season] = _state;
    }

    function setSeasonOfToken(uint256 tokenId, uint256 season) external onlyOwner {
        seasonOfTokens[tokenId] = season;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "URI query for nonexistent token"
        );
        
        if(revealStatus[seasonOfTokens[tokenId]] == 0) {
            string memory currentNotRevealedUri = _notRevealedURI();
            return bytes(currentNotRevealedUri).length > 0
                ? string(abi.encodePacked(currentNotRevealedUri, Strings.toString(tokenId), ".json"))
                : "";
        }
        string memory currentBaseURI = _baseURI();
        return bytes(currentBaseURI).length > 0
            ? string(abi.encodePacked(currentBaseURI, Strings.toString(tokenId), ".json"))
            : "";
    }

    // Withdraw
    // =============================================================================
    function getBalance() public view onlyOwner returns(uint256) {
        return address(this).balance;
    }

    function withdrawAll() external onlyOwner {
        _owner.transfer(getBalance());
    }

    function withdrawAmount(uint256 amount) external onlyOwner {
        require(amount <= getBalance(), "insufficient balance");
        _owner.transfer(amount);
    }

    function transferAmount(address payable to, uint256 amount) external onlyOwner {
        require(amount <= getBalance(), "insufficient balance");
        to.transfer(amount);
    }
    // =============================================================================

    function mintingInformation() external view returns (uint256[15] memory) {
        uint256 currenrMintSeason = _mintSeason;

        uint256[15] memory info =
            [_antibotInterval,
            _publicMintLimitPerBlock,
            _publicMintLimitPerSale,
            _publicMintStartBlockNumber,
            _publicMintPrice,
            _whitelistMintLimitPerBlock,
            _whitelistMintLimitPerSale,
            _whitelistMintStartBlockNumber,
            _whitelistMintPrice,
            _mintSeason,
            _maxSaleAmountTotal,
            _maxSaleAmountPerSeason,
            _mintIndexForSale,
            seasonSales[currenrMintSeason],
            revealStatus[currenrMintSeason]];
        return info;
    }

    function setupSale(uint256 antibotInterval, 
                       uint256 publicMintLimitPerBlock,
                       uint256 publicMintLimitPerSale,
                       uint256 publicMintStartBlockNumber,
                       uint256 publicMintPrice,
                       uint256 whitelistMintLimitPerBlock,
                       uint256 whitelistMintLimitPerSale,
                       uint256 whitelistMintStartBlockNumber,
                       uint256 whitelistMintPrice,
                       uint256 mintSeason,
                       uint256 maxSaleAmountTotal,
                       uint256 maxSaleAmountPerSeason) external onlyOwner {

        _antibotInterval = antibotInterval;
        _publicMintLimitPerBlock = publicMintLimitPerBlock;
        _publicMintLimitPerSale = publicMintLimitPerSale;
        _publicMintStartBlockNumber = publicMintStartBlockNumber;
        _publicMintPrice = publicMintPrice;

        _whitelistMintLimitPerBlock = whitelistMintLimitPerBlock;
        _whitelistMintLimitPerSale = whitelistMintLimitPerSale;
        _whitelistMintStartBlockNumber = whitelistMintStartBlockNumber;
        _whitelistMintPrice = whitelistMintPrice;

        _mintSeason = mintSeason;
        _maxSaleAmountTotal = maxSaleAmountTotal;
        _maxSaleAmountPerSeason = maxSaleAmountPerSeason;
    }

    //Public Mint
    // =============================================================================
    bool public publicMintEnabled;
    mapping(uint256 => mapping(address => uint256)) private publicMintedWallets;

    function setPublicMintEnabled(bool _state) public onlyOwner {
        publicMintEnabled = _state;
    }

    function publicMint(uint256 requestedCount) external payable {
        uint256 currenrMintSeason = _mintSeason;

        require(publicMintEnabled, "Public sale is not enabled");
        require(_lastCallBlockNumber[msg.sender] + _antibotInterval < block.number, "Bot is not allowed");
        require(block.number >= _publicMintStartBlockNumber, "Not yet started");
        require(_mintIndexForSale + requestedCount <= _maxSaleAmountTotal + 1, "Exceed max amount in total");
        require(seasonSales[currenrMintSeason] + requestedCount <= _maxSaleAmountPerSeason, "Exceed max amount per season");
        require(msg.value == _publicMintPrice * requestedCount, "Not enough ether");
        require(requestedCount > 0 && requestedCount <= _publicMintLimitPerBlock, "Too many requests or zero request");
        require(publicMintedWallets[currenrMintSeason][msg.sender] + requestedCount <= _publicMintLimitPerSale, "Exceed max amount per person");

        for(uint256 i = 0; i < requestedCount; i++) {
            _mint(msg.sender, _mintIndexForSale);
            seasonOfTokens[_mintIndexForSale] = currenrMintSeason;
            _mintIndexForSale += 1;
        }

        _lastCallBlockNumber[msg.sender] = block.number;
        publicMintedWallets[currenrMintSeason][msg.sender] += requestedCount;
        seasonSales[currenrMintSeason] += requestedCount;
    }
    // =============================================================================

    // Whitelist Mint
    // =============================================================================
    bool public whitelistMintEnabled;
    mapping(uint256 => mapping(address => uint256)) private whitelistMintedWallets;
    bytes32 public merkleRoot;

    function setMerkleRoot(bytes32 _merkleRoot) public onlyOwner {
      merkleRoot = _merkleRoot;
    }

    function setWhitelistMintEnabled(bool _state) public onlyOwner {
      whitelistMintEnabled = _state;
    }

    function whitelistMint(uint256 requestedCount, bytes32[] calldata _merkleProof) external payable {
        uint256 currenrMintSeason = _mintSeason;

        require(whitelistMintEnabled, "Whitelist sale is not enabled");
        require(_lastCallBlockNumber[msg.sender] + _antibotInterval < block.number, "Bot is not allowed");
        require(block.number >= _whitelistMintStartBlockNumber, "Not yet started");
        require(_mintIndexForSale + requestedCount <= _maxSaleAmountTotal + 1, "Exceed max amount in total");
        require(seasonSales[currenrMintSeason] + requestedCount <= _maxSaleAmountPerSeason, "Exceed max amount per season");
        require(msg.value == _whitelistMintPrice * requestedCount, "Not enough ether");
        require(requestedCount > 0 && requestedCount <= _whitelistMintLimitPerBlock, "Too many requests or zero request");
        require(whitelistMintedWallets[currenrMintSeason][msg.sender] + requestedCount <= _whitelistMintLimitPerSale, "Exceed max amount per person");

        bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
        require(MerkleProof.verify(_merkleProof, merkleRoot, leaf), "Invalid proof");

        for(uint256 i = 0; i < requestedCount; i++) {
            _mint(msg.sender, _mintIndexForSale);
            seasonOfTokens[_mintIndexForSale] = currenrMintSeason;
            _mintIndexForSale += 1;
        }

        _lastCallBlockNumber[msg.sender] = block.number;
        whitelistMintedWallets[currenrMintSeason][msg.sender] += requestedCount;
        seasonSales[currenrMintSeason] += requestedCount;
    }
    // =============================================================================

    function adminMint(uint256 requestedCount, uint256 season) external payable onlyOwner {
        require(_mintIndexForSale + requestedCount <= _maxSaleAmountTotal + 1, "Exceed max amount in total");
        require(seasonSales[season] + requestedCount <= _maxSaleAmountPerSeason, "Exceed max amount per season");
        require(requestedCount > 0, "zero request");

        for(uint256 i = 0; i < requestedCount; i++) {
            _mint(_owner, _mintIndexForSale);
            seasonOfTokens[_mintIndexForSale] = season;
            _mintIndexForSale += 1;
        }

        seasonSales[season] += requestedCount;
    }

    function airDropMint(address user, uint256 requestedCount, uint256 season) external onlyOwner {
        require(_mintIndexForSale + requestedCount <= _maxSaleAmountTotal + 1, "Exceed max amount in total");
        require(seasonSales[season] + requestedCount <= _maxSaleAmountPerSeason, "Exceed max amount per season");
        require(requestedCount > 0, "zero request");

        for(uint256 i = 0; i < requestedCount; i++) {
            _mint(user, _mintIndexForSale);
            seasonOfTokens[_mintIndexForSale] = season;
            _mintIndexForSale += 1;
        }

        seasonSales[season] += requestedCount;
    }

    // OpenZeppelin built-in functions
    // =============================================================================
    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        whenNotPaused
        override
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _afterTokenTransfer(address from, address to, uint256 tokenId)
        internal
        override(ERC721, ERC721Votes)
    {
        super._afterTokenTransfer(from, to, tokenId);
    }

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }
    // =============================================================================
}