contract DisperseETH is Ownable {

    function disperse(address[] calldata wallets) payable external {
        require(wallets.length >= 2, "Can't disperse to a single wallet, just transfer it you dope!");

        uint256 amountPerWallet = SafeMath.div(msg.value,wallets.length);

        if (amountPerWallet < 0.001 ether) {
            revert("Min 0.001 per wallet, take your pennies elsewhere!");
        }

        for(uint i=0; i<wallets.length; i++) {
            payable(wallets[i]).transfer(amountPerWallet);
        }
    }

    function withdraw(address wallet) external onlyOwner {
        require(wallet != address(0), "Can't withdraw to null address");
        require(address(this).balance > 0, "Nothing to withdraw");

        payable(wallet).transfer(address(this).balance);
    }
}