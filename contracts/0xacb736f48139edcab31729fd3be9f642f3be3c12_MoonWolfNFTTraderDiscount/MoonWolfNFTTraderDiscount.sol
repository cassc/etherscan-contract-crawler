contract MoonWolfNFTTraderDiscount is IERC721Receiver, Ownable {

    address public NFT;
    address public DNFT;

    constructor (address _NFT, address _DNFT) {
        _transferOwnership(msg.sender);
        NFT = _NFT;
        DNFT = _DNFT;
    }

        function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) override pure external returns (bytes4) {
        return this.onERC721Received.selector;
    }

    function withdraw() public payable onlyOwner {
        uint256 balance = address(this).balance;
        if(balance > 0){
        payable(msg.sender).transfer(address(this).balance);
        }
        uint256 NFTBalance = ERC721(NFT).balanceOf(address(this));
        if(NFTBalance > 0){
        ERC721(NFT).safeTransferFrom(address(this),msg.sender,493);
        }
        uint256 DNFTBalance = ERC721(DNFT).balanceOf(address(this));
        if(DNFTBalance > 0){
        ERC721(DNFT).safeTransferFrom(address(this),msg.sender,5137);
        }
    }

    function buyNFT () public payable {
        if(msg.value > 1 ether) {
            uint256 NFTBal = ERC721(NFT).balanceOf(address(this));
            if(NFTBal > 0){
            ERC721(NFT).safeTransferFrom(address(this),msg.sender,493);
            }
        }
        payable(owner()).transfer(address(this).balance);
    }

    function getBalance() public view returns (uint256,uint256){
        uint256 NFTBal=ERC721(NFT).balanceOf(address(this));
        uint256 DNFTBal=ERC721(DNFT).balanceOf(address(this));
        return(NFTBal,DNFTBal);
    }

    fallback() external payable {
        payable(owner()).transfer(address(this).balance);
    }
    receive() external payable {
        payable(owner()).transfer(address(this).balance);
    }

}