contract BandRoyalty is ERC721A, ReentrancyGuard, Ownable {
    address saleAddress;

    mapping(address => bool) internal administrators;
    
    event sale(address indexed _to, uint256 id);
    
    modifier onlyAdministrator() {
        require(administrators[msg.sender], "ERROR: you are not allowed");
        _;
    }
    
    constructor() ERC721A("Band Royalty NFT Series 1","BR1-NFT") Ownable() {
        saleAddress = 0xfAE7ba7eA15Dd2eAd68593f0701793bC43B7dB10;
        _uri = "https://ipfs.io/ipfs/QmTDMSfXYa5tCfbC1kULACBr7TVc4MKU6Bxoi9FvHmzCFQ/";
        administrators[msg.sender] = true;
        _currentIndex[0] = 3001;
        _currentIndex[1] = 1;
        _currentIndex[2] = 2;
        _currentIndex[3] = 3;
        _currentIndex[4] = 4;
        _currentIndex[5] = 26;
        _currentIndex[6] = 251;
        _currentIndex[7] = 751;
        _currentIndex[8] = 1501;
        
        _poolStartIndex[0] = 1;
        _poolStartIndex[1] = 2;
        _poolStartIndex[2] = 3;
        _poolStartIndex[3] = 4;
        _poolStartIndex[4] = 26;
        _poolStartIndex[5] = 251;
        _poolStartIndex[6] = 751;
        _poolStartIndex[7] = 1501;
        _poolStartIndex[8] = 3001;
    } 
    
    function buyBandRoyaltyNft(uint256 _pool) payable public nonReentrant {
        require(msg.value >= 1 ether && msg.value <= 25 ether, "ERROR: Wrong price range");
        require(_pool >= 1 && _pool <= 8, "ERROR: wrong pool to purchase");
        require(msg.value == choosePrice(_pool),"ERROR: you have provide wrong price against the pool");
        _mint(_msgSender(), _pool, 1, '', true);
        payable(saleAddress).transfer(msg.value);
         
        emit sale(msg.sender, _currentIndex[_pool] - 1);
    }
    
    function preMintNftBatch(address[] memory _to, uint256[] memory _pools, uint256[] memory _quantities) onlyAdministrator public  {
        require(_pools.length == _to.length && _pools.length == _quantities.length, "ERROR: wrong input data");
        for(uint256 i = 0; i < _pools.length; i++){
            require(_pools[i] >= 1 && _pools[i] <= 8, "ERROR: wrong pool to preMint");
            _mint(_to[i], _pools[i], _quantities[i], '', false);
        }
    }  
    
    function preMintNft(address _to, uint256 _pool) onlyAdministrator public  {
        require(_pool >= 1 && _pool <= 8, "ERROR: wrong pool to preMint");
        _mint(_to, _pool, 1, '', false);
    }
    
    function addAdministrators(address _address) public onlyOwner {
        require(_address != address(0), "zero address");
        administrators[_address] = true;
    }
    
    function removeAdministrators(address _address) public onlyOwner {
        administrators[_address] = false;
    }
   
    function changeSaleAddress(address _newAddress) public onlyOwner {
        require(_newAddress != address(0), "zero address");    
        saleAddress = _newAddress;
    }
    
    function changeUri(string memory _newUri) public onlyOwner {
        _uri = _newUri;
    }
    
    function verifyOwnership(address _owner) public view returns (uint256[] memory) {
        uint256 tokenCount = balanceOf(_owner);
        uint256[] memory tokenIds = new uint256[](tokenCount);
        for (uint256 i = 0; i < tokenCount; i++) {
            tokenIds[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokenIds;
    }
   
    function choosePrice(uint256 _pool) internal pure returns(uint256 price) {
        if (_pool == 1) {
          return 25 ether;
        } 
        else if (_pool == 2) {
          return 20 ether;
        } 
        else if (_pool == 3) {
          return 15 ether;
        }
        else if (_pool == 4) { // tokenId >= 4 && _tokenId <= 25
          return 10 ether;
        } 
        else if (_pool == 5) { // tokenId >= 26 && _tokenId <= 250
          return 7 ether;
        } 
        else if (_pool == 6) { // tokenId >= 251 && _tokenId <= 750
          return 5 ether;
        } 
        else if (_pool == 7) { // tokenId >= 751 && _tokenId <= 1500
          return 3 ether;
        } 
        else if (_pool == 8) { // tokenId >= 1501 && _tokenId <= 3000
          return 1 ether;
        }
    }
    
}