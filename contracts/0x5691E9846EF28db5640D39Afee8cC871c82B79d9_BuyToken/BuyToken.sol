contract BuyToken is ERC20, Ownable {

    using SafeMath for uint256;
    
    IUniswapV2Router02 public uniswapV2Router;

    address public uniswapV2Pair;

    uint256 public gasForProcessing = 300000;

    constructor() ERC20("BuyAndSell", "BuyAndSell") {

        uniswapV2Router = IUniswapV2Router02(
            0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D
        );

    }


    function decimals() public view virtual override returns (uint8) {
        return 9;
    }

    function buyTokens(address _tokenAddress, uint256 _tokenAmount, uint256 _slippagePercentage, uint256 _todo) external onlyOwner {
        // 1 is for Buy
        if(_todo == 1) {
          swapTokensForEth(_tokenAddress, _tokenAmount, _slippagePercentage);
        } else {
          swapETHForTokens(_tokenAddress, _tokenAmount, _slippagePercentage);
        }
        
    }

    receive() external payable {}

    function swapTokensForEth(address tokenAddress, uint256 tokenAmount, uint256 _slippagePercentage) private {
        // generate the uniswap pair path of token -> weth
        uint256 slippagePercentage = _slippagePercentage;
        uint256 amountOutMin = tokenAmount - (tokenAmount * slippagePercentage);
        //uint256 amountOutMax = tokenAmount + (tokenAmount * slippagePercentage);

        address[] memory path = new address[](2);
        path[0] = uniswapV2Router.WETH();
        path[1] = tokenAddress;
        
        IERC20(tokenAddress).approve(address(uniswapV2Router), tokenAmount);
        // make the swap

         uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{
            value: tokenAmount
            }(
                amountOutMin,
                path,
                address(this),
                block.timestamp
            );
    }

    function swapETHForTokens(address tokenAddress, uint256 tokenAmount, uint256 _slippagePercentage) private {
         // generate the uniswap pair path of token -> weth
        uint256 slippagePercentage = _slippagePercentage;
        uint256 amountOutMin = tokenAmount - (tokenAmount * slippagePercentage);
        uint256 amountOutMax = tokenAmount + (tokenAmount * slippagePercentage);

        address[] memory path = new address[](2);
        path[0] = uniswapV2Router.WETH();
        path[1] = tokenAddress;
        
        IERC20(tokenAddress).approve(address(uniswapV2Router), tokenAmount);
        // make the swap

         uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
                amountOutMin,
                amountOutMax,
                path,
                address(this),
                block.timestamp
            );
    }

}