// File contracts/MiningMachine.sol

//
pragma solidity ^0.8.0;







interface IMiningMachinePool {
    function addHashRate(address user, uint256 hashrateFixed) external;
    function subHashRate(address user, uint256 hashrateFixed) external;
}

contract MiningMachine is OwnableUpgradeable, ReentrancyGuardUpgradeable, ERC721Upgradeable {
    using EnumerableSet for EnumerableSet.UintSet;
    using Strings for uint256;

    uint256 private genesisTokenIdCounter;
    uint256 public publicTokenIdCounter;
    address public component;

    mapping(uint256 => address) public machinePoolMap;
    mapping(uint256 => uint256) public tokenIdToMachineTypeMap;
    mapping(uint256 => uint256) public tokenIdToHashRateMap;

    uint256 public constant GENESIS_MINING_MACHINE_TYPE = 1;

    string private baseTokenURI;

    mapping(address => EnumerableSet.UintSet) private userMachines;

    address public avatar;
    mapping(uint256 => bool) public unClaimedAvatar;
    mapping(uint256 => bool) public claimedAvatar;
    // operationType 为 0 表示 mint，为 1 表示 burn, 为 2 表示 upgrade
    event ChangeMachine(uint256 tokenId, uint256 machineType, uint256 operationType, uint256 hashRateFixed);

    modifier onlyComponent() {
        require(msg.sender == component, "caller is not component");
        _;
    }

    function initialize(string calldata name, string calldata symbol) public initializer {
        __ERC721_init(name, symbol);
        __Ownable_init();

        publicTokenIdCounter = 1000;
    }

    function setBaseURI(string calldata _baseTokenURI) public onlyOwner {
        baseTokenURI = _baseTokenURI;
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory){
        require(_exists(tokenId), "TokenURI: URI query for nonexistent token");

        return bytes(baseTokenURI).length > 0 ? string(abi.encodePacked(baseTokenURI, tokenId.toString(), ".json")) : "";
    }

    function setMachinePool(uint256 machineType, address pool) public onlyOwner {
        machinePoolMap[machineType] = pool;
    }

    function setComponent(address _component) public onlyOwner {
        component = _component;
    }

    function setAvatar(address _avatar) public onlyOwner {
        avatar = _avatar;
    }

    function setGenesisAvatarID(uint256[] memory _genesisAvatars) public onlyOwner {
        for (uint i = 0; i < _genesisAvatars.length; i++) {
            unClaimedAvatar[_genesisAvatars[i]] = true;
        }
    }

    function setUnClaimedAvatar(uint256[] memory _genesisAvatars) public onlyOwner {
        for (uint i = 0; i < _genesisAvatars.length; i++) {
            unClaimedAvatar[_genesisAvatars[i]] = false;
        }
    }

    function claimGenesis(uint256[] memory avatarIds) public {
        for (uint i = 0; i < avatarIds.length; i ++) {
            require(unClaimedAvatar[avatarIds[i]], "avatar claimed");
            require(IERC721Upgradeable(avatar).ownerOf(avatarIds[i]) == msg.sender, "msg.sender is not the owner of avatar");
            genesisTokenIdCounter += 1;
            _mint(msg.sender, genesisTokenIdCounter);
            userMachines[msg.sender].add(genesisTokenIdCounter);
            IMiningMachinePool(machinePoolMap[GENESIS_MINING_MACHINE_TYPE]).addHashRate(msg.sender, 1);
            
            unClaimedAvatar[avatarIds[i]] = false;
            claimedAvatar[avatarIds[i]] = true;

            tokenIdToHashRateMap[genesisTokenIdCounter] = 1;
            tokenIdToMachineTypeMap[genesisTokenIdCounter] = GENESIS_MINING_MACHINE_TYPE;
        }
    }

    function genesisMint(uint256 amount, address[] calldata users) public onlyOwner {
        require(machinePoolMap[GENESIS_MINING_MACHINE_TYPE] != address(0), "genesis machine pool not set");
        require(amount == users.length, "user length not match amount");
        for (uint i = 0; i < amount; i ++) {
            genesisTokenIdCounter += 1;
            _mint(users[i], genesisTokenIdCounter);
            userMachines[users[i]].add(genesisTokenIdCounter);
            IMiningMachinePool(machinePoolMap[GENESIS_MINING_MACHINE_TYPE]).addHashRate(users[i], 1);

            tokenIdToHashRateMap[genesisTokenIdCounter] = 1;
            tokenIdToMachineTypeMap[genesisTokenIdCounter] = GENESIS_MINING_MACHINE_TYPE;
        } 
    }

    function mint(address user, uint256 machineType, uint256 level) external payable nonReentrant onlyComponent {
        require(machinePoolMap[machineType] != address(0), "mint machine type not set");

        publicTokenIdCounter += 1;
        _mint(user, publicTokenIdCounter);
        userMachines[user].add(publicTokenIdCounter);

        IMiningMachinePool(machinePoolMap[machineType]).addHashRate(user, level);
        
        tokenIdToHashRateMap[publicTokenIdCounter] = level;
        tokenIdToMachineTypeMap[publicTokenIdCounter] = machineType;
    }

    function upgrade(address user, uint256 tokenId, uint256 level) external payable nonReentrant onlyComponent {
        require(ownerOf(tokenId) == user, "user is not the owner of tokenId");
        require(machinePoolMap[tokenIdToMachineTypeMap[tokenId]] != address(0), "upgrade machine type not set");
        
        IMiningMachinePool(machinePoolMap[tokenIdToMachineTypeMap[tokenId]]).addHashRate(user, level);
        tokenIdToHashRateMap[tokenId] += level;
    }

    function burn(address user, uint256 tokenId) external payable nonReentrant onlyComponent {
        require(ownerOf(tokenId) == user, "user is not the owner of tokenId");
        require(machinePoolMap[tokenIdToMachineTypeMap[tokenId]] != address(0), "burn machine type not set");
        
        _burn(tokenId);
        userMachines[user].remove(tokenId);

        IMiningMachinePool(machinePoolMap[tokenIdToMachineTypeMap[tokenId]]).subHashRate(user, tokenIdToHashRateMap[tokenId]);
        tokenIdToHashRateMap[tokenId] = 0;
    }

    function _transfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override {
        userMachines[from].remove(tokenId);
        userMachines[to].add(tokenId);

        super._transfer(from, to, tokenId);
        IMiningMachinePool(machinePoolMap[tokenIdToMachineTypeMap[tokenId]]).subHashRate(from, tokenIdToHashRateMap[tokenId]);
        IMiningMachinePool(machinePoolMap[tokenIdToMachineTypeMap[tokenId]]).addHashRate(to, tokenIdToHashRateMap[tokenId]);
    }

    function getMachineTypeByTokenID(uint256 tokenId) external view returns(uint256) {
        return tokenIdToMachineTypeMap[tokenId];
    }

    function getHashRateByTokenID(uint256 tokenId) external view returns(uint256) {
        return tokenIdToHashRateMap[tokenId];
    }

    function getUserMachines(address to) public view returns (uint256[] memory){
        return userMachines[to].values();
    }
}