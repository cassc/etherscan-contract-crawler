        address[] calldata _addresses,
        bool[] calldata _values
    ) external {
        require(_addresses.length == _values.length); // dev: Wrong lengths
        for (uint8 i = 0; i < _addresses.length; i++) {
            setMaxWhitelist(_addresses[i], _values[i]);
        }
