        return super.supportsInterface(interfaceId);
    }

    function _msgSender()
        internal
        view
        override(ContextUpgradeable)
        returns (address sender)
    {
        return super._msgSender();
    }

    function getChainID() external virtual view returns (uint256) {
        uint256 id;
        assembly {
            id := chainid()
        }
        return id;
    }
}