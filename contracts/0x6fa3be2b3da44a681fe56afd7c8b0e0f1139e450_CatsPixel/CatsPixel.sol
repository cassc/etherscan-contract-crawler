    string public uriSuffix = ".json";

	constructor() ERC721("Cats Pixel", "CP", 1) {
        setUriPrefix("ipfs://QmQqwVCmPJuLaHXZ3fyLyZkezCBaVvYyrCjMAnHijvZmbi/");
        uint16[7] memory totalSupplyForEachRank = [8000, 500, 250, 120, 60, 30, 1];
        uint8[7] memory collectionNumberForEachRank = [1, 4, 8, 12, 16, 20, 24]; 
        rank[] memory rankTokens = new rank[](totalSupplyForEachRank.length);
        for (uint256 i = 0; i < rankTokens.length; i++) {
            rankTokens[i].NUMBER_TOKENS = totalSupplyForEachRank[i];
            rankTokens[i].REQUIRED_TOKENS_TO_MINT = collectionNumberForEachRank[i];
            rankInfoAll[i] = rankTokens[i];
        }
    }


    modifier alreadyHasAward(uint256 _rank) {
        require(!alreadyReceivedAward[msg.sender][_rank], "You've already got a cat of this rank!");
        _;
        alreadyReceivedAward[msg.sender][_rank] = true;
    }

    // Mint for knights cats
 	function mint() external nonReentrant alreadyHasAward(0) {
  	    uint256 totalCatsMinted = _totalMinted(0);
        require(isPublicSale, "The public sale hasn't started yet");
        require(msg.sender == tx.origin, "The caller is another contract!");
        require(totalCatsMinted + maxMintPublicSale <= rankInfoAll[0].NUMBER_TOKENS, "All tokens of current rank are sold out!");
        _safeMint(msg.sender, maxMintPublicSale, 0);
    }

    //  Mint for lord cats
    function mintExtra(uint256 _rank) external nonReentrant alreadyHasAward(_rank){
        uint256 totalCatsMinted = _totalMinted(_rank);
        require(_rank > 0 && _rank <= 5, "Invalid rank");
        require(isExtraSale, "The extra sale hasnt started yet");
        require(msg.sender == tx.origin, "The caller is another contract!");        
        require(totalCatsMinted + maxMintExtraSale <= rankInfoAll[_rank].NUMBER_TOKENS, "All tokens of current rank are sold out!");
        uint256 numberOfUnique = getNumberOfUniqueTokensByAddress(msg.sender);
        // checking for a award
        require(numberOfUnique >= rankInfoAll[_rank].REQUIRED_TOKENS_TO_MINT, "Insufficient number of tokens of different clans for a reward!");
        _safeMint(msg.sender, maxMintExtraSale, _rank);
    }

    function getNumberOfUniqueTokensByAddress(address _owner) public view returns(uint256) {
        uint256[] memory tokenIDs = getTokenIdsByAddress(_owner);
        uint256 numberTokensPerClan = 400;
        uint256 startTokenId = getStartTokenId();
        uint256 balanceUnique; 
        uint256 leftBorder;
        uint256 rightBorder;
        for (uint i = 0; i < NUMBER_OF_CLANS; i++) {
            leftBorder = (i * numberTokensPerClan) + startTokenId;
            rightBorder = ((i + 1) * numberTokensPerClan) + startTokenId; 
            for (uint j = 0; j < tokenIDs.length; j++) {
                if (tokenIDs[j] >= leftBorder && tokenIDs[j] < rightBorder) {
                    balanceUnique++;
                    break;
                }   
            }
        }
        return balanceUnique;
    }

    // Secret mint for the winner
    function mintEmperor(address to) external onlyOwner nonReentrant {
        uint256 totalCatsMinted = _totalMinted(6);
        require(totalCatsMinted < rankInfoAll[6].NUMBER_TOKENS, "The Emperor is already in the collection");
        _safeMint(to, 1, 6);
    }

    function setPublicSale(bool _isPublicSale) external onlyOwner {
        isPublicSale = _isPublicSale;
    }

    function setExtraSale(bool _isExtraSale) external onlyOwner {
        isExtraSale = _isExtraSale;
    }

    function setMaxMintPublicSale(uint256 _maxMintPublicSale) external onlyOwner {
        maxMintPublicSale = _maxMintPublicSale;
    }

    function setMaxMintExtraSale(uint256 _maxMintExtraSale) external onlyOwner {
        maxMintExtraSale = _maxMintExtraSale;
    }

    function setUriPrefix(string memory _uriPrefix) public onlyOwner {
        uriPrefix = _uriPrefix;
    }

    function setUriSuffix(string memory _uriSuffix) public onlyOwner {
        uriSuffix = _uriSuffix;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return uriPrefix;
    }

    function tokenURI(uint256 _tokenId) public view virtual override returns (string memory) {
        require(_exists(_tokenId),"ERC721Metadata: URI query for nonexistent token");

        string memory currentBaseURI = _baseURI();
        return bytes(currentBaseURI).length > 0 ? string(abi.encodePacked(currentBaseURI, _tokenId.toString(), uriSuffix)): "";
    }

    function withDrawAll() public payable onlyOwner {
	    (bool success, ) = payable(msg.sender).call{value: address(this).balance}("");
		require(success);
	}
}