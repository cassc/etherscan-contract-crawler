		uint256 startIndex;
		uint256 fromToken;
		uint256 toToken;
	}
	
	modifier nonReentrant() {
        // On the first call to nonReentrant, _notEntered will be true
        require(reentryStatus != _ENTERED, "ReentrancyGuard: reentrant call");

        // Any calls to nonReentrant after this point will fail
        reentryStatus = _ENTERED;

        _;

        // By storing the original value once again, a refund is triggered (see
        // https://eips.ethereum.org/EIPS/eip-2200)
        reentryStatus = _NOT_ENTERED;
    }
	
	function initialize(string memory name, string memory symbol, address _beneficiary, uint256 _supplyLimit, string memory baseURI_, uint8 _nftPerAddress) public virtual initializer {
        __ERC721_init(name, symbol);
		__Ownable_init();
		
		saleStatus = status.PAUSED;
		nftPerAddress = _nftPerAddress;
		salePrice = 0.075 ether;
		supplyLimit = _supplyLimit;
		beneficiary = _beneficiary;
		
		baseURI = baseURI_;
		
		airdropStatus = true;
    }
   
	//============
	//sales & nft
	//============
    function airdrop(address[] calldata owners, uint256[] calldata amounts) external onlyOwner {
		require(airdropStatus, "airdrop off");
		
		//remove onchain validation for gas saving
		//please enhance offchain validation instead
		uint256 totalSupply_ = _totalSupply; 
        for (uint256 i; i < owners.length ; i++) {
			uint256 amts = amounts[i];
			address thisOwner = owners[i];
			for(uint256 j; j < amts; j++) {
				_safeMint(thisOwner, ++totalSupply_);
			}
        }
		_totalSupply = totalSupply_;
    }

    function mint(uint256 qty) public payable nonReentrant() {
        uint256 totalSupply_ = _totalSupply;
        require(saleStatus != status.PAUSED,           "Sale not active");
        require(balanceOf(msg.sender) + qty <= nftPerAddress, "Exceed purchase limit");
        require(totalSupply_ + qty <= supplyLimit,     "Exceed supply limit");
        require(salePrice * qty == msg.value,          "Incorrect eth value");
		
		if (saleStatus == status.PRESALE) {
			action whitelister = whitelisters[msg.sender];
			require(whitelister == action.ADD, "Address is not whitelisted");
		} 
        
        for(uint256 i; i < qty; i++) {
            _safeMint(msg.sender, ++totalSupply_);
        }
		
		_totalSupply = totalSupply_;
		
		payable(beneficiary).transfer(address(this).balance);
    }
	
	//======
	//admin 
	//======
    function manageWhitelisters(address[] calldata addrs, action _action ) external onlyOwner {
        for(uint256 i; i < addrs.length; i++){
            whitelisters[addrs[i]] = _action;
        }
    }
	
    function onPresales(uint256 _supplyLimit,uint256 newPrice) public onlyOwner {
		//admin must change carefully
		salePrice = newPrice;
		supplyLimit = _supplyLimit;
        saleStatus = status.PRESALE;
    }

    function onSales(uint256 _supplyLimit,uint256 newPrice) public onlyOwner {
		//admin must change carefully
		salePrice = newPrice;
		supplyLimit = _supplyLimit;
		saleStatus = status.SALE;
    }
	
	function setSaleStatus(status _status) public onlyOwner{
		saleStatus = _status;
	}
	
	function setSalePrice(uint256 newPrice) public onlyOwner{
		salePrice = newPrice;
	}
	
	function setSupplyLimit(uint256 _supplyLimit) public onlyOwner{
		supplyLimit = _supplyLimit;
	}
	
	function setAirdropStatus(bool _airdropStatus) public onlyOwner{
		airdropStatus = _airdropStatus;
	}

	function setBeneficiary(address _beneficiary) public onlyOwner{
		beneficiary = _beneficiary;
	}
	
	function setNftPerAddress(uint8 _nftPerAddress) public onlyOwner{
		nftPerAddress = _nftPerAddress;
	}
	
	function setReveal(uint256 maxToken) public onlyOwner{
		revealed.maxToken = maxToken;//set 0 to off the reveal
		
		if (revealed.startIndex == 0) {
			revealed.startIndex = uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp)));
		}
	}
	
	function setReveal2(uint256 maxToken) public onlyOwner{
		revealed2.maxToken = maxToken;//set 0 to off the reveal
		
		if (revealed2.startIndex == 0) {
			revealed2.startIndex = uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp)));
		}
	}
	//========
	//getters
	//========
	function config() public view returns(uint256, status, uint256, uint256, address, string memory, bool, revealInfo memory, revealInfo memory) {
		return (supplyLimit, saleStatus, salePrice, nftPerAddress, beneficiary, baseURI, airdropStatus, revealed, revealed2);
	}
	
	function isWhitelister(address addr) public view returns(bool) {
		return whitelisters[addr] == action.ADD;
	}
	
    //=======
	//erc721 
	//=======
	function burn (uint256 tokenId) public {
		require(_isApprovedOrOwner(msg.sender, tokenId), "caller is not owner nor approved");
		_totalSupply--;
        _burn(tokenId);
	}
	
	function totalSupply() public view returns (uint256) {
		return _totalSupply;
	}
	
	function tokenURI(uint256 tokenId) public view override returns (string memory) {
	
		require(_exists(tokenId), "token id not exist");
		require(bytes(_baseURI()).length > 0, "base uri empty");
		
		blindBoxEvent[] memory blindBoxEvents = new blindBoxEvent[]((revealed.maxToken > 0 && revealed2.maxToken > 0) ? 2 : 1);

        if (revealed.maxToken > 0) {

            blindBoxEvent memory newEvent;
		
            newEvent.fromToken = 1;
            newEvent.toToken = revealed.maxToken;
            newEvent.startIndex = revealed.startIndex;

            blindBoxEvents[0] = newEvent;
		}
		
		if (revealed2.maxToken > 0) {
			
            blindBoxEvent memory newEvent;
		
            newEvent.fromToken = revealed.maxToken + 1;
            newEvent.toToken = revealed2.maxToken;
            newEvent.startIndex = revealed2.startIndex;

            blindBoxEvents[1] = newEvent;	
		} 

		bool hasBox;
        uint256 seqId;
		if (blindBoxEvents.length > 0) {
			for(uint256 i = blindBoxEvents.length; i > 0; i--) {
				if (tokenId >= blindBoxEvents[i-1].fromToken && tokenId <= blindBoxEvents[i-1].toToken) {
					hasBox = true;
					
					if (blindBoxEvents[i-1].startIndex > 0) { 
						seqId = (tokenId + blindBoxEvents[i-1].startIndex) % (blindBoxEvents[i-1].toToken - blindBoxEvents[i-1].fromToken + 1);
						seqId += blindBoxEvents[i-1].fromToken;
					}
					
					break;
				}
			}
		}
		
		string memory filename;
		if (seqId > 0) {
			filename = string(abi.encodePacked(uint256ToStr(seqId), ".json"));
		} else if (hasBox) {
			filename = "0.json";
		} else {
			filename = string(abi.encodePacked(uint256ToStr(tokenId), ".json"));
		}	
		
		return string(abi.encodePacked(_baseURI(), filename));
	}
	

    function setBaseURI(string memory baseURI_) public onlyOwner {
        baseURI = baseURI_;//must end with /
    }
    
    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }
	
	function baseTokenURI() public view returns (string memory) {
		return baseURI;
	}
	
	function version() public pure returns (string memory) {
		return "4.0";
	}
	
	function uint256ToStr(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT licence
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }
	
	receive() external payable{
		payable(beneficiary).transfer(address(this).balance);
	}
	
	fallback() external payable{
		payable(beneficiary).transfer(address(this).balance);
	}
}