contract TeddyBoy is Initializable, ERC721Upgradeable, OwnableUpgradeable {
    
	bytes16 private constant _HEX_SYMBOLS = "0123456789abcdef";
	enum status{ PAUSED, PRESALE, SALE }
	enum action{ REMOVE, ADD }
	
	bool private airdropStatus;
	
	//after burn run, `totalSupply` cannot be used for new mint
	uint256 private _totalSupply;
	address private beneficiary;
    string private baseURI;
	
    uint256 private supplyLimit; 
    uint private salePrice; 
    uint8 private nftPerAddress;
	status private saleStatus;
	
