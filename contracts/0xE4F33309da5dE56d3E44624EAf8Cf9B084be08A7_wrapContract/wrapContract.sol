pragma solidity ^0.8.4;

contract wrapContract is Ownable {
    using SafeMath for uint256;

    address public Asgard;
    mapping(address => uint256) public lastWrapped;
    mapping (address => uint256) public deposits;
    uint256 public unwrapTime = 60; //3 days

    constructor(address _asgruard){
        Asgard = _asgruard;
    }
    
    function updateUnwrapTime(uint256 _newTime) public onlyOwner{
        unwrapTime = _newTime;
    }

    function wrap(uint256 amount) public returns (bool success){
        require(amount > 0, "Wrap amount must be greater than 0");
        IERC20(Asgard).transferFrom(msg.sender, address(this), amount);
        deposits[msg.sender] += amount;
        lastWrapped[msg.sender] = block.timestamp;
        return true;
    }

    function unwrap(uint256 amount) public returns (bool success){
        require(amount > 0, "Unwrap amount must be greater than 0!");
        require(amount <= deposits[msg.sender], "You do not have enough tokens to unwrap!");
        require(block.timestamp-lastWrapped[msg.sender] > unwrapTime, "Not unlocked yet, please wait for unlock!");

        deposits[msg.sender] -= amount;
        IERC20(Asgard).transfer(msg.sender, amount);
        return true;
    }


    function withdrawEthPool() external onlyOwner() {
        bool success;
        (success,) = address(msg.sender).call{value: address(this).balance}("");
    }

    function emergenceRescueToken(address token) public onlyOwner{
        IERC20(token).transfer(owner(), IERC20(token).balanceOf(address(this)));
    }


}