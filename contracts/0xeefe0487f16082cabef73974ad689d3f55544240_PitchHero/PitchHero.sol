        //   get the next hash.
        // - depending on the flag, either another value from the "main queue" (merging branches) or an element from the
        //   `proof` array.
        for (uint256 i = 0; i < totalHashes; i++) {
            bytes32 a = leafPos < leavesLen ? leaves[leafPos++] : hashes[hashPos++];
            bytes32 b = proofFlags[i]
                ? (leafPos < leavesLen ? leaves[leafPos++] : hashes[hashPos++])
                : proof[proofPos++];
            hashes[i] = _hashPair(a, b);
        }

        if (totalHashes > 0) {
            if (proofPos != proofLen) {
                revert MerkleProofInvalidMultiproof();
            }
            unchecked {
                return hashes[totalHashes - 1];
            }
        } else if (leavesLen > 0) {
            return leaves[0];
        } else {
            return proof[0];
        }
    }

    function _hashPair(bytes32 a, bytes32 b) private pure returns (bytes32) {
        return a < b ? _efficientHash(a, b) : _efficientHash(b, a);
    }

    function _efficientHash(bytes32 a, bytes32 b) private pure returns (bytes32 value) {
        /// @solidity memory-safe-assembly
        assembly {
            mstore(0x00, a)
            mstore(0x20, b)
            value := keccak256(0x00, 0x40)
        }
    }
}

pragma solidity ^0.8.19;

/**
 * @title PitchHero contract
 * @dev Extends ERC721 Non-Fungible Token Standard basic implementation
 */
contract PitchHero is ERC721, Ownable, ReentrancyGuard {
    error PreSaleNotActive();
    error PublicSaleNotActive();
    error NoContracts();

    string public _baseTokenURI;
    bool public presaleActive;
    bool public publicSaleActive;
    uint256 public maxSupply = 2500;
    uint256 public maxMint = 6;
    // uint256 public price_1 = 0.01 ether;
    // uint256 public price_3 = 0.02 ether;
    // uint256 public price_6 = 0.03 ether;

    // for testing
    uint256 public price_1 = 0.00001 ether;
    uint256 public price_3 = 0.00002 ether;
    uint256 public price_6 = 0.00003 ether;

    bytes32 public presaleMerkleRoot;
    bytes32 public ogMerkleRoot;

    mapping(address => uint256) public userTxCount;
    mapping(uint256 => bool) public tokensMinted;
    uint256 public divisor = maxSupply + 10;

    constructor() ERC721("Pitch Hero", "PITCH") {
    }

    modifier callerIsUser() {
        if (msg.sender != tx.origin) revert NoContracts();
        _;
    }

    function setBaseTokenURI(string memory newBaseTokenURI) public onlyOwner {
        _baseTokenURI = newBaseTokenURI;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        return
            string(
                abi.encodePacked(
                    _baseTokenURI,
                    Strings.toString(_tokenId),
                    ".json"
                )
            );
    }

    function togglePresale() external onlyOwner {
        presaleActive = !presaleActive;
    }

    function togglePublicSale() external onlyOwner {
        publicSaleActive = !publicSaleActive;
    }

    function setPresaleMerkleRoot(bytes32 _presaleMerkleRoot)
        external
        onlyOwner
    {
        presaleMerkleRoot = _presaleMerkleRoot;
    }

    function setOgMerkleRoot(bytes32 _ogMerkleRoot)
        external
        onlyOwner
    {
        ogMerkleRoot = _ogMerkleRoot;
    }

    function setPrice_1(uint256 _price_1) external onlyOwner {
        price_1 = _price_1;
    }

    function setPrice_3(uint256 _price_3) external onlyOwner {
        price_3 = _price_3;
    }

    function setPrice_6(uint256 _price_6) external onlyOwner {
        price_6 = _price_6;
    }

    function setUserTxCount(address _wallet, uint256 _txCount) external onlyOwner {
        userTxCount[_wallet] = _txCount;
    }

    function setDivisor(uint256 _divisor) external onlyOwner {
        divisor = _divisor;
    }

    function getPrice(uint256 _quantity) internal view returns (uint256) {
        if (_quantity == 1) {
            return price_1;
        } else if (_quantity == 3) {
            return price_3;
        } else if (_quantity == 6) {
            return price_6;
        } else {
            require(false, "error, invalid quantity.");
        }
    }

    function mintOG(uint256[] calldata _tokenIds, bytes32[] calldata _proof) external payable callerIsUser {
        require(presaleActive || publicSaleActive, "sale inactive");

        uint256 price;

        if (userTxCount[_msgSender()] % divisor == 1) {
            price = 0;
        } else {
            price = getPrice(_tokenIds.length);
        }
        require(msg.value >= price, "Insufficient ETH");

        require (MerkleProof.verify(_proof, ogMerkleRoot, keccak256(abi.encodePacked(_msgSender()))), "bad merkle OG");

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            require(!_exists(_tokenIds[i]), "tokenId already minted");

            tokensMinted[_tokenIds[i]] = true;
            _safeMint(_msgSender(), _tokenIds[i]);
        }
        userTxCount[_msgSender()] += 1;
    }

    function mintPresale(uint256[] calldata _tokenIds, bytes32[] calldata _proof) external payable callerIsUser {
        require(presaleActive, "presale inactive");

        uint256 price = getPrice(_tokenIds.length);
        require(msg.value >= price, "Insufficient ETH");

        require (MerkleProof.verify(_proof, presaleMerkleRoot, keccak256(abi.encodePacked(_msgSender()))), "bad merkle pre");

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            require(!_exists(_tokenIds[i]), "tokenId already minted");

            tokensMinted[_tokenIds[i]] = true;
            _safeMint(_msgSender(), _tokenIds[i]);
        }
        userTxCount[_msgSender()] += 1;

    }

    function mintPublic(uint256[] calldata _tokenIds) external payable callerIsUser {
        require(publicSaleActive, "public sale inactive");

        uint256 price = getPrice(_tokenIds.length);
        require(msg.value >= price, "Insufficient ETH");

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            require(!_exists(_tokenIds[i]), "tokenId already minted");

            tokensMinted[_tokenIds[i]] = true;
            _safeMint(_msgSender(), _tokenIds[i]);
        }
        userTxCount[_msgSender()] += 1;
    }

    event DonationReceived(address sender, uint256 amount);
    receive() external payable {
        emit DonationReceived(msg.sender, msg.value);
    }

    function withdraw_wdp() external payable onlyOwner {
        uint256 balance0 = address(this).balance;

        (bool wsuccess, ) = payable(msg.sender).call{value: balance0 }("");
        require(wsuccess, "withdraw failed");
    }
}