    function resurrect(uint[] calldata tokenIds_, address[] calldata recipients_) external onlyDelegates {
        require(tokenIds_.length == recipients_.length, "Must provide equal tokenIds and recipients");

        address to;
        uint tokenId;
        address zero = address(0);
        for(uint i; i < tokenIds_.length; ++i){
            to = recipients_[i];
            require(recipients_[i] != address(0), "Cannot resurrect to the zero address!");

            tokenId = tokenIds_[i];
            require(!_exists(tokenId), "Cannot resurrect existing token!");

            
            _owners[tokenId] = to;
            // Clear approvals
            _approve(zero, tokenId);
            emit Transfer(zero, to, tokenId);
        }
    }

    /**
     * @dev Activate/deactivate whitelisted presale mint, main mint, and premium mint. Can only be called by delegates.
     */
    function setActive(bool isOpenMintActive_, bool isDailyMintActive_) external onlyDelegates {
        require(isOpenMintActive != isOpenMintActive_ || isDailyMintActive != isDailyMintActive_, "New value matches old");
        isOpenMintActive = isOpenMintActive_;
        isDailyMintActive = isDailyMintActive_;
    }

    /**
     * @dev Set base URI. Can only be called by delegates.
