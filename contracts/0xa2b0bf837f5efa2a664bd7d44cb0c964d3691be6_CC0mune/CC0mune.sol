contract CC0mune is Delegated, ERC721EnumerableLite, PaymentSplitterMod {
    using Strings for uint;

    // General NFT Variables
    uint public maxTokens = 7;
    uint public maxOrder = 1; // maximum order per transaction during main mint phase
    uint public dailyMaxOrder = 1;
    uint public dailyGrossMax = 1;
    uint public dailyMinted = 0;
    uint public price = 0 ether; // price during mint
    uint public dailyPrice = 0 ether; // price for daily mint
    uint public dailyMintStart = 1659990000;
    bool public isOpenMintActive;
    bool public isDailyMintActive;

    string private _tokenURIPrefix = "https://cc0mune.s3.amazonaws.com/public-metadata/";
    string private _tokenURISuffix = ".json";

    // Payment Splitter Variables

    address[] private addressList = [
        0xb0E63031D2d0Ccc1FD99b38c7d6Adb378EC04A83,
        0xBb78e3eEe5B232aaC20e1b25BC02bEb289AF7897
    ];

    uint[] private shareList = [
        50,
        50
    ];

    constructor()
    Delegated()
    ERC721B("CC0mune", "CC0", 0)
    PaymentSplitterMod(addressList, shareList){
    }

    // Public view
    fallback() external payable {}

    function tokenURI(uint tokenId) external view override returns (string memory) {
        require(_exists(tokenId), "This token does not exist!");
        return string(abi.encodePacked(_tokenURIPrefix, tokenId.toString(), _tokenURISuffix));
    }

    function walletOfOwner(address address_) public view override returns (uint[] memory) {
        uint _balance = balanceOf(address_); // get balance of address
        uint[] memory _tokenIds = new uint[](_balance); // initialize array 
        for (uint i = 0; i < _balance; i++) {
            _tokenIds[i] = tokenOfOwnerByIndex(address_, i);
        }
        return _tokenIds;
