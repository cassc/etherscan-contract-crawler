    event MintAsOwner(address indexed to, uint tokenId);
    event MintPublic(address indexed to, uint tokenId);


    // Modifiers
    modifier onlySender {
        require(msg.sender == tx.origin, "No smart contracts!");
        _;
    }

    
    // Public payable
    /**
     * @dev Public mint function - can mint multiple tokens at once. Cannot be called by contracts.
     */
    function mint(uint amount_) external payable onlySender {
        require(isOpenMintActive, "Sale is paused!");
        require(amount_ > 0, "Must mint at least one!");
        require(amount_ <= maxOrder, "Order too big!");
        require(msg.value >= price * amount_, "Ether sent is not correct!");

        uint _supply = totalSupply();
        require(_supply + amount_ <= maxTokens, "Mint/order exceeds supply!");

        for(uint i; i < amount_; ++i){
            uint _mintId = _supply++;
            _mint(msg.sender, _mintId);
            emit MintPublic(msg.sender, _mintId);
        }
    }

      // Public payable
    /**
     * @dev Public daily mint function. Allows minting one NFT per day at dailyMintStart. Cannot be called by contracts.
     */
    function dailyMint(uint amount_) external payable onlySender {
        require(isDailyMintActive, "Sale is paused!");
        require(dailyMintStart <= block.timestamp, "Sale is not active!");
        require(amount_ > 0, "Must mint at least one!");
        require(amount_ <= dailyMaxOrder, "Order too big!");
        require(msg.value >= dailyPrice * amount_, "Ether sent is not correct!");
        require(dailyGrossMax <= dailyMinted + amount_, "Mint would exceed daily max!");

        uint _supply = totalSupply();
        require(_supply + amount_ <= maxTokens, "Mint/order exceeds supply!");

        for(uint i; i < amount_; ++i){
            uint _mintId = _supply++;
            _mint(msg.sender, _mintId);
            dailyMinted++;
            emit MintPublic(msg.sender, _mintId);
        }

        // reset daily mint if daily max is reached
        if (dailyMinted == dailyGrossMax) {
            dailyMintStart += 86400;
            dailyMinted = 0;
        }
    }

    // Delegated payable
    /**
     * @dev Burns tokens from ANY address. Can only be called by delegates.
     */
    function burnFrom(address owner_, uint[] calldata tokenIds_) external payable onlyDelegates {
        for(uint i; i < tokenIds_.length; ++i){
            require(_exists(tokenIds_[i]), "Burn for nonexistent token");
            require(_owners[tokenIds_[i]] == owner_, "Owner mismatch");
            _burn(tokenIds_[i]);
        }
    }

    /**
     * @dev Mints tokens directly to an address or addresses. Can only be called by delegates.
     */
    function mintTo(uint[] calldata amount_, address[] calldata recipient_) external payable onlyDelegates {
        require(amount_.length == recipient_.length, "Must provide equal quantities and recipients");

        uint _totalQuantity;
        uint _supply = totalSupply();
        for(uint i; i < amount_.length; ++i){
            _totalQuantity += amount_[i];
        }
        require(_supply + _totalQuantity <= maxTokens, "Mint/order exceeds supply!");

        for(uint i; i < recipient_.length; ++i){
            for(uint j; j < amount_[i]; ++j){
                uint _mintId = _supply++;
                _mint(recipient_[i], _mintId);
                emit MintAsOwner(msg.sender, _mintId);
            }
        }
    }

    // Delegated nonpayable
    /**
