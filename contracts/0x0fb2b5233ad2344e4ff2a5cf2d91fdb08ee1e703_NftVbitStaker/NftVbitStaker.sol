contract NftVbitStaker is ContextUpgradeable, OwnableUpgradeable{
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    IERC20 public tokenAddress;
    IERC20 public usdtAddress;
    address public ownerAddress;

    function initialize(
        IERC20 _tokenAddress,
        IERC20 _usdtAddress,
        address sender
    ) public initializer {
        tokenAddress = _tokenAddress;
        usdtAddress = _usdtAddress;
        ownerAddress = sender;
        __Ownable_init();
    }

    struct Stake {
        uint256 tokenId;
        bool isStake;
        uint256 timestamp;
        uint256 lastRewardTimestamp;
    }

     struct PoolInfo {
        IERC1155 nftToken;
        bool isActive;
        uint256 rewardUSDT;
        uint256 rewardVbit;
        uint256 totalSeconds;
    }


    // map staker address to stake details
    PoolInfo[] public poolInfo;

    // map staker to total staking time
    mapping (uint256 => mapping (address => Stake)) public stakeInfo;

    function add(IERC1155 _nftToken,uint256 _rewardusdt,uint256 _rewardVbit,uint256 _totalseconds) public  {
        require(msg.sender == ownerAddress,"Not a owner");
        poolInfo.push(PoolInfo({
            nftToken: IERC1155(_nftToken),
            isActive : true,
            rewardUSDT : _rewardusdt*1e10,
            rewardVbit : _rewardVbit*1e18,
            totalSeconds : _totalseconds

        }));
    }

     function set(uint256 _pid, uint256 _rewardusdt,uint256 _rewardVbit,uint256 _totalseconds,bool _isActive) public {
        poolInfo[_pid].rewardUSDT = _rewardusdt;
        poolInfo[_pid].rewardVbit = _rewardVbit;
        poolInfo[_pid].totalSeconds = _totalseconds;
        poolInfo[_pid].isActive = _isActive;
    }


    function _harvest(uint256 _pid) internal {
         PoolInfo storage pool = poolInfo[_pid];
         Stake storage stakes = stakeInfo[_pid][msg.sender];
         uint256 diff = block.timestamp - stakes.lastRewardTimestamp;
         uint256 perSecondUsdt = pool.rewardUSDT/pool.totalSeconds;
         uint256 perSecondVbit = pool.rewardVbit/pool.totalSeconds;
         uint256 userUsdtamount = (diff * perSecondUsdt)/1e10;
         uint256 userVbitamount = diff * perSecondVbit;
         usdtAddress.safeTransfer(msg.sender,userUsdtamount);
         tokenAddress.safeTransfer(msg.sender,userVbitamount);
         stakes.lastRewardTimestamp = block.timestamp;
    }


    function setTokenAddress(address _valobitaddress) public  {
        require(msg.sender == ownerAddress,"Not a owner");
        tokenAddress = IERC20(_valobitaddress);
    }

    function setRewardTokenAddress(address _usdtaddress) public  {
        require(msg.sender == ownerAddress,"Not a owner");
        usdtAddress = IERC20(_usdtaddress);
    }


     function transferownership(address _owner) public  {
        require(msg.sender == ownerAddress,"Not a owner");
        ownerAddress = _owner;
    }



     // Withdraw LP tokens from MasterChef.
    function withdraw(uint256 _pid) public {
        Stake storage stakes = stakeInfo[_pid][msg.sender];
        require(stakes.isStake, "withdraw: not good");
        _harvest(_pid);
    }

    function stake(uint256 _pid,uint256 _tokenId) public {
        PoolInfo storage pool = poolInfo[_pid];
        Stake storage stakes = stakeInfo[_pid][msg.sender];
        require(!stakes.isStake,"You already staked");
        stakes.tokenId = _tokenId;
        stakes.isStake = true;
        stakes.lastRewardTimestamp = block.timestamp;
        stakes.timestamp = block.timestamp;
        pool.nftToken.safeTransferFrom(msg.sender, address(this), _tokenId, 1, "0x00");
    }

    function unstake(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        Stake storage stakes = stakeInfo[_pid][msg.sender];
        _harvest(_pid);
        pool.nftToken.safeTransferFrom(address(this), msg.sender, stakes.tokenId, 1, "0x00");
        stakes.isStake = false;
    }

    function PoolLength() public view returns(uint256){
        return poolInfo.length;
    }

    function availableReward(uint256 _pid,address useraddress) public view returns(uint256,uint256){
        PoolInfo storage pool = poolInfo[_pid];
        Stake storage stakes = stakeInfo[_pid][useraddress];
        if(stakes.isStake){
            uint256 diff = block.timestamp - stakes.lastRewardTimestamp;
            uint perSecondUsdt = pool.rewardUSDT/pool.totalSeconds;
            uint256 perSecondVbit = pool.rewardVbit/pool.totalSeconds;
            uint256 userUsdtamount = diff * perSecondUsdt;
            uint256 userVbitamount = diff * perSecondVbit;
            return (userUsdtamount,userVbitamount);
        }

    }
    function checkdiff(uint256 _pid,address useraddress) public view returns(uint256){
        Stake storage stakes = stakeInfo[_pid][useraddress];
        return block.timestamp - stakes.lastRewardTimestamp;
     }

    function safewithdrawVbit(uint256 _amount) public {
         require(msg.sender == ownerAddress,"Not a owner");
         tokenAddress.safeTransfer(msg.sender,_amount);
    }

    function safewithdrawUsdt(uint256 _amount) public {
         require(msg.sender == ownerAddress,"Not a owner");
         usdtAddress.safeTransfer(msg.sender,_amount);
     }

    function onERC1155Received(
        address operator,
        address from,
        uint256 id,
        uint256 value,
        bytes calldata data
    ) external returns (bytes4) {
        return bytes4(keccak256("onERC1155Received(address,address,uint256,uint256,bytes)"));
    }
}