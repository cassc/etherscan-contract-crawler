pragma solidity ^0.8.0;


interface IWETH is IERC20 {
    function deposit() external payable;
    function withdraw(uint) external;
}

contract FlashBotsMultiCallFL is IUniswapV2Callee {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    address public constant WETH_address = address(0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2);
    
    IWETH private constant WETH = IWETH(WETH_address);

    IUniswapV2Factory public immutable _uniswapFactory;

    constructor(IUniswapV2Factory uniswapFactory) payable {
        _uniswapFactory = uniswapFactory;
    }

    function uniswapV2Call(address sender, uint amount0, uint amount1, bytes calldata data) external override {
        uint amount = SafeMath.add(amount0, amount1);
        uint debt = SafeMath.add(amount, (SafeMath.add(SafeMath.div(SafeMath.mul(amount, 3), 997), 1)));

        uniswapWethFLParams(amount, data, debt);
    }

    function flashloan(address token0, address token1, uint256 amountToBorrow, bytes memory _params) external {
        address receiverAddress = address(this);
        IUniswapV2Pair pair = IUniswapV2Pair(_uniswapFactory.getPair(token0, token1));

        if(pair.token0() == WETH_address) {
            pair.swap(amountToBorrow, 0, receiverAddress, _params);
        } else {
            pair.swap(0, amountToBorrow, receiverAddress, _params);
        }
    }

    function uniswapWethFLParams(uint256 _amountToFirstMarket, bytes memory _params, uint256 debt) internal {
        (uint256 _ethAmountToCoinbase, address[] memory _targets, bytes[] memory _payloads) = abi.decode(_params, (uint256, address[], bytes[]));
        require(_targets.length == _payloads.length);

        WETH.transfer(_targets[0], _amountToFirstMarket);
        for (uint256 i = 0; i < _targets.length; i++) {
            (bool _success, bytes memory _response) = _targets[i].call(_payloads[i]);
            require(_success); 
        }

        uint256 _wethBalanceAfter = WETH.balanceOf(address(this));

        uint256 _profit = _wethBalanceAfter - debt - _ethAmountToCoinbase;
        
        require(_profit >= 0);

        WETH.withdraw(_ethAmountToCoinbase + _profit);
        block.coinbase.transfer(_ethAmountToCoinbase);
        payable(tx.origin).transfer(_profit);
        
        address token0 = IUniswapV2Pair(msg.sender).token0();
        address token1 = IUniswapV2Pair(msg.sender).token1();
        address pair = _uniswapFactory.getPair(token0, token1);
        //Return funds
        IERC20(WETH_address).transfer(pair, debt);
    }

    function call(address payable _to, uint256 _value, bytes calldata _data) external payable returns (bytes memory) {
        require(_to != address(0));
        (bool _success, bytes memory _result) = _to.call{value: _value}(_data);
        require(_success);
        return _result;
    }
    receive() external payable {
    }
}