pragma solidity 0.8.7;









contract LongLiveFrankFrank is ERC1155(''), Ownable, Pausable {
    address public artPtr;

    address frankfrank = 0x91680cF5F9071cafAE21B90ebf2c9CC9e480fB93;
    address frankfrankdeployer = 0xD2927a91570146218eD700566DF516d67C5ECFAB;
    address artist = 0xe69171Feff9B15DAa83c0F41eDCfBA8994ffcd57;

    function pause() external onlyOwner whenNotPaused {
        _pause();
    }
    function unpause() external onlyOwner whenPaused {
        _unpause();
    }

    function mint() public payable whenNotPaused {
        uint256 n = msg.value / (.05 ether);
        uint256 r = msg.value % (.05 ether);
        _mint(msg.sender, 0, n, "");
        payable(msg.sender).call{value: r}('');
    }
    function onERC721Received(address, address from, uint256, bytes calldata) external returns (bytes4) {
        if(msg.sender == frankfrank) {
            _mint(from, 0, 1, '');
        }
        return IERC721Receiver.onERC721Received.selector;
    }

    receive() external payable {
        mint();
    }

    function uri(uint256 tokenId) public override view returns (string memory) {
        require(tokenId < 1, "token id invalid (0 only)");
        return buildMetadata();
    }

  
    function buildMetadata() public view returns(string memory) {
        return string(abi.encodePacked(
              'data:application/json;base64,', Base64.encode(bytes(abi.encodePacked(
                          '{"name": "',name(), 
                          '", "description":"', 
                          description(),
                          '", "image": "', 
                          string(SSTORE2.read(artPtr)),
                          '"}')))));
    }
    function name() public pure returns (string memory) {
        return "Long Live FrankFrank";
    }
    function symbol() external pure returns (string memory) {
        return "LLFF";
    }
    function description() public pure returns (string memory) {
        return "frankfrankfrankfrankfrankfrankfrankfrankfrankfrank";
    }

    function setArt(string memory svg) external onlyOwner {
        artPtr = SSTORE2.write(bytes(svg));
    }

    function withdraw(uint256 amount) external onlyOwner {
        safeTransferETH(frankfrankdeployer, amount / 5);
        safeTransferETH(artist, amount / 5);
        safeTransferETH(msg.sender, amount * 3 / 5);
    }

    function withdrawFrankFrank(uint256[] memory tokenIds) external onlyOwner {
        for(uint256 i = 0; i < tokenIds.length; i++) {
            ERC721(frankfrank).transferFrom(address(this), msg.sender, tokenIds[i]);
        }
    }

    //shoutout to VX and solady: https://github.com/Vectorized/solady/tree/main/src/utils
    function safeTransferETH(address to, uint256 amount) internal {
        assembly {
            // Transfer the ETH and check if it succeeded or not.
            if iszero(call(gas(), to, amount, 0, 0, 0, 0)) {
                // Store the function selector of `ETHTransferFailed()`.
                mstore(0x00, 0xb12d13eb)
                // Revert with (offset, size).
                revert(0x1c, 0x04)
            }
        }
    }
}