    function initialize(address _comptroller) initializer public {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        comptroller = _comptroller;
    }

    function setMintPaused(address[] memory markets, bool state) public {
        require(hasRole(MAINTAINER, msg.sender), "Caller is not a maintainer");

        for (uint i = 0; i < markets.length; i++) {
            address pToken = markets[i];
            bytes memory payload = abi.encodeWithSignature("_setMarketMintPaused(address,bool)", pToken, state);
            comptroller.functionCall(payload);
        }
    }

