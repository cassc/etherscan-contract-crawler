        require(hasRole(MAINTAINER, msg.sender), "Caller is not a maintainer");

        for (uint i = 0; i < markets.length; i++) {
            address pToken = markets[i];
            bytes memory payload = abi.encodeWithSignature("_setMarketTransferPaused(address,bool)", pToken, state);
            comptroller.functionCall(payload);
        }
    }

    function setSeizePaused(address[] memory markets, bool state) public {
