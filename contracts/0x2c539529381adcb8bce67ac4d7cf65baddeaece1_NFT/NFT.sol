    * @dev public returns the tokenURI for a specific tokenID
    */
  function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
    require(_exists(tokenId),"ERC721Metadata: URI query for nonexistent token");
    
    string memory currentBaseURI = _baseURI();
    return bytes(currentBaseURI).length > 0
        ? string(abi.encodePacked(currentBaseURI, tokenId.toString(), baseExtension))
        : "";
  }

  /**
    * @dev public returns a summary of the state of the contract, and if an address is whitelisted or not
    */
  function getMintStatus(address _address) public view virtual returns (Status memory){
    Status memory Status1;
    Status1._pausedGeneric = pausedGeneric;
    Status1._pausedWhitelisted = pausedWhitelisted;
    Status1._maxGenericPerAddressLimit = maxGenericPerAddressLimit;
    Status1._maxWhitelistedPerAddressLimit = maxWhitelistedPerAddressLimit;
    Status1._minWhitelistedPerAddressLimit = minWhitelistedPerAddressLimit;
    Status1._cost = cost;
    Status1._whitelistDiscount = whitelistDiscount;
    Status1._genericDiscount = genericDiscount;
    Status1._refDiscount = refDiscount;
    Status1._isWhitelisted = isWhitelisted(_address);
    Status1._balanceOf = balanceOf(_address);
    Status1._name = name();
    Status1._symbol = symbol();
    Status1._maxSupply = maxSupply;
    Status1._totalSupply = totalSupply();
    return Status1;
  }


  // Owner Only

  /**
    * @dev public onlyOwner returns a list of all whitelisted addresses
    */
  function getAllWhitelist() public view onlyOwner returns (address[] memory)
  {
    uint256 whitelistCount = whitelistedAddresses.length;
    address[] memory whitelist = new address[](whitelistCount);
    for (uint256 i; i < whitelistCount; i++) {
      whitelist[i] = whitelistedAddresses[i];
    }
    return whitelist;
  }

  /**
    * @dev public onlyOwner returns a list of all TopReferer addresses
    */
  function getAllTopReferer() public view onlyOwner returns (address[] memory)
  {
    uint256 refererCount = topRefererAddresses.length;
    address[] memory refererlist = new address[](refererCount);
    for (uint256 i; i < refererCount; i++) {
      refererlist[i] = topRefererAddresses[i];
    }
    return refererlist;
  }


  function setCost(uint256 _cost) public onlyOwner {
    cost = _cost;
  }

  function setRefererPercent(uint256 _percent) public onlyOwner {
    refererPercent = _percent;
  }

  function setRefererRefererPercent(uint256 _percent) public onlyOwner {
    refererRefererPercent = _percent;
  }

  function setWhitelistDiscount(uint256 _percent) public onlyOwner {
    whitelistDiscount = _percent;
  }

  function setGenericDiscount(uint256 _percent) public onlyOwner {
    genericDiscount = _percent;
  }

  function setRefDiscount(uint256 _percent) public onlyOwner {
    refDiscount = _percent;
  }

  function setMaxGenericPerAddressLimit(uint256 _newMaxGenericPerAddressLimit) public onlyOwner {
    maxGenericPerAddressLimit = _newMaxGenericPerAddressLimit;
  }
  
  function setMaxWhitelistedPerAddressLimit(uint256 _newMaxWhitelistedPerAddressLimit) public onlyOwner {
    maxWhitelistedPerAddressLimit = _newMaxWhitelistedPerAddressLimit;
  }

  function setMinWhitelistedPerAddressLimit(uint256 _percent) public onlyOwner {
    minWhitelistedPerAddressLimit = _percent;
  }


  function setBaseURI(string memory _newBaseURI) public onlyOwner {
    baseURI = _newBaseURI;
  }

  function setBaseExtension(string memory _newBaseExtension) public onlyOwner {
    baseExtension = _newBaseExtension;
  }

  function setMaxSupply(uint256 _newMaxSupply) public onlyOwner {
    maxSupply = _newMaxSupply;
  }

  function whitelistUsers(address[] calldata _users) public onlyOwner {
    delete whitelistedAddresses;
    whitelistedAddresses = _users;
  }

  function topRefererUsers(address[] calldata _users) public onlyOwner {
    delete topRefererAddresses;
    topRefererAddresses = _users;
  }

  function pauseGeneric(bool _state) public onlyOwner {
    pausedGeneric = _state;
  }
  function pauseWhitelisted(bool _state) public onlyOwner {
    pausedWhitelisted = _state;
  }
  function lockTransfer(bool _state) public onlyOwner {
    transferLocked = _state;
  }
 
  function withdraw() public payable onlyOwner {
    (bool os, ) = payable(owner()).call{value: address(this).balance}("");
    require(os);
  }
}