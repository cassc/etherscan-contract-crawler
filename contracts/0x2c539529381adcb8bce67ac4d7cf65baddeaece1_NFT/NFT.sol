    require(supply + _mintAmount <= maxSupply, "mint would exceed maxSupply");

    // Requirements if not the Contract Owner Mints
    if (msg.sender != owner()) {
      // if pausedGeneric == True and pausedWhitelisted == True, contract is fully closed for Minting
      require((pausedGeneric == false) || (pausedWhitelisted == false), "contract is closed for miniting");

      // Case pausedGeneric == True and pausedWhitelisted == False  =>  check if Minter is whitelisted and does not Mint to many tokens
      if ((pausedGeneric == true) && (pausedWhitelisted == false)){
        // must be whitelisted
        require(isWhitelisted(msg.sender) == true, "user is not whitelisted");
        uint256 ownerTokenCount = balanceOf(msg.sender);
        // can not exceed maxWhitelistedPerAddressLimit Tokens with this Mint
        require(ownerTokenCount + _mintAmount <= maxWhitelistedPerAddressLimit, "this transaction would exceed the Whitelist NFT limit");
      }

      // Case pausedGeneric == False and pausedWhitelisted == False  =>  check for whitelisted and public Minters to not exceed their max token limits
      if ((pausedGeneric == false) && (pausedWhitelisted == false)){
        uint256 ownerTokenCount = balanceOf(msg.sender);
        if (isWhitelisted(msg.sender) == true) {
          // whitelisted  =>  can not exceed maxWhitelistedPerAddressLimit Tokens with this Mint
          require(ownerTokenCount + _mintAmount <= maxWhitelistedPerAddressLimit, "this transaction would exceed the Whitelist NFT limit");
        } else {
          // public  =>  can not exceed maxGenericPerAddressLimit Tokens with this Mint
          require(ownerTokenCount + _mintAmount <= maxGenericPerAddressLimit, "this transaction would exceed the Generic NFT limit");
