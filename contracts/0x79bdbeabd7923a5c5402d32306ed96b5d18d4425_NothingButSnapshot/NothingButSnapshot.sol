contract NothingButSnapshot  is ERC721A, Ownable {

    uint256 public immutable cost = 0.003 ether;
    uint256 public immutable costMin = 0.001 ether;
    uint256 public immutable maxSUPPLY = 3000;

    modifier callerIsUser() {
        require(tx.origin == msg.sender, "The caller is another contract");
        _;
    }

    constructor()
    ERC721A ("NothingButSnapshot", "Nothing") {
    }

    function mint(uint256 amount) public payable callerIsUser{
        require(totalSupply() + amount <= maxSUPPLY, "sold out");
        require(msg.value >= costMin, "insufficient");
        if (msg.value >= cost * amount) {
            _safeMint(msg.sender, amount);
        }
    }

    function burn(uint256 amount) public onlyOwner {
        _burnMint(amount);
    }

    function withdraw() public onlyOwner {
        uint256 sendAmount = address(this).balance;

        address h = payable(msg.sender);

        bool success;

        (success, ) = h.call{value: sendAmount}("");
        require(success, "Transaction Unsuccessful");
    }
}