// File contracts/templates/AuctionVault.sol



pragma solidity ^0.8.0;






contract AuctionVault is IERC721Receiver, ConveyorBase {
    using EnumerableSet for EnumerableSet.UintSet;
    using SafeERC20 for IERC20;

    struct Auction {
        uint256 price;
        address winner;
        bool ended;
        uint256 floor;
    }

    bool public initialized;
    address public treasury;

    uint256 public bidStartTimestamp;
    uint256 public bidEndTimestamp;
    bool public mintable;

    IERC20 private token;
    IERC721Mintable private nft;

    mapping(uint256 => Auction) private auctions;
    EnumerableSet.UintSet private nftIds;
    mapping(address => EnumerableSet.UintSet) private winned;

    event Bid(address indexed user, uint256 indexed tokenId, uint256 price);
    event ERC721Received(uint256 id);

    modifier bidNotStarted() {
        require(bidStartTimestamp == 0 || (block.timestamp < bidStartTimestamp), "bid already started!");
        _;
    }

    modifier bidStarted() {
        require(bidStartTimestamp != 0 && (block.timestamp >= bidStartTimestamp), "bid not started yet");
        _;
    }

    modifier bidNotEnded() {
        require(bidStartTimestamp != 0 && block.timestamp < bidEndTimestamp, "bid already ended");
        _;
    }

    modifier bidEnded() {
        require(bidStartTimestamp != 0 && block.timestamp >= bidEndTimestamp, "bid not ended yet");
        _;
    }

    modifier auctionIdInitialized(uint256 id) {
        require(auctions[id].price > 0 && auctions[id].floor > 0, "provided id not initialized");
        _;
    }

    modifier onlyInitialized() {
        require(initialized);
        _;
    }

    /**
     * since logic contract will only be used as a proxy implementation
     * the contructor will not have any effect
     */
    constructor() ConveyorBase(address(0)) {}

    function initialize(
        address _forwarder,
        address _treasury,
        bool mintNft,
        uint256 bidStart,
        uint256 bidLast,
        IERC20 bidToken,
        IERC721Mintable bidNft
    ) public {
        require(!initialized);
        initialized = true;

        require(bidStart > block.timestamp, "bid time must be in the future");
        require(bidLast > bidStart, "bid end time must be after bid start time");
        bidStartTimestamp = bidStart;
        bidEndTimestamp = bidLast;
        token = bidToken;
        nft = bidNft;
        mintable = mintNft;

        forwarder = _forwarder;
        _transferOwnership(_msgSender());
        treasury = _treasury;

        //needs to be explicit set
        conveyorIsEnabled = true;
    }

    // === GETTERS ===
    function paymentToken() external view returns (address) {
        return address(token);
    }

    function getNft() external view returns (address) {
        return address(nft);
    }

    function getNftIds() external view returns (uint256[] memory) {
        return nftIds.values();
    }

    function getCurrentPrice(uint256 tokenId) external view returns (uint256) {
        return auctions[tokenId].price;
    }

    function getCurrentHighestBidder(uint256 tokenId) external view returns (address) {
        return auctions[tokenId].winner;
    }

    function getAuctionStatus(uint256 tokenId) external view returns (bool) {
        return auctions[tokenId].ended;
    }

    function getFloor(uint256 tokenId) external view returns (uint256) {
        return auctions[tokenId].floor;
    }

    function claimed(address _user) external view returns (uint256[] memory) {
        return winned[_user].values();
    }

    function claimedCount(address _user) external view returns (uint256) {
        return winned[_user].length();
    }

    // === SETTERS ===
    function setBidInterval(uint256 start, uint256 end) external onlyInitialized onlyOwner bidNotStarted {
        require(start > block.timestamp, "bid time must be in the future");
        require(end > bidStartTimestamp, "bid end time must be after bid start time");
        bidStartTimestamp = start;
        bidEndTimestamp = end;
    }

    function setAuction(
        uint256[] calldata ids,
        uint256[] calldata prices,
        uint256[] calldata floors
    ) external onlyInitialized onlyOwner bidNotStarted {
        uint256 n = ids.length;
        require(prices.length == n && floors.length == n, "parameters length not consistent");
        for (uint256 i = 0; i < n; i++) {
            if (!mintable) {
                require(nft.ownerOf(ids[i]) == address(this), "vault is not the owner of the provided id");
            }
            Auction storage auction = auctions[ids[i]];
            auction.price = prices[i];
            auction.floor = floors[i];
            nftIds.add(ids[i]);
        }
    }

    function bid(
        uint256 nft_id,
        uint256 bid_price,
        uint256 deadline,
        bytes calldata sig
    ) external onlyConveyor onlyInitialized bidStarted bidNotEnded auctionIdInitialized(nft_id) {
        require(bid_price - auctions[nft_id].price >= auctions[nft_id].floor, "bid price too low");

        INFTFairTreasury(treasury).collectPayment(_msgSender(), address(token), bid_price, deadline, sig);

        if (auctions[nft_id].winner != address(0)) {
            token.safeTransfer(auctions[nft_id].winner, auctions[nft_id].price);
        }
        auctions[nft_id].winner = _msgSender();
        auctions[nft_id].price = bid_price;

        emit Bid(_msgSender(), nft_id, bid_price);
    }

    /**
     * we keep the finalize specified nft funciton to avoid tx gas out of limit if too many nft exists
     */
    function finalize(uint256 nft_id) external onlyInitialized bidEnded {
        if (nft_id == 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff) {
            //finalize all the nft id
            uint256[] memory ids = nftIds.values();
            for (uint256 i = 0; i < ids.length; i++) {
                _finalize(ids[i]);
            }
        } else {
            _finalize(nft_id);
        }
    }

    function onERC721Received(
        address,
        address,
        uint256 tokenId,
        bytes calldata
    ) external override returns (bytes4) {
        require(!mintable && _msgSender() == address(nft), "FORBIDDEN: cannot receive NFT");
        emit ERC721Received(tokenId);
        return IERC721Receiver.onERC721Received.selector;
    }

    /**
     * @notice Recover unclaimed NFTs
     */
    function recoverToken(uint256 id, address recipient) external onlyInitialized onlyOwner bidEnded {
        require(!mintable && auctions[id].winner == address(0), "cannot recover token");
        nft.safeTransferFrom(address(this), recipient, id);
        auctions[id].ended = true;
    }

    function _finalize(uint256 nft_id) internal {
        if (auctions[nft_id].ended || auctions[nft_id].winner == address(0)) {
            return;
        } else {
            auctions[nft_id].ended = true;

            if (mintable) {
                nft.safeMint(auctions[nft_id].winner, nft_id, "");
            } else {
                nft.safeTransferFrom(address(this), auctions[nft_id].winner, nft_id, "");
            }

            winned[auctions[nft_id].winner].add(nft_id);
        }
    }
}