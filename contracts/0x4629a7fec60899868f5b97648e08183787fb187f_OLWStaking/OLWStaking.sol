pragma solidity ^0.8.4;

////import "@crypto-outlaws/bullet/contracts/IBullet.sol";
////import "@openzeppelin/contracts/access/Ownable.sol";
////import "@openzeppelin/contracts/security/Pausable.sol";
////import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
////import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
////import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
////import "@openzeppelin/contracts/utils/Context.sol";
////import "@openzeppelin/contracts/utils/math/SafeMath.sol";
////import "./ERC721Staking/IERC721Staking.sol";

/// @title Staking
/// @author New Frontiers Entertainment and Gaming LLC
/// @dev staking contract
/// @custom:security-contact [email protected]
contract OLWStaking is Pausable, Ownable, ReentrancyGuard, IERC721Staking {
    IERC20 public immutable override rewardsToken;
    IERC721 public immutable override stakingToken;
    uint256 public override totalStaked = 0;

    uint256 private constant _DECIMALS = 18;
    uint256 private immutable _rewardInterval;
    uint256 private immutable _specialRewardFactor;
    uint256[] private _halvingCycles;
    uint256[] private _specialTokens;

    using SafeMath for uint256;

    struct StakedToken {
        address staker;
        uint256 tokenId;
    }

    struct Staker {
        StakedToken[] specialStakedTokens;
        StakedToken[] stakedTokens;
        uint256 timeOfLastUpdate;
        uint256 totalSpecialStaked;
        uint256 totalStaked;
        uint256 unclaimedRewards;
    }

    mapping(address => Staker) private _stakers;

    mapping(uint256 => address) private _stakerAddressesByTokenId;

    constructor(
        IERC721 stakingToken_,
        IBullet bulletToken_,
        uint256 rewardInterval_,
        uint256 halvingCycles_,
        uint256 specialRewardFactor_,
        uint256[] memory specialTokens_
    ) ReentrancyGuard() {
        require(
            halvingCycles_ > 0 && halvingCycles_ < 256,
            "halving cycles must be greater than 0 and less than 256"
        );
        require(
            specialRewardFactor_ >= 1,
            "special reward factor must be greater than or equal to 1"
        );
        _halvingCycles = new uint256[](halvingCycles_);
        _halvingCycles[0] = block.timestamp;
        _rewardInterval = rewardInterval_;
        rewardsToken = IERC20(address(bulletToken_));
        stakingToken = stakingToken_;
        _specialRewardFactor = specialRewardFactor_;
        _specialTokens = specialTokens_;
    }

    function currentRewardRate() external view returns (uint256) {
        return _getRewardRate(_getLastHalvingCycleIndex()) * 10**_DECIMALS;
    }

    function balanceOf(address staker_)
        public
        view
        override
        returns (uint256 balance)
    {
        return
            _stakers[staker_].totalStaked +
            _stakers[staker_].totalSpecialStaked;
    }

    function walletOf(address staker_)
        external
        view
        override
        returns (uint256[] memory tokenIds)
    {
        uint256[] memory _tokenIds = new uint256[](
            _stakers[staker_].totalStaked + _stakers[staker_].totalSpecialStaked
        );
        uint256 j = 0;
        if (_stakers[staker_].totalStaked > 0) {
            for (
                uint256 i = 0;
                i < _stakers[staker_].stakedTokens.length;
                i++
            ) {
                if (
                    _stakers[staker_].stakedTokens[i].staker != address(0) &&
                    j < _tokenIds.length
                ) {
                    _tokenIds[j] = _stakers[staker_].stakedTokens[i].tokenId;
                    j++;
                }
            }
        }
        if (_stakers[staker_].totalSpecialStaked > 0) {
            for (
                uint256 i = 0;
                i < _stakers[staker_].specialStakedTokens.length;
                i++
            ) {
                if (
                    _stakers[staker_].specialStakedTokens[i].staker !=
                    address(0) &&
                    j < _tokenIds.length
                ) {
                    _tokenIds[j] = _stakers[staker_]
                        .specialStakedTokens[i]
                        .tokenId;
                    j++;
                }
            }
        }
        return _tokenIds;
    }

    function ownerOf(uint256 tokenId_)
        external
        view
        override
        returns (address staker)
    {
        return _stakerAddressesByTokenId[tokenId_];
    }

    function unclaimedRewards(address staker_)
        external
        view
        override
        returns (uint256 reward)
    {
        return (_calculateRewards(staker_) +
            _stakers[_msgSender()].unclaimedRewards);
    }

    function deposit(uint256 tokenId_) external override nonReentrant {
        return _deposit(tokenId_, _msgSender());
    }

    function batchDeposit(uint256[] memory tokenIds_) external nonReentrant {
        require(tokenIds_.length > 0, "token ids must be greater than 0");
        for (uint256 i = 0; i < tokenIds_.length; i++) {
            _deposit(tokenIds_[i], _msgSender());
        }
    }

    function withdraw(uint256 tokenId_) external override nonReentrant {
        return _withdraw(tokenId_, _msgSender());
    }

    function batchWithdraw(uint256[] memory tokenIds_) external nonReentrant {
        require(tokenIds_.length > 0, "token ids must be greater than 0");
        for (uint256 i = 0; i < tokenIds_.length; i++) {
            _withdraw(tokenIds_[i], _msgSender());
        }
    }

    function claim() external override nonReentrant returns (uint256 rewards) {
        return _claim(_msgSender());
    }

    function batchWithdrawAndClaim(uint256[] memory tokenIds_)
        external
        nonReentrant
    {
        require(tokenIds_.length > 0, "token ids must be greater than 0");
        for (uint256 i = 0; i < tokenIds_.length; i++) {
            _withdraw(tokenIds_[i], _msgSender());
        }
        _claim(_msgSender());
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    function activateNextHalvingCycle() external onlyOwner {
        uint256 lastHalvingCycleIndex = _getLastHalvingCycleIndex();
        require(
            lastHalvingCycleIndex < (_halvingCycles.length - 1),
            "all halving cycles have been activated"
        );
        _halvingCycles[lastHalvingCycleIndex + 1] = block.timestamp;
    }

    function withdrawFor(address staker_, uint256 tokenId_) external onlyOwner {
        if (tokenId_ > 0) {
            _withdraw(tokenId_, staker_);
            return;
        }
        for (uint256 j = 0; j < _stakers[staker_].stakedTokens.length; j++) {
            if (_stakers[staker_].stakedTokens[j].staker != address(0)) {
                _withdraw(_stakers[staker_].stakedTokens[j].tokenId, staker_);
            }
        }
        for (
            uint256 j = 0;
            j < _stakers[staker_].specialStakedTokens.length;
            j++
        ) {
            if (_stakers[staker_].specialStakedTokens[j].staker != address(0)) {
                _withdraw(
                    _stakers[staker_].specialStakedTokens[j].tokenId,
                    staker_
                );
            }
        }
    }

    function _beforeDeposit(address staker_, uint256 tokenId_)
        private
        whenNotPaused
    {}

    function _afterDeposit(address staker_, uint256 tokenId_) private {}

    function _beforeWithdraw(address staker_, uint256 tokenId_)
        private
        whenNotPaused
    {}

    function _afterWithdraw(address staker_, uint256 tokenId_) private {}

    function _beforeClaim(address staker_, uint256 rewards_)
        private
        whenNotPaused
    {}

    function _afterClaim(address staker_, uint256 rewards_) private {}

    function _deposit(uint256 tokenId_, address staker_) private {
        require(
            stakingToken.ownerOf(tokenId_) == staker_,
            "must own token to deposit"
        );
        _beforeDeposit(staker_, tokenId_);
        if (balanceOf(staker_) > 0) {
            _stakers[staker_].unclaimedRewards += _calculateRewards(staker_);
        }
        stakingToken.transferFrom(staker_, address(this), tokenId_);
        StakedToken memory stakedToken = StakedToken(staker_, tokenId_);
        if (_isSpecialToken(tokenId_)) {
            _stakers[staker_].specialStakedTokens.push(stakedToken);
            _stakers[staker_].totalSpecialStaked++;
        } else {
            _stakers[staker_].stakedTokens.push(stakedToken);
            _stakers[staker_].totalStaked++;
        }
        _stakers[staker_].timeOfLastUpdate = block.timestamp;
        _stakerAddressesByTokenId[tokenId_] = staker_;
        totalStaked++;
        _afterDeposit(staker_, tokenId_);
    }

    function _withdraw(uint256 tokenId_, address staker_) private {
        require(balanceOf(staker_) > 0, "must have staked token to withdraw");
        require(
            _stakerAddressesByTokenId[tokenId_] == staker_,
            "must own token to withdraw"
        );
        _beforeWithdraw(staker_, tokenId_);
        _stakers[staker_].unclaimedRewards += _calculateRewards(staker_);
        uint256 index = 0;
        if (_isSpecialToken(tokenId_)) {
            for (
                uint256 i = 0;
                i < _stakers[staker_].specialStakedTokens.length;
                i++
            ) {
                if (
                    _stakers[staker_].specialStakedTokens[i].tokenId ==
                    tokenId_ &&
                    _stakers[staker_].specialStakedTokens[i].staker !=
                    address(0)
                ) {
                    index = i;
                    break;
                }
            }
            _stakers[staker_].specialStakedTokens[index].staker = address(0);
            _stakers[staker_].totalSpecialStaked--;
        } else {
            for (
                uint256 i = 0;
                i < _stakers[staker_].stakedTokens.length;
                i++
            ) {
                if (
                    _stakers[staker_].stakedTokens[i].tokenId == tokenId_ &&
                    _stakers[staker_].stakedTokens[i].staker != address(0)
                ) {
                    index = i;
                    break;
                }
            }
            _stakers[staker_].stakedTokens[index].staker = address(0);
            _stakers[staker_].totalStaked--;
        }
        _stakers[staker_].timeOfLastUpdate = block.timestamp;
        _stakerAddressesByTokenId[tokenId_] = address(0);
        totalStaked--;
        stakingToken.transferFrom(address(this), staker_, tokenId_);
        _afterWithdraw(staker_, tokenId_);
    }

    function _claim(address staker_) private returns (uint256) {
        uint256 _rewards = (_calculateRewards(staker_) +
            _stakers[staker_].unclaimedRewards);
        require(_rewards > 0, "must have rewards to claim");
        _beforeClaim(staker_, _rewards);
        _stakers[staker_].timeOfLastUpdate = block.timestamp;
        _stakers[staker_].unclaimedRewards = 0;
        IBullet _bulletToken = IBullet(address(rewardsToken));
        if (_bulletToken.balanceOf(address(rewardsToken)) >= _rewards) {
            _bulletToken.transferToStaker(staker_, _rewards);
        } else {
            _bulletToken.stakerMint(staker_, _rewards);
        }
        _afterClaim(staker_, _rewards);
        return _rewards;
    }

    /**
     *          _____________
     *         /             \
     *    |---*--|---*----|---*----*->
     *    G   D  H   C    H   W    C
     *
     * G=genesis
     * D=deposit
     * H=halving
     * W=withdraw
     * C=claim
     */
    function _calculateRewards(address staker_) private view returns (uint256) {
        uint256 rewards = 0;
        uint256 timeOfLastUpdate = _stakers[staker_].timeOfLastUpdate;
        uint256 lastHalvingCycleIndex = _getLastHalvingCycleIndex();
        for (
            uint256 i = _getFirstHalvingCycleIndex(staker_);
            i <= lastHalvingCycleIndex;
            i++
        ) {
            if (i == lastHalvingCycleIndex) {
                rewards =
                    rewards +
                    ((((block.timestamp - timeOfLastUpdate) *
                        (_stakers[staker_].totalStaked +
                            (_stakers[staker_].totalSpecialStaked *
                                _specialRewardFactor)) *
                        _getRewardRate(i)) / _rewardInterval) * 10**_DECIMALS);
            } else {
                rewards =
                    rewards +
                    ((((_halvingCycles[i + 1] - timeOfLastUpdate) *
                        (_stakers[staker_].totalStaked +
                            (_stakers[staker_].totalSpecialStaked *
                                _specialRewardFactor)) *
                        _getRewardRate(i)) / _rewardInterval) * 10**_DECIMALS);
                timeOfLastUpdate = _halvingCycles[i + 1];
            }
        }
        return rewards;
    }

    function _getFirstHalvingCycleIndex(address staker_)
        private
        view
        returns (uint256)
    {
        for (uint256 i = _halvingCycles.length - 1; i > 0; i--) {
            if (
                _halvingCycles[i] != 0 &&
                _stakers[staker_].timeOfLastUpdate > _halvingCycles[i]
            ) {
                return i;
            }
        }
        return 0;
    }

    function _getLastHalvingCycleIndex() private view returns (uint256) {
        uint256 index = 0;
        for (uint256 i = 1; i < _halvingCycles.length; i++) {
            if (_halvingCycles[i] == 0) {
                return index;
            }
            index = i;
        }
        return index;
    }

    function _getRewardRate(uint256 halvingCycleIndex_)
        private
        view
        returns (uint256)
    {
        return 2**(_halvingCycles.length - halvingCycleIndex_ - 1);
    }

    function _isSpecialToken(uint256 tokenId_) private view returns (bool) {
        for (uint256 i = 0; i < _specialTokens.length; i++) {
            if (_specialTokens[i] == tokenId_) {
                return true;
            }
        }
        return false;
    }
}