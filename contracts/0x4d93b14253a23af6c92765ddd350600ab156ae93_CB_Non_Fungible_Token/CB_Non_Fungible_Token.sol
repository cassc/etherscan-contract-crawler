        for (uint256 i = 0; i < _addresses.length; i++) {
            whitelistedAddress[_addresses[i]] = true;
        }
    }
    function addToWhitelisted(address _address) public onlyOwner {
        require(whitelistedAddress[_address], "NFT: Address is already whitelisted!");
        whitelistedAddress[_address] = true;
