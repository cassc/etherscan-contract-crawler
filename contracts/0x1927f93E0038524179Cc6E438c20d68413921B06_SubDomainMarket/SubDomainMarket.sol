// File contracts/SubDomainMarket.sol


pragma solidity ^0.8.0;















contract SubDomainMarket is
    WrapNFT,
    Order,
    Whitelist,
    Ownable,
    ISubDomainMarket
{
    ENS public ens;
    ISubDomainNFTMinter public minter;
    mapping(bytes32 => uint64) subNodeExpiries;
    mapping(uint256 => uint64) subNodeMaxExpiries;
    bool public isPausing;

    modifier whenNotPaused() {
        require(!isPausing, "is pausing");
        _;
    }

    function initialize(
        address owner_,
        address beneficiary_,
        address originalAddress_,
        address ens_,
        address minter_
    ) public initializer {
        __ReentrancyGuard_init();
        initOwnable(owner_);
        beneficiary = payable(beneficiary_);
        originalAddress = originalAddress_;
        ens = ENS(ens_);
        minter = ISubDomainNFTMinter(minter_);
        _fee = 10000;
    }

    function initList(
        uint256 parentId,
        address token,
        uint256 pricePerYear,
        uint64 minDuration,
        uint64 expiry
    ) external whenNotPaused override {
        stake(parentId);
        IBaseRegistrar(originalAddress).reclaim(parentId, address(this));
        list(parentId, token, pricePerYear, minDuration, expiry);
        minter.deploySubDomainNFT(parentId, msg.sender);
    }

    function list(
        uint256 parentId,
        address token,
        uint256 pricePerYear,
        uint64 minDuration,
        uint64 expiry
    ) public whenNotPaused override {
        require(
            IBaseRegistrar(originalAddress).nameExpires(parentId) >= expiry,
            "invalid expiry"
        );
        super.list(parentId, token, pricePerYear, minDuration, expiry);
    }

    function registerSubnode(
        RegisterParam calldata registerParam,
        MerkleParam calldata merkleParam
    ) external payable whenNotPaused override {
        require(
            inWhiteList(registerParam.parentId, msg.sender, merkleParam),
            "not in whitelist "
        );
        bytes32 label = ENSUtils.getLabel(registerParam.subName);
        bytes32 node = ENSUtils.getNode(registerParam.parentId);
        bytes32 subnode = keccak256(abi.encodePacked(node, label));
        require(subNodeExpiries[subnode] < block.timestamp, "not expired");
        ens.setSubnodeRecord(
            node,
            label,
            registerParam.to,
            registerParam.ens_resolver,
            0
        );
        uint64 expiry = fulfill(
            registerParam.parentId,
            registerParam.subName,
            uint64(block.timestamp),
            registerParam.duration,
            registerParam.token,
            registerParam.pricePerYear
        );
        subNodeExpiries[subnode] = expiry;
        updateMaxExpiry(registerParam.parentId, expiry);
        addMerkleMinted(registerParam.parentId, msg.sender);
        minter.mintSubDomainNFT(
            registerParam.parentId,
            subnode,
            expiry,
            registerParam.to
        );
        emit RegisterSubnode(
            registerParam.parentId,
            registerParam.subName,
            expiry,
            registerParam.to
        );
    }

    function renewSubnode(
        uint256 parentId,
        string calldata subName,
        uint64 duration,
        address token,
        uint256 pricePerYear
    ) external payable whenNotPaused  {
        bytes32 label = ENSUtils.getLabel(subName);
        bytes32 node = ENSUtils.getNode(parentId);
        bytes32 subnode = keccak256(abi.encodePacked(node, label));
        require(ens.recordExists(subnode), "record not exists");
        require(
            subNodeExpiries[subnode] > block.timestamp,
            "subnode is expired"
        );
        uint64 expiry = fulfill(
            parentId,
            subName,
            subNodeExpiries[subnode],
            duration,
            token,
            pricePerYear
        );
        subNodeExpiries[subnode] = expiry;
        updateMaxExpiry(parentId, expiry);
        emit RenewSubnode(parentId, subName, expiry);
        minter.renewSubDomainNFT(parentId, subnode, expiry);
    }

    function setSubnodeOwner(
        uint256 parentId,
        bytes32 label,
        address to
    ) public {
        address _owner = minter.ownerOfSubDomainNFT(parentId, label);
        require(_owner == msg.sender, "only owner of subDomainNFT");
        bytes32 node = ENSUtils.getNode(parentId);
        ens.setSubnodeOwner(node, label, to);
    }

    function expireOf(bytes32 subNode) public view returns (uint64) {
        return subNodeExpiries[subNode];
    }

    function setABI(
        uint256 parentId,
        uint256 contentType,
        bytes calldata data
    ) external onlyApprovedOrOwner(parentId) {
        bytes32 node = ENSUtils.getNode(parentId);
        IResolver(ens.resolver(node)).setABI(node, contentType, data);
    }

    function setAddr(uint256 parentId, address addr)
        external
        onlyApprovedOrOwner(parentId)
    {
        bytes32 node = ENSUtils.getNode(parentId);
        IResolver(ens.resolver(node)).setAddr(node, addr);
    }

    function setAddr(
        uint256 parentId,
        uint256 coinType,
        bytes calldata a
    ) external onlyApprovedOrOwner(parentId) {
        bytes32 node = ENSUtils.getNode(parentId);
        IResolver(ens.resolver(node)).setAddr(node, coinType, a);
    }

    function setContenthash(uint256 parentId, bytes calldata hash)
        external
        onlyApprovedOrOwner(parentId)
    {
        bytes32 node = ENSUtils.getNode(parentId);
        IResolver(ens.resolver(node)).setContenthash(node, hash);
    }

    function setDnsrr(uint256 parentId, bytes calldata data)
        external
        onlyApprovedOrOwner(parentId)
    {
        bytes32 node = ENSUtils.getNode(parentId);
        IResolver(ens.resolver(node)).setDnsrr(node, data);
    }

    function setName(uint256 parentId, string calldata _name)
        external
        onlyApprovedOrOwner(parentId)
    {
        bytes32 node = ENSUtils.getNode(parentId);
        IResolver(ens.resolver(node)).setName(node, _name);
    }

    function setPubkey(
        uint256 parentId,
        bytes32 x,
        bytes32 y
    ) external onlyApprovedOrOwner(parentId) {
        bytes32 node = ENSUtils.getNode(parentId);
        IResolver(ens.resolver(node)).setPubkey(node, x, y);
    }

    function setText(
        uint256 parentId,
        string calldata key,
        string calldata value
    ) external onlyApprovedOrOwner(parentId) {
        bytes32 node = ENSUtils.getNode(parentId);
        IResolver(ens.resolver(node)).setText(node, key, value);
    }

    function setInterface(
        uint256 parentId,
        bytes4 interfaceID,
        address implementer
    ) external onlyApprovedOrOwner(parentId) {
        bytes32 node = ENSUtils.getNode(parentId);
        IResolver(ens.resolver(node)).setInterface(
            node,
            interfaceID,
            implementer
        );
    }

    function multicall(bytes[] calldata data)
        external
        returns (bytes[] memory results)
    {
        results = new bytes[](data.length);
        for (uint256 i = 0; i < data.length; i++) {
            (bool success, bytes memory result) = address(this).delegatecall(
                data[i]
            );
            require(success);
            results[i] = result;
        }
        return results;
    }

    function setPause(bool pause_) public onlyOwner {
        isPausing = pause_;
        if (isPausing) {
            emit Paused(address(this));
        } else {
            emit Unpaused(address(this));
        }
    }

    function updateMaxExpiry(uint256 parentId, uint64 expiry) internal {
        if (expiry > subNodeMaxExpiries[parentId]) {
            subNodeMaxExpiries[parentId] = expiry;
        }
    }

    function delist(uint256 parentId)
        public
        virtual
        override
        whenNotPaused
        onlyApprovedOrOwner(parentId)
    {
        delete whitelistType[parentId];
        delete merkleRootMapping[parentId];
        super.delist(parentId);
    }

    function redeem(uint256 parentId) public virtual override whenNotPaused{
        require(
            subNodeMaxExpiries[parentId] < block.timestamp,
            "some subnodes are not expired"
        );
        delist(parentId);
        super.redeem(parentId);
    }

    function isAuthorised(uint256 parentId)
        internal
        view
        virtual
        override
        returns (bool)
    {
        return _isApprovedOrOwner(msg.sender, parentId);
    }
}