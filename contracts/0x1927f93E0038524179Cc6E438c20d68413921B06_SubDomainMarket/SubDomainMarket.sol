    mapping(uint256 => bytes32) internal merkleRootMapping;

    function isAuthorised(uint256 parentId)
        internal
        view
        virtual
        returns (bool);

    modifier authorised(uint256 parentId) {
        require(isAuthorised(parentId));
        _;
    }

    function whitelistTypeOf(uint256 parentId)
        external
        view
        virtual
        override
        returns (WhitelistType)
    {
        return whitelistType[parentId];
    }

    function addMerkleMinted(uint256 parentId, address addr) internal virtual {
        if (whitelistType[parentId] == WhitelistType.Merkle) {
            _minted[keyOf(parentId)][addr]++;
        }
    }

    function inWhiteList(
        uint256 parentId,
        address addr,
        MerkleParam calldata merkleParam
    ) public view returns (bool) {
        WhitelistType _type = whitelistType[parentId];
        if (_type == WhitelistType.None) {
            return true;
        } else if (_type == WhitelistType.Merkle) {
            return inMerkle(parentId, addr, merkleParam);
        }
        return false;
    }

    function inMerkle(
        uint256 parentId,
        address addr,
        MerkleParam calldata merkleParam
    ) internal view returns (bool) {
        bytes32 node = keccak256(abi.encodePacked(merkleParam.index, addr, merkleParam.limit));
        bool isIn = MerkleProof.verify(
            merkleParam.merkleProof,
            merkleRootMapping[parentId],
            node
        );
        return isIn && getMinted(parentId, addr) < merkleParam.limit;
    }

    function setWhitelistType(uint256 parentId, WhitelistType value)
        external
        virtual
        override
        authorised(parentId)
    {
        whitelistType[parentId] = value;
        emit UpdeteWhitelistType(parentId, value);
    }

    function setMerkleRoot(uint256 parentId, bytes32 merkleRoot_)
        public
        virtual
        authorised(parentId)
    {
        merkleRootMapping[parentId] = merkleRoot_;
    }

    function getMinted(uint256 parentId, address addr)
        public
        view
        returns (uint256)
    {
        return _minted[keyOf(parentId)][addr];
    }

    function whitelistOf(uint256 parentId, address who)
        public
        view
        returns (
            WhitelistType type_,
            bytes32 merkleRoot_,
            uint256 minted_
        )
    {
        type_ = whitelistType[parentId];
        merkleRoot_ = merkleRootMapping[parentId];
        minted_ = _minted[keyOf(parentId)][who];
    }

    function keyOf(uint256 parentId) internal view returns (bytes32) {
        return
            keccak256(abi.encodePacked(parentId, merkleRootMapping[parentId]));
    }
}


