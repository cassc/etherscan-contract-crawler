contract Fee is IFee, ReentrancyGuardUpgradeable {
    uint16 internal _fee;
    address payable public beneficiary;
    mapping(address => uint256) public balanceOfFee;

    modifier onlyBeneficiary() {
        require(msg.sender == beneficiary, "only beneficiary");
        _;
    }

    function setFee(uint16 fee) public onlyBeneficiary {
        require(fee <= 1e4, "invalid fee");
        _fee = fee;
    }

    function getFee() public view returns (uint16) {
        return _fee;
    }

    function payFee(address token, uint256 amount)
        internal
        returns (uint256 paid)
    {
        paid = (amount * _fee) / 1e5;
        balanceOfFee[token] += paid;
    }

    function claimFee(address[] calldata tokens)
        external
        onlyBeneficiary
        nonReentrant
    {
        for (uint256 index = 0; index < tokens.length; index++) {
            uint256 balance = balanceOfFee[tokens[index]];
            if (balance > 0) {
                if (tokens[index] == address(0)) {
                    Address.sendValue(beneficiary, balance);
                } else {
                    SafeERC20.safeTransfer(
                        IERC20(tokens[index]),
                        beneficiary,
                        balance
                    );
                }
                balanceOfFee[tokens[index]] = 0;
            }
        }
    }

}


