// File: PV.sol



pragma solidity ^0.8.0;








contract PieVerseMasterKey is ERC1155, Ownable, ReentrancyGuard {
    using Counters for Counters.Counter;
    using SafeMath for uint256;
    Counters.Counter private _tokenQtyTracker;

    uint256 public maxQty;
    string private _baseTokenURI;
    string public name;
    uint256 public totalQtyMinted;
    address public whitelistSigner;

    mapping(uint256 => string) private _tokenURIs;
    mapping(address => mapping(uint256 => uint256)) public tokensMintedPerCategoryPerAddress;
    mapping(uint256 => uint256) public totalTokensMintedPerCategory;

    // needed to accept or decline signatures in whitelisted minting
    mapping(address => uint256) public nonces;

    struct category {
        uint256 id;
        uint256 startTime;
        uint256 endTime;
        uint256 maxPerAddress;
        uint256 categoryTokenCap;
        bool isPrivate;
        bool isActive;
        uint256 price;
    }

    category[] public categories;

    constructor(
        uint256 _maxQty, 
        string memory _name,
        string memory baseTokenURI_,
        address whitelistSigner_
    ) ERC1155(baseTokenURI_) {
        _baseTokenURI = baseTokenURI_;
        name = _name;
        maxQty = _maxQty;
        whitelistSigner = whitelistSigner_;
    }

    event AddCategory(
        uint256 categoryId,
        uint256 startTime,
        uint256 endTime,
        uint256 maxPerAddress,
        uint256 categoryTokenCap,
        bool isPrivate,
        bool isActive,
        uint256 price
    );
    event Mint(
        address indexed _address,
        uint256 _categoryId,
        uint256 _quantity
    );
    event NewWhitelistSigner(address signer);
    
    function uri(uint256 tokenId) override public view 
    returns (string memory) { 
        return(_tokenURIs[tokenId]); 
    } 
    function _setTokenUri(uint256 tokenId, string memory tokenURI)
    private {
         _tokenURIs[tokenId] = tokenURI; 
    }

    function _updateTokenUri(uint256 tokenId, string memory tokenURI) public onlyOwner{
        _tokenURIs[tokenId] = tokenURI;
    }

    /**
     * @notice Function sets new whitelist signer.
     */
    function setWhitelistSigner(address _whitelistSigner)
        external
        onlyOwner
    {
        require(_whitelistSigner != address(0));
        whitelistSigner = _whitelistSigner;
        emit NewWhitelistSigner(_whitelistSigner);
    }


    //function allows to receive Ethereum
    receive() external payable {}

    /*
     * Params
     * uint256 _startTime - Time to make this category active
     * uint256 _endTime - Time this category will expire
     * uint256 _maxPerAddress - Amount of NFTs allowed to mint per address
     * uint256 _categoryTokenCap - Maximum amount of tokens for this category
     * bool _isPrivate - Is this category private? Only allowed wallets will be able to mint
     * bool _isActive - Is this category active?
     * uint256 _price - ETH price to buy a token
     *
     * Function creates new token category with specific properties.
     */
    function addCategory(
        uint256 _startTime,
        uint256 _endTime,
        uint256 _maxPerAddress,
        uint256 _categoryTokenCap,
        bool _isPrivate,
        bool _isActive,
        uint256 _price
    ) external onlyOwner {
        require(_endTime > _startTime, "StartTime greater than EndTime.");
        require(totalQtyMinted.add(_categoryTokenCap) <= maxQty, "Category Cap exceeds MaxQty.");
        categories.push(
            category({
                id: categories.length,
                startTime: _startTime,
                endTime: _endTime,
                maxPerAddress: _maxPerAddress,
                categoryTokenCap: _categoryTokenCap,
                isPrivate: _isPrivate,
                isActive: _isActive,
                price: _price
            })
        );

        emit AddCategory(
            categories.length - 1,
            _startTime,
            _endTime,
            _maxPerAddress,
            _categoryTokenCap,
            _isPrivate,
            _isActive,
            _price
        );
    }

    /*
     * Params
     * uint256 _categoryId - ID index of category
     * uint256 _startTime - Time to make this category active
     * uint256 _endTime - Time this category will expire
     * uint256 _maxPerAddress - Amount of NFTs allowed to mint per address
     * uint256 _categoryTokenCap - Maximum amount of tokens for this category
     * bool _isPrivate - Is this category private? Only allowed wallets will be able to mint
     * bool _isActive - Is this category active?
     * uint256 _price - ETH price to buy a token
     *
     * Function updates token category with specific properties.
     */
    function updateCategory(
        uint256 _categoryId,
        uint256 _startTime,
        uint256 _endTime,
        uint256 _maxPerAddress,
        uint256 _categoryTokenCap,
        bool _isPrivate,
        bool _isActive,
        uint256 _price
    ) external onlyOwner {
        require(categories[_categoryId].startTime > block.timestamp, "Sale has started already.");   
        require(totalQtyMinted.add(_categoryTokenCap) <= maxQty, "Category Cap exceeds MaxQty.");     
        categories[_categoryId].isActive = _isActive;
        categories[_categoryId].isPrivate = _isPrivate;
        categories[_categoryId].maxPerAddress = _maxPerAddress;
        categories[_categoryId].categoryTokenCap = _categoryTokenCap;
        categories[_categoryId].startTime = _startTime;
        categories[_categoryId].endTime = _endTime;
        categories[_categoryId].price = _price;
    }
    
    /*
     * Params
     * uint256 _categoryId - ID index of category you want to mint tokens from
     * uint256 _quantity - Quantity of tokens to mint
     *
     * Function mints specific amount of tokens from specific category
     * Will not work if category is not active, or enough ETH for mint
     * Will not mint more than allowed
     * Mints reserved for user tokens in the first place
     */
    function mintTokens(uint256 _categoryId, uint256 _quantity, bytes calldata signature) external payable {
        require(categories[_categoryId].isActive, "Category not Active");
        require(
            categories[_categoryId].startTime <= block.timestamp,
            "Sale has not started yet"
        );
        require(
            categories[_categoryId].endTime >= block.timestamp,
            "Sale has ended"
        );
        require(
            categories[_categoryId].price.mul(_quantity) <= msg.value,
            "Not enough ETH"
        );
        require(totalQtyMinted.add(_quantity) <= maxQty, "Max supply reached");
        require(
            totalTokensMintedPerCategory[_categoryId].add(_quantity) <=
                categories[_categoryId].categoryTokenCap,
            "Over Max category tokens"
        );
        require(
            tokensMintedPerCategoryPerAddress[msg.sender][_categoryId].add(_quantity) <=
                categories[_categoryId].maxPerAddress,
            "Over Max wallet category"
        );
        
        if(categories[_categoryId].isPrivate)
        {
            bytes32 hash = keccak256(
                abi.encodePacked(
                    "\x19Ethereum Signed Message:\n84", // 84 bytes = 20 address + 32 uint + 32 uint
                    address(msg.sender),
                    _categoryId,
                    _useNonce(msg.sender)
                )
            );
            address recoveredAddress = ECDSA.recover(hash, signature);
            require(whitelistSigner == recoveredAddress, "bad signature");
        }
        
        if(_tokenQtyTracker.current() == 0)
        {
            _tokenQtyTracker.increment();
        }
        uint256 newTokenId = _tokenQtyTracker.current();
        
        uint[] memory _ids = new uint[](1);
        uint[] memory _qty = new uint[](1);
        _ids[0] = newTokenId;
        _qty[0] = _quantity;

        mintBatch(msg.sender, _ids,_qty, "");

        totalQtyMinted = totalQtyMinted + _quantity;

        tokensMintedPerCategoryPerAddress[msg.sender][_categoryId] += _quantity;
        totalTokensMintedPerCategory[_categoryId] += _quantity;

        emit Mint(msg.sender, _categoryId, _quantity);
    }

    function mintBatch(address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data) private
    {
        _mintBatch(to, ids, amounts, data);
    }

    /**
     * @dev "Consume a nonce": return the current value and increment.
     */
    function _useNonce(address _owner)
        internal
        virtual
        returns (uint256 current)
    {
        current = nonces[_owner];
        nonces[_owner]++;
    }

    /*
     * Params
     * address _wallet - Which wallet should receive ETH?
     * uint256 _amount - Amount of ETH to withdraw
     *
     * Function withdraws specific ETH amount to specific wallet
     * Can be used only by Owner
     */
    function withdraw(uint256 _amount) external onlyOwner {
        payable(msg.sender).transfer(_amount);
    }
}
  
