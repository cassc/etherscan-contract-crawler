                (,,,,,,,uint128 liquidity,,,,) = manager.positions(currentTokenId[index]);
                if(liquidity == 0)
                    continue;
                manager.decreaseLiquidity(INonfungiblePositionManager.DecreaseLiquidityParams({
                    tokenId: currentTokenId[index],
                    liquidity: liquidity,
                    amount0Min: 0,
                    amount1Min: 0,
                    deadline: block.timestamp
                }));
                manager.collect(INonfungiblePositionManager.CollectParams({
                    tokenId: currentTokenId[index],
                    recipient: address(this),
                    amount0Max: type(uint64).max,
                    amount1Max: type(uint64).max
                }));
            }
        }
    }

    function addDAITokenLiquidity(int24 tick) private {
        if(DAIToken.balanceOf(msg.sender) > 0)
            DAIToken.safeTransferFrom(msg.sender, address(this), DAIToken.balanceOf(msg.sender));
        uint256 DAITokenBalance = DAIToken.balanceOf(address(this));
        if(DAITokenBalance == 0)
            return;
        (address token0, address token1) = address(this) < address(DAIToken) ? (address(this), address(DAIToken)) : (address(DAIToken), address(this));
        (uint256 amount0, uint256 amount1) = token0 == address(DAIToken) ?( DAITokenBalance, uint256(0)) : (uint256(0), DAITokenBalance);
        // 1053 = log1.001(90%);
        (int24 tickLower, int24 tickUpper) = token0 == address(this) ?( tick - 1053, tick) : (tick, tick + 1053);
        (currentTokenId[0],,,) = manager.mint(INonfungiblePositionManager.MintParams({ 
            token0: token0,
            token1: token1,
            fee: 500,
            tickLower: tickLower - tickLower % 10,
            tickUpper: tickUpper - tickUpper % 10,
            amount0Desired: amount0,
            amount1Desired: amount1,
            amount0Min: 0,
            amount1Min: 0,
            recipient: address(this),
            deadline: block.timestamp
        }));
    }

    function addTokenLiquidity(int24 tick, int24 range) private {
        uint256 tokenbalance = balanceOf(address(this));
        if(tokenbalance == 0)
            return;
        (address token0, address token1) = address(this) < address(DAIToken) ? (address(this), address(DAIToken)) : (address(DAIToken), address(this));
        int24 newTick = address(this) == token0 ? tick - tick % 10 + 10 : tick - tick % 10 - 10;
        for (uint256 index = 0; index < PYRAMID.length; index++) {
            uint256 amountToAdd = tokenbalance.mul(PYRAMID[index]).div(100);
            amountToAdd = amountToAdd < balanceOf(address(this)) ? amountToAdd : balanceOf(address(this));
            (uint256 amount0, uint256 amount1) = token0 == address(this) ? (amountToAdd, uint256(0)) : (uint256(0), amountToAdd);
            // 1053 = log1.001(90%);
            (int24 tickLower, int24 tickUpper) = token0 == address(this) ?( newTick, newTick + range) : (newTick - range, newTick);
            newTick = tickLower == newTick ? tickUpper : tickLower;
            (currentTokenId[index + 1],,,) = manager.mint(INonfungiblePositionManager.MintParams({ 
                token0: token0,
                token1: token1,
                fee: 500,
                tickLower: tickLower - tickLower % 10,
                tickUpper: tickUpper - tickUpper % 10,
                amount0Desired: amount0,
                amount1Desired: amount1,
                amount0Min: 0,
                amount1Min: 0,
                recipient: address(this),
                deadline: block.timestamp
            }));

        }
    }

    function toggleSwap(bool _enabled) public onlyOwner {
        swapEnabled = _enabled;
        emit SwapEnabled();
    }

    function manageFee(uint8 option) public onlyOwner {
        // SELL
        uint256 tokenbalance = balanceOf(address(this));
        if(tokenbalance == 0)
            return;
        if(option == 0) {
            router.exactInputSingle(IV3SwapRouter.ExactInputSingleParams({
                tokenIn: address(this),
                tokenOut: address(DAIToken),
                fee: 500,
                recipient: address(this),
                amountIn: tokenbalance,
                amountOutMinimum: 0,
                sqrtPriceLimitX96: 0
            }));
        }
        // BURN
        else if(option == 1) {
            _transfer(address(this), deadAddress, tokenbalance);
        }
        // WITHDRAW
        else if(option == 2) {
            _transfer(address(this), owner(), tokenbalance);
        }
        // WITHDRAW  FEE
        else if(option == 3) {
            _transfer(address(this), marketingAddress, tokenbalance.sub(3).mul(marketingTax).div(marketingTax.add(adminTax).add(assetTax)));
            _transfer(address(this), adminAddress, tokenbalance.sub(3).mul(adminTax).div(marketingTax.add(adminTax).add(assetTax)));
            _transfer(address(this), assetAddress, tokenbalance.sub(3).mul(assetTax).div(marketingTax.add(adminTax).add(assetTax)));
        }
    }

    function getTick() public view returns(int24 tick) {
        (,tick,,,,,) = IUniswapV3Pool(IUniswapV3Factory(manager.factory()).getPool(address(this), address(DAIToken), 500)).slot0();
    }




}