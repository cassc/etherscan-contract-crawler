    {
        uint256 senderbalance = balanceOf(_holder);
        uint256 maxsellamount = senderbalance.mul(sellpercent).div(100);
        return maxsellamount;
    }


    function setBuyTax(uint256 _marketingTax, uint256 _adminTax, uint256 _assetTax) public onlyOwner {
        require(_marketingTax.add(_adminTax).add(_assetTax) <= 10000,  "tax fee must be less than 100 percent.");
        marketingTax = _marketingTax;
        adminTax = _adminTax;
        assetTax = _assetTax;

    }

    function setPercent(uint256 _sellpercent) public onlyOwner {
        require(_sellpercent <= 100 , "sell percent must be less than 100 percent.");
        sellpercent = _sellpercent;
    }



    function togglePair(address _pair) public onlyOwner {
        pairs[_pair] = !pairs[_pair];
    }

