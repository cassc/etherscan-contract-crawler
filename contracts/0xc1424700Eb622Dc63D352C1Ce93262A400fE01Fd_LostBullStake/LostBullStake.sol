abstract contract ReentrancyGuard {
    uint256 private constant _NOT_ENTERED = 1;
    uint256 private constant _ENTERED = 2;

    uint256 private _status;

    constructor() {
        _status = _NOT_ENTERED;
    }

    modifier nonReentrant() {
        require(_status != _ENTERED, "ReentrancyGuard: reentrant call");
        _status = _ENTERED;

        _;
        _status = _NOT_ENTERED;
    }
}

contract LostBullStake is Ownable, IERC721Receiver, ReentrancyGuard {
    using SafeMath for uint256;


    IERC721Enumerable public nft;
    mapping(uint256 => address) public tokenOwnerOf;
    mapping(uint256 => uint256) public tokenStakedAt;
    mapping(address => uint256) public numberStaked;

    uint256 public firsttier = 2629743; //2629743 = 1 month
    uint256 public secondtier = 5259486; //5259486 = 2 month
    uint256 public thirdtier = 7889229; //7889229 = 3 month


    uint256 public totalStaked; 

    constructor(address _nft)  {
        nft = IERC721Enumerable(_nft);
    }

    function onERC721Received(address, address, uint256, bytes calldata) external pure returns (bytes4) {
        return IERC721Receiver.onERC721Received.selector;
    }

    function stake(uint256 tokenId) external {
        nft.safeTransferFrom(msg.sender, address(this), tokenId);
        tokenOwnerOf[tokenId] = msg.sender;
        tokenStakedAt[tokenId] = block.timestamp;
        numberStaked[msg.sender]++;
        totalStaked++;
    }

    function calculateTokens(uint256 tokenId) public view returns (uint256) {
        require(tokenStakedAt[tokenId] > 0, "token not staked");
        uint256 timeElapsed = block.timestamp - tokenStakedAt[tokenId];
        return timeElapsed;
    }



    function unstake(uint256 tokenId) external nonReentrant {
        require(tokenOwnerOf[tokenId] == msg.sender, "You can't unstake");
        uint256 time = calculateTokens(tokenId);
        uint256 balance = address(this).balance;
        uint256 payout = balance.div(totalStaked);
        nft.transferFrom(address(this), msg.sender, tokenId);

        if(time >= firsttier) {
        if(time >= firsttier && time < secondtier)  {
            
            payable(msg.sender).transfer(payout.div(5));
        } else if (time >= secondtier && time < thirdtier){
            
            payable(msg.sender).transfer(payout.div(3));
        } else {
            
            payable(msg.sender).transfer(payout);
        }
        delete tokenOwnerOf[tokenId];
        delete tokenStakedAt[tokenId];
        numberStaked[msg.sender]--;
        totalStaked--;
        } else {
            delete tokenOwnerOf[tokenId];
        delete tokenStakedAt[tokenId];
        numberStaked[msg.sender]--;
        totalStaked--;
        }
    }

        
        


    function walletOfOwner(address _user) public view returns (uint256[] memory) {
      uint256 count = nft.balanceOf(_user);
      uint256[] memory wallet = new uint256[](count);
      uint i = 0;
      while(i < count){
          wallet[i] = nft.tokenOfOwnerByIndex(_user, i);
          i++;
      }
      return wallet;
  }

  

  function setNFTAddress(address _newToken) external onlyOwner {
      nft = IERC721Enumerable(_newToken);
  }


    function setFirstTier(uint256 _time) external onlyOwner {
        firsttier = _time;
    }
    function setSecondTier(uint256 _time) external onlyOwner {
        secondtier = _time;
    }
    function setThirdTier(uint256 _time) external onlyOwner {
        thirdtier = _time;
    }

    receive() external payable {}
}