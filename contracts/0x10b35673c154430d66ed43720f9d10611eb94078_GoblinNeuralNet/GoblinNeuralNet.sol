contract Fields {
    // emits URIChanged event when the URI changes
    event URIChanged(string initialURI, string finalURI);
    // emits URIChanged event when the URI changes
    event DisableURIChange();
    // emits Minted event when somebody mints a token to a given address
    event Minted(address minter, address to, uint256 tokenID);
    // emits Airdrop event when the owner of the contract mints a token to a specific address
    event Airdrop(address to, uint256 tokenID);

    struct TeamMember {
        uint16 percentage;
        uint256 balance;
    }

    // flag to signal that the minting is started
    bool public mintedStarted = true;

    // flag to signal that the minting is started
    bool public canChangeURI = true;

    // maximum number of NFTs that can be minted in a single transaction
    uint8 internal constant MAX_FREE_TOKENS_PER_TRANSACTION = 3;

    // maximum number of NFTs that can be minted in a single transaction
    uint8 internal constant MAX_TOKENS_PER_TRANSACTION = 10;

    // the number of minted tokens
    uint16 public totalSupply;

    bool public freezSupply;

    // current maximum supply can be minted.
    uint16 public currentMaxSupply = 1000;

    // maximum number of NFTs minted for free
    uint16 public maxFreeNFTs = 500;

    // number of minted NFTs for free
    uint16 public mintedFreeNFTs;

    // the price to mint one token
    uint256 public mintPrice = 0.01 ether;

    // receiver address of the team members
    address[2] public receiverAddresses;

    // details of funds received by team member
    mapping(address => TeamMember) public team;
}


