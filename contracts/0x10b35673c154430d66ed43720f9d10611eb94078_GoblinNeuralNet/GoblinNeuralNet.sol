error NFTTransferPaused();
error ETHTransferFailed();

error TooManyToMintInOneTransaction();
error NotEnoughtETH();
error ToManyToMint();
error NotAllowed();
error NoBalanceToWithdraw();
error ToMuchToWithdraw();
