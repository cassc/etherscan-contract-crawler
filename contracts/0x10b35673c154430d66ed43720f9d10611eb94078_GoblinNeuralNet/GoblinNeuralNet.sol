contract GoblinNeuralNet is Base, ReentrancyGuard {
    constructor(
        address[2] memory receiverAddresses_,
        uint8[2] memory receiverPercentages_
    ) ERC1155("Goblin Neural Net", "GONET") {
        receiverAddresses = receiverAddresses_;
        for (uint256 i; i < receiverAddresses_.length; i++) {
            team[receiverAddresses_[i]] = TeamMember(
                receiverPercentages_[i],
                0
            );
        }
    }

    function airdrop(address[] memory tos, uint256[] memory numberOfTokens)
        public
        onlyOwner
    {
        require(tos.length == numberOfTokens.length, "invalid input");

        uint16 currentTokenId = totalSupply;

        for (uint256 i = 0; i < tos.length; i++) {
            address to = tos[i];
            uint256 count = numberOfTokens[i];
            for (uint256 j = 0; j < count; j++) {
                currentTokenId++;
                _mint(to, currentTokenId, 1, "");
                emit Airdrop(to, currentTokenId);
            }
        }

        totalSupply = currentTokenId;
    }

    function mintFree(address to, uint16 numberOfTokens) public nonReentrant {
        require(
            numberOfTokens <= MAX_FREE_TOKENS_PER_TRANSACTION,
            "too many tokens to mint in one transaction"
        );
        require(
            mintedFreeNFTs + numberOfTokens <= maxFreeNFTs,
            "can't mint so many NFTs for free"
        );
        mintedFreeNFTs += numberOfTokens;
        _mintTokens(to, numberOfTokens);
    }

    function mint(address to, uint256 numberOfTokens)
        public
        payable
        nonReentrant
    {
        require(
            numberOfTokens <= MAX_TOKENS_PER_TRANSACTION,
            "too many tokens to mint in one transaction"
        );
        require(
            msg.value >= mintPrice * numberOfTokens,
            "ether sent is not correct"
        );

        splitFunds();

        _mintTokens(to, numberOfTokens);
    }

    function _mintTokens(address to, uint256 numberOfTokens) private {
        require(
            totalSupply + numberOfTokens <= currentMaxSupply,
            "too many tokens to mint"
        );
        uint256 currentSupply = totalSupply;
        unchecked {
            totalSupply += uint16(numberOfTokens);
        }
        for (uint256 i = 1; i <= numberOfTokens; i++) {
            _mint(to, currentSupply + i, 1, "");
            emit Minted(msg.sender, to, currentSupply + i);
        }
    }

    function setURI(string memory newURI) external onlyOwner {
        require(canChangeURI, "changes of metadata are disabled");
        emit URIChanged(uri(0), newURI);
        _setURI(newURI);
    }

    function disableMetadataChanges() public onlyOwner {
        canChangeURI = false;
        emit DisableURIChange();
    }

    function setMintedStarted(bool mintedStarted_) public onlyOwner {
        require(mintedStarted != mintedStarted, "no change to make");
        mintedStarted = mintedStarted_;
    }

    function setMintPrice(uint256 mintPrice_) public onlyOwner {
        require(mintPrice_ >= 0, "price must be greater than zero");
        mintPrice = mintPrice_;
    }

    function freezSupplyForEternity() external onlyOwner {
        require(!freezSupply, "already done");
        freezSupply = true;
    }

    function setTheCurrentMaxTokens(uint16 currentMaxSupply_) public onlyOwner {
        require(!freezSupply, "can't change supply");
        require(
            totalSupply < currentMaxSupply_ && currentMaxSupply_ < MAX_SUPPLY,
            "invalid new max supply"
        );
        currentMaxSupply = currentMaxSupply_;
    }

    function setMaxNFTsMintableForFree(uint16 maxFreeNFTs_) public onlyOwner {
        require(
            mintedFreeNFTs <= maxFreeNFTs_ &&
                maxFreeNFTs_ <= currentMaxSupply &&
                maxFreeNFTs_ - mintedFreeNFTs < currentMaxSupply - totalSupply,
            "invalid new max free NFTs"
        );
        maxFreeNFTs = maxFreeNFTs_;
    }
}