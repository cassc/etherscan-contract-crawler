pragma solidity 0.6.12;

contract LockContract is Ownable {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;

    mapping(address => bool) public acceptableOtherTokens;
    mapping(address => uint256[]) public userIds;
    mapping (uint256 => Items) public lockedToken; // track all locks
    address public treasury;

    IBEP20 public VENTOKEN = IBEP20(0x3A529A8d4f2ea64D206339fA12A3aF4D431F53c3); // VEN TOKEN
    uint256 public id = 0;


    constructor(address _treasury) public {
        acceptableOtherTokens[0x2260FAC5E5542a773Aa44fBCfeDf7C193bc2C599] = true; // WBTC
        acceptableOtherTokens[0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2] = true; // WETH
        acceptableOtherTokens[0x6B175474E89094C44Da98b954EedeAC495271d0F] = true; // DAI
        acceptableOtherTokens[0xdAC17F958D2ee523a2206206994597C13D831ec7] = true; // USDT
        acceptableOtherTokens[0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48] = true; // USDC
        treasury = _treasury;
    }

    function addOtherSupportedTokens(address _token) public onlyOwner {
        require(acceptableOtherTokens[_token] == false, 'already approved');
        acceptableOtherTokens[_token] = true;
    }


    struct Items {
        address tokenAddress; // ven token
        address tokenAddress2; // wbtc, weth, dai, usdt or usdc
        address withdrawalAddress; // user address
        uint256 tokenAmount;  // VEN deposited
        uint256 tokenAmount2; // other tokens deposited - tax
        uint256 unlockTime; // after how much time
        bool withdrawn; // keep tracks of withdraw locks
    }
 
    function getIdData(uint256 _id) public view returns(Items memory){
        return lockedToken[_id];
    }

    function changeTreasury(address _treasury) public {
        require(msg.sender == treasury, 'invalid address');
        treasury = _treasury;
    }

    // FUNCTION to lock funds
    // B.  VEN has a minimum lock Users can choose to lock 0.25%, 0.5%, 0.75% and 1% of TOTAL VEN supply.
    // 0.25% = 278,055 VEN
    // 0.5% = 556,111 VEN
    // 0.75% = 834,167 VEN
    // 1% = 1,112, 223 VEN

    function lockFunds(address othertoken, uint256 otherAmount, uint256 venAmount, uint256 _days) public {
        require(otherAmount > 0, 'invalid amount');
        require(venAmount == 278055000000000000000000 || venAmount == 556111000000000000000000 || venAmount == 834167000000000000000000 || venAmount == 1112223000000000000000000, 'invalid venAmount locked');
        require(_days == 30 || _days == 60 || _days == 90 || _days == 180, 'invalid days amount'); // we use self defined locks with 30/60/90/180 days 
        require(acceptableOtherTokens[othertoken] == true, 'invalid other token');

        // transfer VEN tokens in smart contract
        VENTOKEN.safeTransferFrom(msg.sender, address(this), venAmount);
        // 10% tax to treasury wallet for other token
        uint256 _tax = otherAmount.mul(1000).div(10000);
        IBEP20(othertoken).safeTransferFrom(msg.sender, address(this), otherAmount.sub(_tax));
        IBEP20(othertoken).safeTransferFrom(msg.sender, treasury, _tax);

        uint256 lockTime = _days.mul(86400); // we multiply seconds in a day by the number of days provided


        lockedToken[id].tokenAddress = 0x3A529A8d4f2ea64D206339fA12A3aF4D431F53c3;
        lockedToken[id].tokenAddress2 = othertoken;
        lockedToken[id].withdrawalAddress = msg.sender;
        lockedToken[id].tokenAmount = venAmount;
        lockedToken[id].tokenAmount2 = otherAmount.sub(_tax);
        lockedToken[id].unlockTime = lockTime;
        lockedToken[id].withdrawn = false;
        userIds[msg.sender].push(id);
        id = id.add(1);

    }

    function withdrawFunds(uint256 _id) public {
        require(block.timestamp >= lockedToken[_id].unlockTime, 'tokens still in lock');
        require(msg.sender == lockedToken[_id].withdrawalAddress, 'Not owner of the deposit');
        require(!lockedToken[_id].withdrawn, 'tokens withdrawn');

        VENTOKEN.safeTransfer(msg.sender, lockedToken[_id].tokenAmount);
        IBEP20(lockedToken[_id].tokenAmount2).safeTransfer(msg.sender, lockedToken[_id].tokenAmount2);
        lockedToken[_id].withdrawn = true;
    }


    function getIdsWithPagination(uint256 cursor, uint256 howMany, address _sender) public view returns(uint256[] memory values, uint256 newCursor){
        uint256 length = howMany;
        if (length > userIds[_sender].length - cursor) {
            length = userIds[_sender].length - cursor;
        }

        values = new uint256[](length);
        for (uint256 i = 0; i < length; i++) {
            values[i] = userIds[_sender][cursor + i];
        }
        return (values, cursor + length);
    }
}