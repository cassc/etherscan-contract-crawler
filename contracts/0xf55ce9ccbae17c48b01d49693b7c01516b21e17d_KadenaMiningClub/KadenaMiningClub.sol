        require(!_exists(tokenId), "ERC721: token already minted");

        _beforeTokenTransfer(address(0), to, tokenId);

        _balances[to] += 1;
        _owners[tokenId] = to;
        _kadenaAddresses[tokenId] = kadenaAddress;
        _amountMinted[tokenId] = amountMintedBy;
        emit Transfer(address(0), to, tokenId);

        _afterTokenTransfer(address(0), to, tokenId);
    }

    function _burn(uint256 tokenId) internal virtual {
        address owner = ERC721.ownerOf(tokenId);

        _beforeTokenTransfer(owner, address(0), tokenId);

        // Clear approvals
        _approve(address(0), tokenId);

        _balances[owner] -= 1;
        delete _owners[tokenId];

        emit Transfer(owner, address(0), tokenId);

        _afterTokenTransfer(owner, address(0), tokenId);
    }

    function _approve(address to, uint256 tokenId) internal virtual {
        _tokenApprovals[tokenId] = to;
        emit Approval(ERC721.ownerOf(tokenId), to, tokenId);
    }

    function _setApprovalForAll(
        address owner,
        address operator,
        bool approved
    ) internal virtual {
        require(owner != operator, "ERC721: approve to caller");
        _operatorApprovals[owner][operator] = approved;
        emit ApprovalForAll(owner, operator, approved);
    }

    function _requireMinted(uint256 tokenId) internal view virtual {
        require(_exists(tokenId), "ERC721: invalid token ID");
    }

    function _checkOnERC721Received(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    ) private returns (bool) {
        if (to.isContract()) {
            try
                IERC721Receiver(to).onERC721Received(
                    _msgSender(),
                    from,
                    tokenId,
                    data
                )
            returns (bytes4 retval) {
                return retval == IERC721Receiver.onERC721Received.selector;
            } catch (bytes memory reason) {
                if (reason.length == 0) {
                    revert(
                        "ERC721: transfer to non ERC721Receiver implementer"
                    );
                } else {
                    assembly {
                        revert(add(32, reason), mload(reason))
                    }
                }
            }
        } else {
            return true;
        }
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual {}

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual {}
}

pragma solidity >=0.7.0 <0.9.0;

contract KadenaMiningClub is ERC721, Ownable {
    using Strings for uint256;
    using Counters for Counters.Counter;
    using SafeERC20 for IERC20;
    Counters.Counter private kadenaSupply;
    Counters.Counter private ethSupply;

    string public uriPrefix = "";
    string public uriSuffix = ".json";
    string public hiddenMetadataUri;

    uint256 public cost = 0.046 ether;
    uint256 public costInUSD = 85 * 10 ** 6;
    uint256 public maxKadenaSupply = 8500;
    uint256 public maxMintAmountPerTx = 600;

    bool public paused = false;
    bool public revealed = false;
    bool public allowListOnly = false;

    IERC20 public usdt; 
    IERC20 public usdc;

    constructor() ERC721("Kadena Mining Club Vials", "KMC") {
        setHiddenMetadataUri("ipfs://Qmd9JS8kB94swABnpF3ReqEwDfGhYuo7Kx7ZBvMxJCFPB4/");
        usdt = IERC20(0xdAC17F958D2ee523a2206206994597C13D831ec7); // mainnet USDT contract address
        usdc = IERC20(0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48); // mainnet USDC address
    }

    modifier mintCompliance(uint256 _mintAmount) {
        require(
            _mintAmount > 0 && _mintAmount <= maxMintAmountPerTx,
            "Invalid mint amount!"
        );
        require(
            kadenaSupply.current() + _mintAmount <= maxKadenaSupply,
            "Max kadenaSupply exceeded!"
        );
        _;
    }

    function totalSupply() public view returns (uint256) {
        return kadenaSupply.current();
    }

    function totalEthSupply() public view returns (uint256) {
        return ethSupply.current();
    }

    function mintTo(
        address recipient,
        uint256 _mintAmount,
        string memory kadenaAddress
    ) public mintCompliance(_mintAmount) {
        require(!paused, "The contract is paused!");
        usdc.safeTransferFrom(msg.sender, address(this), _mintAmount * costInUSD);
        _mintLoop(recipient, _mintAmount, kadenaAddress);
    }

    function mintWithUSDT(
        uint256 _mintAmount,
        string memory kadenaAddress
    ) public mintCompliance(_mintAmount) {
        require(!paused, "The contract is paused!");
        uint256 totalCost = costInUSD * _mintAmount;
        usdt.safeTransferFrom(msg.sender, address(this), totalCost);
        _mintLoop(msg.sender, _mintAmount, kadenaAddress);
    }

    function mintWithETH(
        uint256 _mintAmount,
        string memory kadenaAddress
    ) public payable mintCompliance(_mintAmount) {
        require(!paused, "The contract is paused!");
        require(msg.value >= cost * _mintAmount, "Insufficient funds!");
        _mintLoop(msg.sender, _mintAmount, kadenaAddress);
    }

    function walletOfOwner(
        address _owner
    ) public view returns (uint256[] memory) {
        uint256 ownerTokenCount = balanceOf(_owner);
        uint256[] memory ownedTokenIds = new uint256[](ownerTokenCount);
        uint256 currentTokenId = 1;
        uint256 ownedTokenIndex = 0;

        while (
            ownedTokenIndex < ownerTokenCount &&
            currentTokenId <= maxKadenaSupply
        ) {
            address currentTokenOwner = ownerOf(currentTokenId);
            if (currentTokenOwner == _owner) {
                ownedTokenIds[ownedTokenIndex] = currentTokenId;
                ownedTokenIndex++;
            }
            currentTokenId++;
        }
        return ownedTokenIds;
    }

    function tokenURI(
        uint256 _tokenId
    ) public view virtual override returns (string memory) {
        require(
            _exists(_tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );
        return hiddenMetadataUri;
    }

    function setCost(uint256 _cost) public onlyOwner {
        cost = _cost;
    }

    function setCostInUSD(uint256 _costInUSD) public onlyOwner {
        costInUSD = _costInUSD;
    }

    function setHiddenMetadataUri(
        string memory _hiddenMetadataUri
    ) public onlyOwner {
        hiddenMetadataUri = _hiddenMetadataUri;
    }

    function setPaused(bool _state) public onlyOwner {
        paused = _state;
    }

    function getUSDTBalance() public view returns (uint256) {
        return usdt.balanceOf(address(this));
    }

    function getUSDCBalance() public view returns (uint256) {
        return usdc.balanceOf(address(this));
    }

    function withdraw() public onlyOwner {
        // Withdraw ETH
        uint256 ethBalance = address(this).balance;
        if (ethBalance > 0) {
            (bool os, ) = payable(owner()).call{value: ethBalance}("");
            require(os, "ETH withdrawal failed!");
        }

        // Withdraw USDT
        uint256 usdtBalance = usdt.balanceOf(address(this));
        if (usdtBalance > 0) {
            usdt.safeTransfer(owner(), usdtBalance);
        }

        // Withdraw USDC
        uint256 usdcBalance = usdc.balanceOf(address(this));
        if (usdcBalance > 0) {
            usdc.safeTransfer(owner(), usdcBalance);
        }
    }

    function _mintLoop(
        address _receiver,
        uint256 _mintAmount,
        string memory kadenaAddress
    ) internal {
        for (uint256 i = 0; i < _mintAmount; i++) {
            kadenaSupply.increment();
        }
        ethSupply.increment();
        _safeMint(_receiver, ethSupply.current(), kadenaAddress, _mintAmount);
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return uriPrefix;
    }
}